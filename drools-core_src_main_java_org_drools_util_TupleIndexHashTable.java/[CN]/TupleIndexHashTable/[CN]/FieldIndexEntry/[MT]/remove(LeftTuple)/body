{
  LeftTuple previous=this.first;
  LeftTuple current=previous;
  while (current != null) {
    final LeftTuple next=(LeftTuple)current.getNext();
    if (tuple.equals(current)) {
      if (this.first == current) {
        this.first=next;
      }
 else {
        previous.setNext(next);
      }
      current.setNext(null);
      return current;
    }
    previous=current;
    current=next;
  }
  return current;
}
