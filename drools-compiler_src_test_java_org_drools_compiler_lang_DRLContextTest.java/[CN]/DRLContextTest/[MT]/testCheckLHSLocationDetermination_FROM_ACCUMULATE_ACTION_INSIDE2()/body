{
  String input="rule MyRule \n" + "	when \n" + "		Class ( property > 0 ) from accumulate( \n"+ "			$cheese : Cheese( type == $likes ), \n"+ "			init( int total = 0; ), \n"+ "			action( total += $ch";
  DRLParser parser=getParser(input);
  parser.enableEditorInterface();
  try {
    parser.compilationUnit();
  }
 catch (  Exception ex) {
  }
  assertEquals(Location.LOCATION_LHS_FROM_ACCUMULATE_ACTION_INSIDE,getLastIntegerValue(parser.getEditorInterface().get(0).getContent()));
}
