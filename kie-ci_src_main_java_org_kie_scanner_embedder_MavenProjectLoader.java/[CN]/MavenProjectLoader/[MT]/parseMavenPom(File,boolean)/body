{
  if (!pomFile.exists()) {
    return null;
  }
  MavenRequest mavenRequest=createMavenRequest(offline);
  mavenRequest.setPom(pomFile.getAbsolutePath());
  try {
    return new MavenEmbedder(mavenRequest).readProject(pomFile);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
