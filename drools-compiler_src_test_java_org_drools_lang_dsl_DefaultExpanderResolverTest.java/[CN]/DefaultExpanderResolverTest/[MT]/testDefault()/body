{
  final InputStreamReader reader=new InputStreamReader(this.getClass().getResourceAsStream("test.dsl"));
  final DefaultExpanderResolver res=new DefaultExpanderResolver(reader);
  final DefaultExpander exp=(DefaultExpander)res.get("oooh",null);
  assertNotNull(exp);
  assertEquals("Person(name==\"foo\")",exp.expand("when","Person has name of foo"));
  assertEquals("wild();",exp.expand("then","Go wild"));
  assertEquals("something",exp.expand("when","nothing"));
  assertEquals("something",exp.expand("then","nothing"));
}
