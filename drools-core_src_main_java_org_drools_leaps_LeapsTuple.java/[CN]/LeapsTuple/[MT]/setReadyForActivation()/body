{
  this.readyForActivation=true;
  for (int i=0, length=this.notFactHandles.length; this.notConstraintsPresent && i < length && this.readyForActivation; i++) {
    if (this.notFactHandles[i].size() > 0) {
      this.readyForActivation=false;
    }
  }
  for (int i=0, length=this.existsFactHandles.length; this.existsConstraintsPresent && i < length && this.readyForActivation; i++) {
    if (this.notFactHandles[i].size() == 0) {
      this.readyForActivation=false;
    }
  }
}
