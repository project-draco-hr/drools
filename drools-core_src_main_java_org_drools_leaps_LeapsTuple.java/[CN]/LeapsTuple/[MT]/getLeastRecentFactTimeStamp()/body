{
  if (this.factHandles != null) {
    long recency=-1;
    for (int i=0; i < this.factHandles.length; i++) {
      if (i == 0) {
        recency=this.factHandles[0].getRecency();
      }
 else       if (this.factHandles[i].getRecency() < recency) {
        recency=this.factHandles[i].getRecency();
      }
    }
    return recency;
  }
 else {
    return -1L;
  }
}
