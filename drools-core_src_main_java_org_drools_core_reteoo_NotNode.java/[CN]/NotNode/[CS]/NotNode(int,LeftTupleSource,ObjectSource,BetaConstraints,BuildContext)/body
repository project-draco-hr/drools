{
  super(id,context.getPartitionId(),context.getKnowledgeBase().getConfiguration().isMultithreadEvaluation(),leftInput,rightInput,joinNodeBinder,context);
  this.tupleMemoryEnabled=context.isTupleMemoryEnabled();
  emptyBetaConstraints=joinNodeBinder.getConstraints().length == 0 || context.isEmptyForAllBetaConstraints();
}
