{
  if (other == this) {
    return true;
  }
  if (this.hashCode != other.hashCode) {
    return false;
  }
  if (this.handle.getId() != other.handle.getId()) {
    return false;
  }
  if (this.parent == null) {
    return (other.parent == null);
  }
 else {
    return this.parent.equals(other.parent);
  }
}
