{
  try {
    final PredicateContextEntry ctx=(PredicateContextEntry)context;
    return this.expression.evaluate(handle.getObject(),ctx.leftTuple,this.previousDeclarations,this.localDeclarations,ctx.workingMemory);
  }
 catch (  final Exception e) {
    throw new RuntimeDroolsException("Exception executing predicate " + this.expression,e);
  }
}
