{
  try {
    PredicateContextEntry ctx=(PredicateContextEntry)context;
    return this.expression.evaluate(object,ctx.leftTuple,declaration,previousDeclarations,localDeclarations,ctx.workingMemory);
  }
 catch (  Exception e) {
    throw new RuntimeDroolsException("Exception executing predicate " + this.expression,e);
  }
}
