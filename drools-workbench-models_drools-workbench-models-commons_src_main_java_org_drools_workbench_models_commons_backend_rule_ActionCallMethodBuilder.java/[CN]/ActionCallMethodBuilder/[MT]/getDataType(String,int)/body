{
  String dataType;
  MethodInfo methodInfo=getMethodInfo();
  if (methodInfo != null) {
    dataType=methodInfo.getParams().get(index++);
  }
 else {
    dataType=boundParams.get(param);
  }
  if (dataType == null) {
    return inferDataType(param,isJavaDialect);
  }
 else {
    return dataType;
  }
}
