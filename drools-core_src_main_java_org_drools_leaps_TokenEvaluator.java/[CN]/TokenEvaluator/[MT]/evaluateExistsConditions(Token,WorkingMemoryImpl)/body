{
  List enablingFactHandles=null;
  if (token.getCurrentRuleHandle().getLeapsRule().containsExistsColumns()) {
    enablingFactHandles=new LinkedList();
    for (Iterator it=token.getCurrentRuleHandle().getLeapsRule().getExistsColumnsIterator(); it.hasNext(); ) {
      enablingFactHandles.addAll(TokenEvaluator.getExistsEnablingFacts(token,workingMemory,(ColumnConstraints)it.next()));
    }
  }
  return enablingFactHandles;
}
