{
  boolean satisfied=false;
  FactHandleImpl factHandle=token.getFactHandleAtPosition(index);
  ColumnConstraints constraints=token.getCurrentRuleHandle().getLeapsRule().getColumnConstraintsAtPosition(index);
  if (constraints.evaluateAlphas(factHandle,token,workingMemory)) {
    satisfied=constraints.getBeta().isAllowed(factHandle,token,token.getWorkingMemory());
  }
  return satisfied;
}
