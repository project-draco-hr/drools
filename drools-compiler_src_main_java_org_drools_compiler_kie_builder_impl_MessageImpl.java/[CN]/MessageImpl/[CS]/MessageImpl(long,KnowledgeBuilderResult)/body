{
  this.id=id;
switch (result.getSeverity()) {
case ERROR:
    level=Level.ERROR;
  break;
case WARNING:
level=Level.WARNING;
break;
default :
level=Level.INFO;
}
path=result.getResource().getSourcePath();
if (result.getLines().length > 0) {
line=result.getLines()[0];
}
 else {
line=-1;
}
column=0;
text=result.getMessage();
}
