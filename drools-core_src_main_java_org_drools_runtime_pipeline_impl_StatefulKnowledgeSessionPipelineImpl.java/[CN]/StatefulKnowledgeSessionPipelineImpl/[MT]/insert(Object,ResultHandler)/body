{
  ClassLoader cl=((InternalRuleBase)(((KnowledgeBaseImpl)((StatefulKnowledgeSession)this.ksession).getKnowledgeBase()).getRuleBase())).getRootClassLoader();
  StatefulKnowledgeSessionPipelineContextImpl context=new StatefulKnowledgeSessionPipelineContextImpl(this.ksession,this.entryPoint,resultHandler,cl);
  emit(object,context);
}
