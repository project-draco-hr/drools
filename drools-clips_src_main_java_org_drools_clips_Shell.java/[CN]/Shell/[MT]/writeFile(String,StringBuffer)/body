{
  FileOutputStream fout;
  try {
    File f=new File(file);
    if (!f.exists()) {
      f.createNewFile();
    }
    fout=new FileOutputStream(f);
    fout.write(sessionLog.toString().getBytes());
    fout.flush();
    fout.close();
  }
 catch (  FileNotFoundException e) {
    System.err.println("File " + file + " does not exist.");
  }
catch (  IOException e) {
    e.printStackTrace();
  }
}
