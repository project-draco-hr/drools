{
  char[] chars=grammarTemplate.toCharArray();
  for (int i=0; i < chars.length; i++) {
switch (chars[i]) {
case '{':
      startHole();
    break;
case '}':
  endHole();
break;
default :
buffer.append(chars[i]);
break;
}
}
String buf=this.buffer.toString();
if (!buf.equals("")) addChunk(buf);
return this.chunks;
}
