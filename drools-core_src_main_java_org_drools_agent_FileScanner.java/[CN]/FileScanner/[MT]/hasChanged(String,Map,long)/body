{
  if (!updates.containsKey(path)) {
    updates.put(path,new Long(fileLastModified));
    return true;
  }
 else {
    Long last=(Long)updates.get(path);
    if (last.intValue() < fileLastModified) {
      updates.put(path,new Long(fileLastModified));
      return true;
    }
 else {
      return false;
    }
  }
}
