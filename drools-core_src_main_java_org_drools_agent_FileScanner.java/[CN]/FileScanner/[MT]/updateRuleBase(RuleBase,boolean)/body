{
  Package[] changes=getChangeSet();
  if (changes == null)   return;
  for (int i=0; i < changes.length; i++) {
    Package p=changes[i];
    if (removeExistingPackages) {
      removePackage(p.getName(),rb);
    }
    try {
      rb.addPackage(p);
    }
 catch (    Exception e) {
      throw new RuntimeDroolsException(e);
    }
  }
}
