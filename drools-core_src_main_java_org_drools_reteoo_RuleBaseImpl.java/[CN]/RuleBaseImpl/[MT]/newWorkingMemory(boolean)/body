{
  WorkingMemoryImpl workingMemory=new WorkingMemoryImpl(this);
  if (keepReference) {
    this.workingMemories.put(workingMemory,RuleBaseImpl.PRESENT);
  }
  InitialFactHandle handle=new InitialFactHandle((FactHandleImpl)workingMemory.newFactHandle());
  PropagationContext propagationContext=new PropagationContextImpl(workingMemory.getNextPropagationIdCounter(),PropagationContext.ASSERTION,null,null);
  assertObject(handle,handle.getObject(),propagationContext,workingMemory);
  return workingMemory;
}
