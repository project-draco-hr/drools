{
  PackageDescr pkg=DescrFactory.newPackage().name("org.kie").newImport().target("java.util.List").end().newImport().target("org.kie.examples.*").end().getDescr();
  assertEquals(2,pkg.getImports().size());
  assertEquals("java.util.List",pkg.getImports().get(0).getTarget());
  assertEquals("org.kie.examples.*",pkg.getImports().get(1).getTarget());
}
