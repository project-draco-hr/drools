{
  PackageDescr pkg=DescrFactory.newPackage().name("org.kie").attribute("dialect").value("mvel").end().getDescr();
  assertEquals("org.kie",pkg.getName());
  assertEquals("mvel",pkg.getAttribute("dialect").getValue());
  assertNull(pkg.getAttribute("salience"));
  KnowledgePackage kpkg=compilePkgDescr(pkg);
  assertEquals("org.kie",kpkg.getName());
}
