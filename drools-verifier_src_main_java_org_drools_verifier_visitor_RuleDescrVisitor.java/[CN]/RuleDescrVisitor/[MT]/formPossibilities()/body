{
  for (  SubPattern subPattern : solvers.getPatternPossibilities()) {
    Pattern pattern=data.getVerifierObject(VerifierComponentType.PATTERN,subPattern.getPatternPath());
    data.add(subPattern);
  }
  for (  SubRule subRule : solvers.getRulePossibilities()) {
    VerifierRule rule=data.getVerifierObject(VerifierComponentType.RULE,subRule.getRulePath());
    data.add(subRule);
  }
}
