{
  String drl="package org.drools\n" + "rule R1 when\n" + "   $m : Message( message == \"" + message + "\" )\n"+ "then\n"+ "end\n";
  GAV gav=kf.newGav("org.kie","hello-world",version);
  KieFileSystem kfs=kf.newKieFileSystem().generateAndWritePomXML(gav).write("src/main/resources/KBase1/org/pkg1/r1.drl",drl).writeKModuleXML(createKieProjectWithPackages(kf,"*").toXML());
  ks.newKieBuilder(kfs).buildAll();
}
