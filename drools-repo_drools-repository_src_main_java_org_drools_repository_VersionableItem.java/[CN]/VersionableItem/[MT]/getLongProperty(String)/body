{
  try {
    Node theNode=getVersionContentNode();
    if (theNode.hasProperty(property)) {
      Property data=theNode.getProperty(property);
      return data.getValue().getLong();
    }
 else {
      return 0;
    }
  }
 catch (  RepositoryException e) {
    throw new RulesRepositoryException(e);
  }
}
