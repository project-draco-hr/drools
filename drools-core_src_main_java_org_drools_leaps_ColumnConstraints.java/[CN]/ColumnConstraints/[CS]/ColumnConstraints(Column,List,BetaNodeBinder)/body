{
  this.column=column;
  if (beta != null) {
    this.beta=beta;
    this.betaPresent=true;
  }
 else {
    this.beta=null;
    this.betaPresent=false;
  }
  if (alpha != null && alpha.size() > 0) {
    this.alphaConstraints=(LiteralConstraint[])alpha.toArray(new LiteralConstraint[0]);
    this.alphaPresent=true;
  }
 else {
    this.alphaConstraints=null;
    this.alphaPresent=false;
  }
}
