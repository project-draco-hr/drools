{
  if (this == object) {
    return true;
  }
  if (object == null || !(object instanceof Rule)) {
    return false;
  }
  Rule other=(Rule)object;
  return (this.name.equals(other.name) && this.agendaGroup.equals(other.agendaGroup) && this.salience == other.salience && this.noLoop == other.noLoop);
}
