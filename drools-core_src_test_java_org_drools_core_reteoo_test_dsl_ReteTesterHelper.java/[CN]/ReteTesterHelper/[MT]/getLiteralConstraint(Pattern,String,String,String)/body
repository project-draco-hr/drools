{
  final Class<?> clazz=((ClassObjectType)pattern.getObjectType()).getClassType();
  final InternalReadAccessor extractor=store.getReader(clazz,fieldName,getClass().getClassLoader());
  FieldValue fieldValue=FieldFactory.getInstance().getFieldValue(value,extractor.getValueType(),null);
  return new MvelConstraintTestUtil(fieldName + evaluatorString + value,fieldValue,extractor);
}
