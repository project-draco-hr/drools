{
  MockObjectSource source=new MockObjectSource(15);
  LeftInputAdapterNode liaNode=new LeftInputAdapterNode(1,0,source);
  assertEquals(1,liaNode.getId());
  assertLength(0,source.getObjectSinks());
  liaNode.attach();
  assertLength(1,source.getObjectSinks());
  assertSame(liaNode,source.getObjectSinks().get(0));
}
