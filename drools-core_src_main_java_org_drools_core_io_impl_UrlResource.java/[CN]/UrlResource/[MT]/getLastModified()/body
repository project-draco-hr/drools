{
  try {
    long lm=grabLastMod();
    if (lm == 0 && cacheFileExists()) {
      return getCacheFile().lastModified();
    }
    return lm;
  }
 catch (  IOException e) {
    if (cacheFileExists()) {
      return getCacheFile().lastModified();
    }
 else {
      throw new RuntimeException("Unable to get LastMofified for ClasspathResource",e);
    }
  }
}
