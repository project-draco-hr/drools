{
  URLConnection con=openURLConnection(this.url);
  con.setUseCaches(false);
  if (con instanceof HttpURLConnection) {
    if ("enabled".equalsIgnoreCase(basicAuthentication)) {
      String userpassword=username + ":" + password;
      byte[] authEncBytes=userpassword.getBytes();
      ((HttpURLConnection)con).setRequestProperty("Authorization","Basic " + new String(authEncBytes));
    }
  }
  return con.getInputStream();
}
