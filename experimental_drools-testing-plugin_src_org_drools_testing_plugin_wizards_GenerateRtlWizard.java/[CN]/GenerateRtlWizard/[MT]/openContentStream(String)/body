{
  String contents="";
  try {
    BufferedReader br=new BufferedReader(new FileReader(fileName));
    String line;
    while ((line=br.readLine()) != null) {
      contents=contents + line;
    }
  }
 catch (  Exception e) {
    throwCoreException(e.getMessage());
  }
  return new ByteArrayInputStream(contents.getBytes());
}
