{
  try {
    Assert.assertFalse(this.extractor.isNullValue(null,this.bean));
    InternalReadAccessor nullExtractor=ClassFieldAccessorCache.getInstance().getReader(TestBean.class,"nullAttr",getClass().getClassLoader());
    Assert.assertTrue(nullExtractor.isNullValue(null,this.bean));
  }
 catch (  final Exception e) {
    fail("Should not throw an exception");
  }
}
