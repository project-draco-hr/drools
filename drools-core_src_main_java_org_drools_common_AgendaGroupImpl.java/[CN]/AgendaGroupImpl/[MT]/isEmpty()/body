{
  boolean empty=this.priorityQueue.isEmpty();
  while (!empty) {
    ActivationQueue queue=(ActivationQueue)this.priorityQueue.get();
    if (queue.isEmpty()) {
      queue.setActivated(false);
      this.priorityQueue.remove();
    }
 else {
      break;
    }
    empty=this.priorityQueue.isEmpty();
  }
  return empty;
}
