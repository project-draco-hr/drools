{
  String input="rule MyRule \n" + "	when \n" + "		Class ( property > 0 ) from accumulate( \n"+ "			$cheese : Cheese( type == $likes ), \n"+ "			init( int total = 0; ), \n"+ "			action( total += $cheese.getPrice(); ), \n"+ "           result( new Integer( total ) ) \n"+ "		) \n"+ "		";
  DRL5xParser parser=getParser(input);
  parser.enableEditorInterface();
  try {
    parser.compilation_unit();
  }
 catch (  Exception ex) {
  }
  assertEquals(Location.LOCATION_LHS_BEGIN_OF_CONDITION,getLastIntegerValue(parser.getEditorInterface().get(0).getContent()));
}
