{
  StringBuilder sb=new StringBuilder();
  sb.append("package org.drools.test\n");
  sb.append("global java.util.List list\n\n");
  sb.append("declare  ");
  sb.append(typeName);
  if (annotations != null) {
    for (    String annotation : annotations) {
      sb.append("\n");
      sb.append(annotation);
    }
  }
  sb.append("\n");
  if (fields != null) {
    for (    String field : fields) {
      sb.append("\n");
      sb.append(field);
    }
  }
  sb.append("\n");
  sb.append("end\n");
  return sb.toString();
}
