{
  VariableResolverFactory v=variableFactory;
  while (v != null) {
    if (v instanceof LocalVariableResolverFactory)     return v;
    v=v.getNextFactory();
  }
  if (variableFactory == null)   return variableFactory=new LocalVariableResolverFactory(new HashMap<String,Object>());
 else   return new LocalVariableResolverFactory(new HashMap<String,Object>()).setNextFactory(variableFactory);
}
