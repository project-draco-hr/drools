{
  final BetaMemory memory=(BetaMemory)workingMemory.getNodeMemory(this);
  final ObjectMatches objectMatches=memory.add(workingMemory,handle);
  final BetaNodeConstraints binder=getJoinNodeBinder();
  for (final Iterator it=memory.leftTupleIterator(workingMemory,handle); it.hasNext(); ) {
    final ReteTuple leftTuple=(ReteTuple)it.next();
    final TupleMatch tupleMatch=attemptJoin(leftTuple,handle,objectMatches,binder,workingMemory);
    if (tupleMatch != null) {
      this.sink.propagateAssertTuple(leftTuple,handle,tupleMatch,context,workingMemory);
    }
  }
}
