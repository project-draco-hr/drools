{
  System.out.println("Replying to web service invocation " + workItem.getParameter("PartnerLink") + " "+ workItem.getParameter("PortType")+ " "+ workItem.getParameter("Operation")+ ", faultName = "+ workItem.getParameter("FaultName")+ ", message = "+ workItem.getParameter("Message")+ ": "+ result);
  Map<String,Object> results=new HashMap<String,Object>();
  results.put("Result",result);
  results.put("FaultName",faultName);
  workingMemory.getWorkItemManager().completeWorkItem(workItem.getId(),results);
}
