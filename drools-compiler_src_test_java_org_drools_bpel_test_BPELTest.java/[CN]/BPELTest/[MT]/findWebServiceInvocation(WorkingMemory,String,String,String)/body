{
  Set<WorkItem> workItems=workingMemory.getWorkItemManager().getWorkItems();
  for (Iterator<WorkItem> iterator=workItems.iterator(); iterator.hasNext(); ) {
    WorkItem workItem=iterator.next();
    if ("WebServiceInvocation".equals(workItem.getName()) && workItem.getParameter("PartnerLink").equals(partnerLink) && workItem.getParameter("PortType").equals(portType)&& workItem.getParameter("Operation").equals(operation)) {
      return workItem;
    }
  }
  return null;
}
