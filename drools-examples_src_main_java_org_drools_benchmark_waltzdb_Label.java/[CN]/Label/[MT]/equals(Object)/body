{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final Label other=(Label)obj;
  if (id == null) {
    if (other.id != null)     return false;
  }
 else   if (!id.equals(other.id))   return false;
  if (n1 == null) {
    if (other.n1 != null)     return false;
  }
 else   if (!n1.equals(other.n1))   return false;
  if (n2 == null) {
    if (other.n2 != null)     return false;
  }
 else   if (!n2.equals(other.n2))   return false;
  if (n3 == null) {
    if (other.n3 != null)     return false;
  }
 else   if (!n3.equals(other.n3))   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  if (type == null) {
    if (other.type != null)     return false;
  }
 else   if (!type.equals(other.type))   return false;
  return true;
}
