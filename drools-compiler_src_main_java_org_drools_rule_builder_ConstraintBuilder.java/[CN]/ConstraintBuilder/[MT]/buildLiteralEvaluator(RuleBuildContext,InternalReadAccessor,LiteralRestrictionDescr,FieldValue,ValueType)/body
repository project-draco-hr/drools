{
  EvaluatorDefinition.Target right=getRightTarget(extractor);
  EvaluatorDefinition.Target left=EvaluatorDefinition.Target.FACT;
  return getEvaluator(context,literalRestrictionDescr,vtype,literalRestrictionDescr.getEvaluator(),literalRestrictionDescr.isNegated(),literalRestrictionDescr.getParameterText(),left,right);
}
