{
  assertEquals(0,counter.get());
  workingMemory.queueWorkingMemoryAction(new FinalAction(counter));
  assertEquals(0,counter.get());
  workingMemory.queueWorkingMemoryAction(new FinalAction(counter));
  assertEquals(0,counter.get());
  workingMemory.executeQueuedActions();
  assertEquals(0,counter.get());
  workingMemory.executeQueuedActions();
  assertEquals(0,counter.get());
}
