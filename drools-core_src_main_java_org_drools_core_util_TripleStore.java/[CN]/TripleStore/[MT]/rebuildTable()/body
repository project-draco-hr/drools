{
  Triple[] tabs=new Triple[size];
  int k=0;
  for (int j=0; j < table.length; j++) {
    Triple t=(Triple)table[j];
    while (t != null) {
      tabs[k++]=t;
      t=(Triple)t.getNext();
    }
  }
  table=new Entry[size];
  size=0;
  for (int j=0; j < tabs.length; j++) {
    put(tabs[j],false);
  }
}
