{
  setupJoinNode();
  test().left().insert(a0,a1).right().insert(b0,b1).result().insert(t(a1,b0),t(a0,b1)).left(a0,a1).right(b0,b1).run().getActualResultLeftTuples().clear();
  test().right().insert(b3).result().insert(t(a1,b3),t(a0,b3)).left(a0,a1).right(b0,b1,b3).run().getActualResultLeftTuples().clear();
  test().left().insert(a2).delete(a1).right().insert(b4).delete(b0).result().insert(t(a0,b4),t(a2,b1),t(a2,b3),t(a2,b4)).delete(t(a1,b0),t(a1,b3)).left(a0,a2).right(b1,b3,b4).run().getActualResultLeftTuples().clear();
