{
  Declaration[] startDeclarations=declrs[0];
  Declaration[] endDeclarations=declrs[1];
  return new CronTrigger(timestamp,evalDateExpression(this.startTime,leftTuple,startDeclarations,wm),evalDateExpression(this.endTime,leftTuple,startDeclarations,wm),this.repeatLimit,this.cronExpression,calendarNames,calendars);
}
