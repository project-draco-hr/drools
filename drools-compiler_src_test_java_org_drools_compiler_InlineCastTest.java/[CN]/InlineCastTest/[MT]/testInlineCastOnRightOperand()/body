{
  String str="import org.drools.*;\n" + "rule R1 when\n" + "   $person : Person( )\n"+ "   String( this == $person.address#LongAddress.country )\n"+ "then\n"+ "end\n";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  Person mark1=new Person("mark");
  mark1.setAddress(new LongAddress("uk"));
  ksession.insert(mark1);
  ksession.insert("uk");
  assertEquals(1,ksession.fireAllRules());
  ksession.dispose();
}
