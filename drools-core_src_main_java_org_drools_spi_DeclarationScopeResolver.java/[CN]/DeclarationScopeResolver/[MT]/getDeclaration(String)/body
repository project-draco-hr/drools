{
  for (int i=this.buildStack.size() - 1; i >= 0; i--) {
    final Declaration declaration=(Declaration)((RuleConditionElement)this.buildStack.get(i)).getInnerDeclarations().get(name);
    if (declaration != null) {
      return declaration;
    }
  }
  for (int i=0, length=this.maps.length; i < length; i++) {
    if (this.maps[i].containsKey((name))) {
      final GlobalExtractor global=new GlobalExtractor(name,this.maps[i]);
      final Pattern dummy=new Pattern(0,global.getObjectType());
      final Declaration declaration=new Declaration(name,global,dummy);
      return declaration;
    }
  }
  return null;
}
