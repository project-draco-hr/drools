{
  if (operator == null) {
    return;
  }
  buf.append(" ");
  buf.append(operator);
  buf.append(" ");
switch (type) {
case ISingleFieldConstraint.TYPE_RET_VALUE:
    buf.append("( ");
  buf.append(operator);
buf.append(" )");
break;
case ISingleFieldConstraint.TYPE_LITERAL:
buf.append('"');
buf.append(value);
buf.append('"');
break;
default :
buf.append(value);
}
buf.append(" ");
}
