{
  buf.append(pattern.type);
  if (pattern.patterns != null && pattern.patterns.length > 1) {
    buf.append(" (");
    for (int i=0; i < pattern.patterns.length; i++) {
      renderSubPattern(pattern,i);
      if (i != pattern.patterns.length - 1) {
        buf.append(" and ");
      }
    }
    buf.append(") \n");
  }
 else {
    buf.append(" ");
    renderSubPattern(pattern,0);
    buf.append("\n");
  }
}
