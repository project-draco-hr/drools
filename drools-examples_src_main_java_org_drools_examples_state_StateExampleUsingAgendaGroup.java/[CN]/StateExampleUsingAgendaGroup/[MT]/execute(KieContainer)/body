{
  KieSession ksession=kc.newKieSession("StateAgendaGroupKS");
  final State a=new State("A");
  final State b=new State("B");
  final State c=new State("C");
  final State d=new State("D");
  ksession.insert(a);
  ksession.insert(b);
  ksession.insert(c);
  ksession.insert(d);
  ksession.fireAllRules();
  ksession.dispose();
}
