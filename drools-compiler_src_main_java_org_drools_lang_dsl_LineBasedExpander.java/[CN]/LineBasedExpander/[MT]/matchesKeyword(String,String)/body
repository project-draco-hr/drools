{
  if (line.length() < token.length())   return false;
  if (!line.startsWith(token))   return false;
  String rest=line.substring(token.length()).trim();
  if (rest.length() == 0)   return true;
  char next=rest.charAt(0);
  if (next == '#' || next == '/') {
    return true;
  }
 else {
    return false;
  }
}
