{
  if (setters != null) {
    for (    String statement : setters) {
      int dotPos=statement.indexOf('.');
      int argStart=statement.indexOf('(');
      String methodName=statement.substring(dotPos + 1,argStart).trim();
      String field=getSettedField(methodName);
      String value=unwrapParenthesis(statement);
      String dataType=inferDataType(action,field,boundParams,dmo,imports);
      if (dataType == null) {
        dataType=inferDataType(value,boundParams,isJavaDialect);
      }
      action.addFieldValue(buildFieldValue(isJavaDialect,field,value,dataType,boundParams,dmo));
    }
  }
}
