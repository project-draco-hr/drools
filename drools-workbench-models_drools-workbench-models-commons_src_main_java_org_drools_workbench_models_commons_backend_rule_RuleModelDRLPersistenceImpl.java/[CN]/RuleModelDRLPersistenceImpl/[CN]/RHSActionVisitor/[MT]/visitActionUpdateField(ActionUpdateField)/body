{
  buf.append(indentation);
  if (isDSLEnhanced) {
    buf.append(">");
  }
  buf.append("modify( ").append(action.getVariable()).append(" ) {\n");
  this.generateModifyMethodCalls(action.getFieldValues());
  buf.append("\n").append(indentation);
  if (isDSLEnhanced) {
    buf.append(">");
  }
  buf.append("}\n");
}
