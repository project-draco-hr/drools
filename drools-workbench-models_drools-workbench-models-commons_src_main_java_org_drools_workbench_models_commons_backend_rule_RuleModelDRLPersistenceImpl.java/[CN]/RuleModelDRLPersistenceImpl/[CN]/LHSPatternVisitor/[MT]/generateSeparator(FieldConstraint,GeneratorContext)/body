{
  if (!gctx.isHasOutput()) {
    return;
  }
  if (gctx.getDepth() == 0) {
    if (buf.length() > 2 && !(buf.charAt(buf.length() - 2) == ',')) {
      buf.append(", ");
    }
  }
 else {
    CompositeFieldConstraint cconstr=(CompositeFieldConstraint)gctx.getParent().getFieldConstraint();
    buf.append(cconstr.getCompositeJunctionType() + " ");
  }
}
