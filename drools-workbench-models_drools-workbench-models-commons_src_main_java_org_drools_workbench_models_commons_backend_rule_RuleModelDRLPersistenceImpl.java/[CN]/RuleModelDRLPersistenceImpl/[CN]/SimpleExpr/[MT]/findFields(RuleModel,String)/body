{
  ModelField[] fields=dmo.getProjectModelFields().get(type);
  if (fields != null) {
    return fields;
  }
  for (  String i : m.getImports().getImportStrings()) {
    if (i.endsWith("." + type)) {
      fields=dmo.getProjectModelFields().get(i);
      if (fields != null) {
        return fields;
      }
    }
  }
  return null;
}
