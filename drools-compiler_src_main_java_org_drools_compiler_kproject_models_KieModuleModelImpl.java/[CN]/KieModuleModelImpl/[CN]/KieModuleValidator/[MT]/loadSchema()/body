{
  SchemaFactory factory=SchemaFactory.newInstance("http://www.w3.org/2001/XMLSchema");
  try {
    URL url=KieModuleModel.class.getClassLoader().getResource("../../../kie-api/src/main/resources/org/kie/api/kmodule.xsd");
