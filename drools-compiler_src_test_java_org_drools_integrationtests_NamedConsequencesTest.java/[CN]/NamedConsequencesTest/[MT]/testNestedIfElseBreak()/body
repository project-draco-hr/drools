{
  String str="import org.drools.Cheese;\n " + "global java.util.List results;\n" + "\n"+ "rule R1 when\n"+ "    $a: Cheese ( type == \"stilton\" )\n"+ "    if ( type.startsWith(\"a\") ) do[t0] else if ( price > 10 ) do[t1] else break[t2]\n"+ "    $b: Cheese ( type == \"cheddar\" )\n"+ "then\n"+ "    results.add( $b.getType() );\n"+ "then[t0]\n"+ "    results.add( \"WRONG!\" );\n"+ "then[t1]\n"+ "    results.add( $a.getType() );\n"+ "then[t2]\n"+ "    results.add( $a.getType().toUpperCase() );\n"+ "end\n";
  List<String> results=executeTestWithDRL(str);
  assertEquals(1,results.size());
  assertTrue(results.contains("STILTON"));
}
