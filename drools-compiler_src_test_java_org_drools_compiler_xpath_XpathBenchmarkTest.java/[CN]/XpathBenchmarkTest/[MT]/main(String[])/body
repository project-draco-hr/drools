{
  int n=50000;
  System.out.println("Relational version");
  runTest(new RelationalTest(),n);
  System.out.println("-------------------------------------");
  System.out.println("From version");
  runTest(new FromTest(),n);
  System.out.println("-------------------------------------");
  System.out.println("Xpath version");
  runTest(new XpathTest(),n);
}
