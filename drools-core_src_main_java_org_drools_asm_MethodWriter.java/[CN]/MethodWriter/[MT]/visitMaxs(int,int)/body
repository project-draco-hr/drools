{
  if (computeMaxs) {
    int max=0;
    Label stack=blockStack;
    while (stack != null) {
      Label l=stack;
      stack=stack.next;
      int start=l.beginStackSize;
      int blockMax=start + l.maxStackSize;
      if (blockMax > max) {
        max=blockMax;
      }
      Edge b=l.successors;
      while (b != null) {
        l=b.successor;
        if (!l.pushed) {
          l.beginStackSize=start + b.stackSize;
          l.pushed=true;
          l.next=stack;
          stack=l;
        }
        b=b.next;
      }
    }
    this.maxStack=max;
  }
 else {
    this.maxStack=maxStack;
    this.maxLocals=maxLocals;
  }
}
