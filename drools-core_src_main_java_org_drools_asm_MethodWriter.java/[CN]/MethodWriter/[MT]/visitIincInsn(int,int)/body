{
  if (this.computeMaxs) {
    final int n=var + 1;
    if (n > this.maxLocals) {
      this.maxLocals=n;
    }
  }
  if ((var > 255) || (increment > 127) || (increment < -128)) {
    this.code.putByte(196).put12(Opcodes.IINC,var).putShort(increment);
  }
 else {
    this.code.putByte(Opcodes.IINC).put11(var,increment);
  }
}
