{
  if (this.computeMaxs) {
    if (this.currentBlock != null) {
      this.currentBlock.maxStackSize=this.maxStackSize;
      addSuccessor(this.stackSize,label);
    }
    this.currentBlock=label;
    this.stackSize=0;
    this.maxStackSize=0;
  }
  this.resize|=label.resolve(this,this.code.length,this.code.data);
}
