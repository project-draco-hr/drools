{
  if (computeMaxs) {
    if (!handler.pushed) {
      handler.beginStackSize=1;
      handler.pushed=true;
      handler.next=blockStack;
      blockStack=handler;
    }
  }
  ++catchCount;
  Handler h=new Handler();
  h.start=start;
  h.end=end;
  h.handler=handler;
  h.desc=type;
  h.type=type != null ? cw.newClass(type) : 0;
  if (lastHandler == null) {
    catchTable=h;
  }
 else {
    lastHandler.next=h;
  }
  lastHandler=h;
}
