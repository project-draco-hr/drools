{
  final String className="returnValue" + context.getNextId();
  returnValueRestrictionDescr.setClassMethodName(className);
  final Map map=createVariableContext(className,(String)returnValueRestrictionDescr.getContent(),context,previousDeclarations,localDeclarations,(String[])usedIdentifiers[1].toArray(new String[usedIdentifiers[1].size()]));
  map.put("readLocalsFromTuple",Boolean.FALSE);
  generatTemplates("returnValueMethod","returnValueInvoker",context,className,map,returnValueRestriction,returnValueRestrictionDescr);
}
