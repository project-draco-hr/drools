{
  MethodVisitor mv=cw.visitMethod(access,name,desc,signature,exceptions);
  mv.visitCode();
  body.setClassGenerator(cg);
  body.setMethodVisitor(mv);
  try {
    body.body(mv);
    mv.visitMaxs(0,0);
  }
 catch (  Exception e) {
    throw new RuntimeException("Error writing method " + name,e);
  }
  mv.visitEnd();
}
