{
  final KnowledgeBuilder kbuilder=KnowledgeBuilderFactory.newKnowledgeBuilder();
  kbuilder.add(ResourceFactory.newClassPathResource("Fibonacci.drl",FibonacciExample.class),KnowledgeType.DRL);
  final KnowledgeBase kbase=KnowledgeBaseFactory.newKnowledgeBase();
  kbase.addKnowledgePackages(kbuilder.getKnowledgePackages());
  final StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  final WorkingMemoryFileLogger logger=new WorkingMemoryFileLogger(ksession);
  logger.setFileName("log/fibonacci");
  ksession.insert(new Fibonacci(10));
  ksession.fireAllRules();
  logger.writeToDisk();
  ksession.dispose();
}
