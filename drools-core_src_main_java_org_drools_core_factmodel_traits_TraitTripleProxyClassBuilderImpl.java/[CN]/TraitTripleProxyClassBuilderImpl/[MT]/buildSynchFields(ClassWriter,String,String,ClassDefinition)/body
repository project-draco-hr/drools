{
{
    MethodVisitor mv=cw.visitMethod(ACC_PRIVATE,"synchFields","()V",null,null);
    mv.visitCode();
    for (    FieldDefinition fld : def.getFieldsDefinitions()) {
      if (fld.getInitExpr() != null) {
        synchField(mv,fld,proxyName);
      }
    }
    mv.visitInsn(RETURN);
    mv.visitMaxs(0,0);
    mv.visitEnd();
  }
}
