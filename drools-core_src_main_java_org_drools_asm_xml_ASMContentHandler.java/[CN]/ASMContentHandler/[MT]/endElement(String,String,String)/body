{
  String name=localName;
  if (name == null || name.length() < 1) {
    name=qName;
  }
  final Rule r=(Rule)this.RULES.match(this.match);
  if (r != null) {
    r.end(name);
  }
  final int slash=this.match.lastIndexOf('/');
  if (slash >= 0) {
    this.match=this.match.substring(0,slash);
  }
 else {
    this.match="";
  }
}
