{
  String name=localName;
  if (name == null || name.length() < 1) {
    name=qName;
  }
  StringBuffer sb=new StringBuffer(match);
  if (match.length() > 0) {
    sb.append('/');
  }
  sb.append(name);
  match=sb.toString();
  Rule r=(Rule)RULES.match(match);
  if (r != null)   r.begin(name,list);
}
