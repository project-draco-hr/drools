{
  String name=localName;
  if (name == null || name.length() < 1) {
    name=qName;
  }
  final StringBuffer sb=new StringBuffer(this.match);
  if (this.match.length() > 0) {
    sb.append('/');
  }
  sb.append(name);
  this.match=sb.toString();
  final Rule r=(Rule)this.RULES.match(this.match);
  if (r != null) {
    r.begin(name,list);
  }
}
