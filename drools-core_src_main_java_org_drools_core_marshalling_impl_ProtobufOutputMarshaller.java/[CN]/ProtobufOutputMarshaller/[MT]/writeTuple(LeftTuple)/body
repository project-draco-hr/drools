{
  ProtobufMessages.Tuple.Builder _tb=ProtobufMessages.Tuple.newBuilder();
  for (LeftTuple entry=tuple; entry != null; entry=entry.getParent()) {
    InternalFactHandle handle=entry.getLastHandle();
    if (handle != null) {
      _tb.addHandleId(handle.getId());
    }
  }
  return _tb.build();
}
