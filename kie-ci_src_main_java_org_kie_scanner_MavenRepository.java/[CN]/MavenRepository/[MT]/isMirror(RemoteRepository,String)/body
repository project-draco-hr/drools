{
  return mirrorOf.equals("*") || (mirrorOf.equals("external:*") && !repo.getUrl().startsWith("file:")) || (mirrorOf.startsWith("external:*") && !repo.getUrl().startsWith("file:") && !mirrorOf.contains("!" + repo.getId()))|| (mirrorOf.startsWith("*") && !mirrorOf.contains("!" + repo.getId()))|| (!mirrorOf.startsWith("*") && !mirrorOf.startsWith("external:*") && mirrorOf.contains(repo.getId()));
}
