{
  final Column column=new Column(0,new ClassObjectType(DroolsQuery.class));
  final ClassFieldExtractor extractor=new ClassFieldExtractor(DroolsQuery.class,"name");
  final FieldValue field=FieldFactory.getFieldValue(query.getName(),ValueType.STRING_TYPE);
  final LiteralConstraint constraint=new LiteralConstraint(extractor,ValueType.STRING_TYPE.getEvaluator(Operator.EQUAL),field);
  column.addConstraint(constraint);
  subrule.addChild(0,column);
}
