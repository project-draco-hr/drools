{
  Class clazz=null;
  try {
    clazz=loader.loadClass(classname);
  }
 catch (  ClassNotFoundException e1) {
    try {
      addJars(jars);
      clazz=loader.loadClass(classname);
    }
 catch (    IOException e) {
      throw new IllegalStateException(e.getMessage());
    }
catch (    ClassNotFoundException e) {
      errors.add("Class not found: " + classname);
    }
  }
  return clazz;
}
