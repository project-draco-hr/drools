{
  List result=new ArrayList();
  for (int i=0; i < fields.length; i++) {
    String field=fields[i];
    if (field.equals("class") || field.equals("hashCode") || field.equals("toString")) {
    }
 else {
      result.add(field);
    }
  }
  return (String[])result.toArray(new String[result.size()]);
}
