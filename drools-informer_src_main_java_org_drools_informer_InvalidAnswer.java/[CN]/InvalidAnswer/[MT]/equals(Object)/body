{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final InvalidAnswer other=(InvalidAnswer)obj;
  if (questionId == null) {
    if (other.questionId != null)     return false;
  }
 else   if (!questionId.equals(other.questionId))   return false;
  if (reason == null) {
    if (other.reason != null)     return false;
  }
 else   if (!reason.equals(other.reason))   return false;
  return true;
}
