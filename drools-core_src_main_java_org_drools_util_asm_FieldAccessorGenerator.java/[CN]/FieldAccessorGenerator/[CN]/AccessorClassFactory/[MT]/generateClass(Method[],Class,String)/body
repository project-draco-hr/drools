{
  final ClassWriter cw=new ClassWriter(true);
  generatedClassName=generatedClassName.replaceAll("\\.","/");
  cw.visit(Opcodes.V1_2,Opcodes.ACC_PUBLIC + Opcodes.ACC_SUPER,generatedClassName,null,"java/lang/Object",new String[]{Type.getInternalName(FieldAccessor.class)});
  cw.visitSource(getShortName(targetClass) + ".java",null);
  doConstructor(cw);
  doMethods(cw,Type.getInternalName(targetClass),getters,targetClass.isInterface());
  cw.visitEnd();
  return cw.toByteArray();
}
