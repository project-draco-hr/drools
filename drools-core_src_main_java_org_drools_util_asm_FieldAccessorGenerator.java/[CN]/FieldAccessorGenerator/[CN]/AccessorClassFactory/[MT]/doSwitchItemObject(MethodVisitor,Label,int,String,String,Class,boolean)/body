{
  final String returnType="()" + Type.getDescriptor(returnClass);
  mv.visitLabel(label);
  mv.visitVarInsn(Opcodes.ALOAD,target);
  if (isInterface) {
    mv.visitMethodInsn(Opcodes.INVOKEINTERFACE,targetType,targetMethod,returnType);
  }
 else {
    mv.visitMethodInsn(Opcodes.INVOKEVIRTUAL,targetType,targetMethod,returnType);
  }
  mv.visitInsn(Opcodes.ARETURN);
}
