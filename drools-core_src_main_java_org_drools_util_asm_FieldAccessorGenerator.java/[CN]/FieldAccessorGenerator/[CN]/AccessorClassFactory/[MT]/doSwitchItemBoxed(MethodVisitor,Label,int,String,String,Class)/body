{
  Class boxType=null;
  boxType=getBoxType(scalarType);
  String scalarDescriptor=Type.getDescriptor(scalarType);
  String internalBoxName=Type.getInternalName(boxType);
  mv.visitLabel(switchItem);
  mv.visitTypeInsn(NEW,internalBoxName);
  mv.visitInsn(DUP);
  mv.visitVarInsn(ALOAD,target);
  mv.visitMethodInsn(INVOKEVIRTUAL,targetType,targetMethod,"()" + scalarDescriptor);
  mv.visitMethodInsn(INVOKESPECIAL,internalBoxName,"<init>","(" + scalarDescriptor + ")V");
  mv.visitInsn(ARETURN);
}
