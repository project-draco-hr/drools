{
  if (type == null) {
    return null;
  }
  if (modelFields.containsKey(type)) {
    return type;
  }
  for (  Map.Entry<String,ModelField[]> entry : modelFields.entrySet()) {
    for (    ModelField mf : entry.getValue()) {
      if ("this".equals(mf.getName()) && type.equals(mf.getClassName())) {
        return entry.getKey();
      }
    }
  }
  return null;
}
