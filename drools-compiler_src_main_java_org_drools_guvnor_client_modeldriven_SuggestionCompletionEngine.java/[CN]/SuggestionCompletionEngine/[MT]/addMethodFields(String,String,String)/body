{
  Map<String,List<String>> method=methodFields.get(factName);
  List<String> paramList=null;
  if (method == null) {
    method=new HashMap<String,List<String>>();
    methodFields.put(factName,method);
    paramList=new ArrayList<String>();
    method.put(methodName,paramList);
  }
 else {
    paramList=method.get(methodName);
    if (paramList == null) {
      paramList=new ArrayList<String>();
      method.put(methodName,paramList);
    }
  }
  paramList.add(paramTypes);
}
