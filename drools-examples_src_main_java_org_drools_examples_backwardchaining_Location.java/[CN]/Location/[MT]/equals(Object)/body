{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Location location1=(Location)o;
  if (item != null ? !item.equals(location1.item) : location1.item != null) {
    return false;
  }
  if (location != null ? !location.equals(location1.location) : location1.location != null) {
    return false;
  }
  return true;
}
