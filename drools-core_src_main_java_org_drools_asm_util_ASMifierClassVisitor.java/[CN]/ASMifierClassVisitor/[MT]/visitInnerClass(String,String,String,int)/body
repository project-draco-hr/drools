{
  this.buf.setLength(0);
  this.buf.append("cw.visitInnerClass(");
  appendConstant(name);
  this.buf.append(", ");
  appendConstant(outerName);
  this.buf.append(", ");
  appendConstant(innerName);
  this.buf.append(", ");
  appendAccess(access | ASMifierClassVisitor.ACCESS_INNER);
  this.buf.append(");\n\n");
  this.text.add(this.buf.toString());
}
