{
  MarshallerReaderContext context=new MarshallerReaderContext(stream,ruleBase,RuleBaseNodes.getNodeMap(ruleBase),factory,marshallingConfig.isMarshallProcessInstances(),marshallingConfig.isMarshallWorkItems());
  session=InputMarshaller.readSession((ReteooStatefulSession)session,context);
  context.close();
  return session;
}
