{
  MarshallerWriteContext context=new MarshallerWriteContext(stream,ruleBase,(InternalWorkingMemory)session,RuleBaseNodes.getNodeMap(ruleBase),this.factory,marshallingConfig.isMarshallProcessInstances(),marshallingConfig.isMarshallWorkItems());
  OutputMarshaller.writeSession(context);
  context.close();
}
