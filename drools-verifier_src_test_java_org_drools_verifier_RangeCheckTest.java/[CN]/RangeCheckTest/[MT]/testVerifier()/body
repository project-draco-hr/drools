{
  VerifierBuilder vBuilder=VerifierBuilderFactory.newVerifierBuilder();
  Assert.assertFalse(vBuilder.hasErrors());
  Assert.assertEquals(0,vBuilder.getErrors().size());
  Verifier verifier=vBuilder.newVerifier();
  verifier.addResourcesToVerify(new ClassPathResource("RangeTest.drl",Verifier.class),ResourceType.DRL);
  Assert.assertFalse(verifier.hasErrors());
  Assert.assertEquals(0,verifier.getErrors().size());
  boolean works=verifier.fireAnalysis();
  Assert.assertTrue(works);
  VerifierReport result=verifier.getResult();
  Assert.assertNotNull(result);
  for (  VerifierMessageBase message : result.getBySeverity(Severity.ERROR)) {
    System.out.println(message);
  }
  Assert.assertEquals(0,result.getBySeverity(Severity.ERROR).size());
}
