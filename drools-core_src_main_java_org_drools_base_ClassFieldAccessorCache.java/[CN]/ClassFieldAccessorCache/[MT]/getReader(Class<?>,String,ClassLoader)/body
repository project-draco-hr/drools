{
  if (readerCache == null) {
    readerCache=new HashMap<AccessorKey,ClassFieldReader>();
  }
  final AccessorKey key=new AccessorKey(clazz,fieldName);
  if (readerCache.containsKey(key)) {
    return readerCache.get(key);
  }
 else {
    final ClassFieldReader ex=new ClassFieldReader(clazz,fieldName,classLoader,factory);
    readerCache.put(key,ex);
    return ex;
  }
}
