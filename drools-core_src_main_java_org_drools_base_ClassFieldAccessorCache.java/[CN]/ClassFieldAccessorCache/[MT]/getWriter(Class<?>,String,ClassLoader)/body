{
  if (writerCache == null) {
    writerCache=new HashMap<AccessorKey,ClassFieldWriter>();
  }
  final AccessorKey key=new AccessorKey(clazz,fieldName);
  if (writerCache.containsKey(key)) {
    return writerCache.get(key);
  }
 else {
    final ClassFieldWriter writer=new ClassFieldWriter(clazz,fieldName,classLoader,factory);
    writerCache.put(key,writer);
    return writer;
  }
}
