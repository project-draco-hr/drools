{
  FieldExtractor extractor=ClassFieldExtractorCache.getInstance().getExtractor(clazz,fieldName,getClass().getClassLoader());
  Declaration declaration=new Declaration(identifier,extractor,new Pattern(0,new ClassObjectType(clazz)));
  Evaluator evaluator=StringFactory.getInstance().getEvaluator(operator);
  return new VariableConstraint(extractor,declaration,evaluator);
}
