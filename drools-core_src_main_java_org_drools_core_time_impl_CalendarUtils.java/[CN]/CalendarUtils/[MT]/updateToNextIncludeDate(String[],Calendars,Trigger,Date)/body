{
  if (calendarNames == null || calendarNames.length == 0) {
    return next;
  }
  while (next != null) {
    boolean included=true;
    for (    String cal : calendarNames) {
      if (!calendars.get(cal).isTimeIncluded(next.getTime())) {
        included=false;
        break;
      }
    }
    if (included == true) {
      break;
    }
 else {
      next=trigger.nextFireTime();
    }
  }
  return next;
}
