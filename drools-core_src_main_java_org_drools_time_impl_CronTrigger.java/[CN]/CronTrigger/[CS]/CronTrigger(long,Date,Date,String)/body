{
  setCronExpression(cronExpression);
  if (startTime == null) {
    startTime=new Date(timestamp);
  }
  setStartTime(startTime);
  if (endTime != null) {
    setEndTime(endTime);
  }
  setTimeZone(TimeZone.getDefault());
  this.nextFireTime=getFireTimeAfter(new Date(getStartTime().getTime() - 1000l));
}
