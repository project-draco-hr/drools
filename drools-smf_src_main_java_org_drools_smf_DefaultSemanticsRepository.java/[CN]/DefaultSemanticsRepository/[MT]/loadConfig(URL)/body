{
  InputStream config=url.openStream();
  BufferedReader in=new BufferedReader(new InputStreamReader(config));
  try {
    String line;
    while ((line=in.readLine()) != null) {
      line=line.trim();
      if (line.equals("") || line.startsWith("#")) {
        continue;
      }
      loadSemantics(line);
    }
  }
  finally {
    in.close();
  }
}
