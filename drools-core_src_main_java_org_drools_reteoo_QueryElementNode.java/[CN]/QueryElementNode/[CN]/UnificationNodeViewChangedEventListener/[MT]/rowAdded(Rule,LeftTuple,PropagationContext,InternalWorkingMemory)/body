{
  Variable[] vars=query.getVariables();
  Object[] objects=new Object[this.variables.length];
  for (int i=0, length=this.variables.length; i < length; i++) {
    objects[i]=vars[this.variables[i]].getValue();
  }
  QueryElementFactHandle handle=new QueryElementFactHandle(objects,workingMemory.getFactHandleFactory().getAtomicId().incrementAndGet(),workingMemory.getFactHandleFactory().getAtomicRecency().incrementAndGet());
  RightTuple rightTuple=new RightTuple(handle);
  this.sink.createChildLeftTuplesforQuery(this.leftTuple,rightTuple,this.tupleMemoryEnabled);
}
