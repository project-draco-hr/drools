{
  String pkg="package org.test\n import org.drools.guvnor.client.modeldriven.SuggestionCompletionEngine2Test.Person\n";
  SuggestionCompletionLoader loader=new SuggestionCompletionLoader();
  SuggestionCompletionEngine engine=loader.getSuggestionEngine(pkg,new ArrayList<JarInputStream>(),new ArrayList<DSLTokenizedMappingFile>(),new ArrayList<String>());
  String[] accessors=engine.getFieldCompletions(FieldAccessorsAndMutators.ACCESSOR,"SuggestionCompletionEngine2Test$Person");
  assertEquals(3,accessors.length);
  String[] mutators=engine.getFieldCompletions(FieldAccessorsAndMutators.MUTATOR,"SuggestionCompletionEngine2Test$Person");
  assertEquals(3,mutators.length);
}
