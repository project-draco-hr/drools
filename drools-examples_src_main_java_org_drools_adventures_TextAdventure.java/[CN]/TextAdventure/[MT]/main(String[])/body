{
  EventQueue.invokeLater(new Runnable(){
    public void run(){
      try {
        AdventureFrame frame=new AdventureFrame();
        frame.setVisible(true);
        GameEngine engine=new GameEngine();
        engine.createGame(new TextFieldChannel(frame.getOutputTextArea()),new TextFieldChannel(frame.getLocalEventsTextArea()));
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
);
}
