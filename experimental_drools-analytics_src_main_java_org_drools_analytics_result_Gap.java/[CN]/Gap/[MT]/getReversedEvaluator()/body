{
  if (restriction.getEvaluator().equals("!=")) {
    return "==";
  }
 else   if (restriction.getEvaluator().equals("==")) {
    return "!=";
  }
 else   if (restriction.getEvaluator().equals(">")) {
    return "<=";
  }
 else   if (restriction.getEvaluator().equals("<")) {
    return ">=";
  }
 else   if (restriction.getEvaluator().equals(">=")) {
    return "<";
  }
 else   if (restriction.getEvaluator().equals("<=")) {
    return ">";
  }
  return null;
}
