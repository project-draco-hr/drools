{
  if (evaluator.equals("!=")) {
    return "==";
  }
 else   if (evaluator.equals("==")) {
    return "!=";
  }
 else   if (evaluator.equals(">")) {
    return "<=";
  }
 else   if (evaluator.equals("<")) {
    return ">=";
  }
 else   if (evaluator.equals(">=")) {
    return "<";
  }
 else   if (evaluator.equals("<=")) {
    return ">";
  }
  return evaluator;
}
