{
  if (this.alphaConstraints.length == 0 && this.betaConstraints.length == 0) {
    return true;
  }
  for (int i=0; i < this.alphaConstraints.length; i++) {
    if (this.alphaConstraints[i].isAllowed(handle,((MultiFieldConstraintContextEntry)context).workingMemory,((MultiFieldConstraintContextEntry)context).alphas[i])) {
      return true;
    }
  }
  for (int i=0; i < this.betaConstraints.length; i++) {
    if (this.betaConstraints[i].isAllowedCachedLeft(((MultiFieldConstraintContextEntry)context).betas[i],handle)) {
      return true;
    }
  }
  return false;
}
