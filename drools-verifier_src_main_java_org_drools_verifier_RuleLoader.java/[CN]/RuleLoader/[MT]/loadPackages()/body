{
  String[] fileNames=new String[]{"equivalence/Rules.drl","incoherence/Patterns.drl","incoherence/Restrictions.drl","missingEquality/MissingEquality.drl","optimisation/RestrictionOrder.drl","optimisation/PatternOrder.drl","rangeChecks/Dates.drl","rangeChecks/Doubles.drl","rangeChecks/Integers.drl","rangeChecks/Patterns.drl","rangeChecks/Variables.drl","rangeChecks/Clean.drl","redundancy/Restrictions.drl","redundancy/Notes.drl","redundancy/Consequence.drl","redundancy/Patterns.drl","redundancy/Possibilities.drl","redundancy/Rules.drl","redundancy/Warnings.drl","reports/RangeCheckReports.drl","Consequence.drl"};
  Collection<Package> packages=new ArrayList<Package>();
  for (int i=0; i < fileNames.length; i++) {
    try {
      InputStreamReader reader=new InputStreamReader(RuleLoader.class.getResourceAsStream(fileNames[i]));
      packages.add(loadPackage(reader));
      reader.close();
    }
 catch (    DroolsParserException e) {
      e.printStackTrace();
    }
catch (    NullPointerException e) {
      System.err.println("Error when opening file " + fileNames[i] + ".");
      e.printStackTrace();
    }
catch (    IOException e) {
      e.printStackTrace();
    }
  }
  return packages;
}
