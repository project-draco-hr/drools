{
  if (this == obj) {
    return true;
  }
  if (obj == null || obj instanceof AbstractCompositeRestriction) {
    return false;
  }
  final AbstractCompositeRestriction other=(AbstractCompositeRestriction)obj;
  if (!Arrays.equals(this.restrictions,other.restrictions)) {
    return false;
  }
  return true;
}
