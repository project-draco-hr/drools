{
  FactHandle factHandle=new DisconnectedFactHandle(reader.getAttribute("factHandle"));
  List<Setter> setters=new ArrayList();
  while (reader.hasMoreChildren()) {
    reader.moveDown();
    Setter setter=CommandFactory.newSetter(reader.getAttribute("accessor"),reader.getAttribute("value"));
    setters.add(setter);
    reader.moveUp();
  }
  Command cmd=CommandFactory.newModify(factHandle,setters);
  return cmd;
}
