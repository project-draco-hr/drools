{
  DroolsMVELFactory factory=(DroolsMVELFactory)this.prototype.clone();
  KnowledgeHelper knowledgeHelper=new DefaultKnowledgeHelper(workingMemory);
  factory.setContext(tuple,knowledgeHelper,null,workingMemory,null);
  Package pkg=workingMemory.getRuleBase().getPackage("MAIN");
  if (pkg != null) {
    MVELDialectRuntimeData data=(MVELDialectRuntimeData)pkg.getDialectRuntimeRegistry().getDialectData(this.id);
    factory.setNextFactory(data.getFunctionFactory());
  }
  return ((Boolean)MVEL.executeExpression(this.expr,factory)).booleanValue();
}
