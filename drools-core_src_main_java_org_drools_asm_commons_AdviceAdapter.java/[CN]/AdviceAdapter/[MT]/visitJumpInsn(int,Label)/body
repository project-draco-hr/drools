{
  this.mv.visitJumpInsn(opcode,label);
  if (this.constructor) {
switch (opcode) {
case IFEQ:
case IFNE:
case IFLT:
case IFGE:
case IFGT:
case IFLE:
case IFNULL:
case IFNONNULL:
      popValue();
    break;
case IF_ICMPEQ:
case IF_ICMPNE:
case IF_ICMPLT:
case IF_ICMPGE:
case IF_ICMPGT:
case IF_ICMPLE:
case IF_ACMPEQ:
case IF_ACMPNE:
  popValue();
popValue();
break;
case JSR:
pushValue(AdviceAdapter.OTHER);
break;
}
addBranch(label);
}
}
