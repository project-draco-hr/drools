{
  super.visitVarInsn(opcode,var);
  if (this.constructor) {
switch (opcode) {
case ILOAD:
case FLOAD:
      pushValue(AdviceAdapter.OTHER);
    break;
case LLOAD:
case DLOAD:
  pushValue(AdviceAdapter.OTHER);
pushValue(AdviceAdapter.OTHER);
break;
case ALOAD:
pushValue(var == 0 ? AdviceAdapter.THIS : AdviceAdapter.OTHER);
break;
case ASTORE:
case ISTORE:
case FSTORE:
popValue();
break;
case LSTORE:
case DSTORE:
popValue();
popValue();
break;
}
}
}
