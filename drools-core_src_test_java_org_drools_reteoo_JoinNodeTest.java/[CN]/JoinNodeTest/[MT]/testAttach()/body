{
  assertEquals(15,node.getId());
  assertLength(0,objectSource.getObjectSinks());
  assertLength(0,tupleSource.getTupleSinks());
  node.attach();
  assertLength(1,objectSource.getObjectSinks());
  assertLength(1,tupleSource.getTupleSinks());
  assertSame(node,objectSource.getObjectSinks().get(0));
  assertSame(node,tupleSource.getTupleSinks().get(0));
}
