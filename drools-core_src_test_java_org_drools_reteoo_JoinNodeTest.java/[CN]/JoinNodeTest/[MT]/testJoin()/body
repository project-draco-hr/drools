{
  FactHandleImpl f0=new FactHandleImpl(0);
  workingMemory.putObject(f0,"test0");
  ReteTuple tuple1=new ReteTuple(2,f0,workingMemory);
  node.assertTuple(tuple1,context,workingMemory);
  FactHandleImpl f1=new FactHandleImpl(1);
  workingMemory.putObject(f1,"test1");
  node.assertObject("test1",f1,context,workingMemory);
  Object[] list=(Object[])sink.getAsserted().get(0);
  ReteTuple joinedTuple=(ReteTuple)list[0];
  assertNull(joinedTuple.get(1));
  assertEquals("test0",joinedTuple.get(2));
  assertNull(joinedTuple.get(3));
  assertNull(joinedTuple.get(4));
  assertEquals("test1",joinedTuple.get(5));
}
