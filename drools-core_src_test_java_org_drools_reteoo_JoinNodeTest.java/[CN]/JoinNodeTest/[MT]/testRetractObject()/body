{
  FactHandleImpl f0=new FactHandleImpl(0);
  node.retractObject(f0,context,workingMemory);
  assertLength(0,sink.getRetracted());
  node.assertObject("test0",f0,context,workingMemory);
  assertEquals(1,memory.rightMemorySize());
  node.retractObject(f0,context,workingMemory);
  assertLength(0,sink.getRetracted());
  assertEquals(0,memory.leftMemorySize());
  assertEquals(0,memory.rightMemorySize());
}
