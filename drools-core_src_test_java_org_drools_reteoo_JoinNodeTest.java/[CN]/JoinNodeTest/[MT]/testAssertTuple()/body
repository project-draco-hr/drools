{
  FactHandleImpl f0=new FactHandleImpl(0);
  ReteTuple tuple1=new ReteTuple(0,f0,workingMemory);
  node.assertTuple(tuple1,context,workingMemory);
  assertEquals(1,memory.leftMemorySize());
  assertEquals(0,memory.rightMemorySize());
  node.assertTuple(tuple1,context,workingMemory);
  assertEquals(1,memory.leftMemorySize());
  FactHandleImpl f1=new FactHandleImpl(1);
  ReteTuple tuple2=new ReteTuple(0,f1,workingMemory);
  node.assertTuple(tuple2,context,workingMemory);
  assertEquals(2,memory.leftMemorySize());
  TupleMatches betaMemory1=memory.getBetaMemory(tuple1.getKey());
  TupleMatches betaMemory2=memory.getBetaMemory(tuple2.getKey());
  assertLength(0,betaMemory1.getMatches());
  assertLength(0,betaMemory2.getMatches());
  assertLength(0,sink.getAsserted());
}
