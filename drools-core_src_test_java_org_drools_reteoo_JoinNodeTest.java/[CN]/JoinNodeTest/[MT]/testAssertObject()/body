{
  FactHandleImpl f0=new FactHandleImpl(0);
  node.assertObject("test2",f0,context,workingMemory);
  assertEquals(0,memory.leftMemorySize());
  assertEquals(1,memory.rightMemorySize());
  node.assertObject("test0",f0,context,workingMemory);
  assertEquals(1,memory.rightMemorySize());
  FactHandleImpl f1=new FactHandleImpl(1);
  node.assertObject("test1",f1,context,workingMemory);
  assertEquals(2,memory.rightMemorySize());
  assertEquals(0,memory.leftMemorySize());
  assertLength(0,sink.getAsserted());
}
