{
  FactHandleImpl f0=(FactHandleImpl)this.workingMemory.assertObject("test0");
  this.node.assertObject(f0,this.context,this.workingMemory);
  FactHandleImpl f1=new FactHandleImpl(1);
  ReteTuple tuple1=new ReteTuple(f1);
  this.node.assertTuple(tuple1,this.context,this.workingMemory);
  Map map=tuple1.getTupleMatches();
  TupleMatch match=(TupleMatch)map.get(f0);
  ObjectMatches matches=(ObjectMatches)this.memory.rightObjectIterator(this.workingMemory,tuple1).next();
  assertSame(match,matches.getFirstTupleMatch());
  assertSame(match.getJoinedTuples().get(0),((Object[])this.sink.getAsserted().get(0))[0]);
  FactHandleImpl f2=new FactHandleImpl(2);
  ReteTuple tuple2=new ReteTuple(f2);
  this.node.assertTuple(tuple2,this.context,this.workingMemory);
  assertSame(((Map)tuple2.getTupleMatches()).get(f0),matches.getFirstTupleMatch().getNext());
  FactHandleImpl f3=(FactHandleImpl)this.workingMemory.assertObject("test2");
  this.node.assertObject(f3,this.context,this.workingMemory);
  matches=getMatchesFor(tuple1,f3);
  assertSame(((Map)tuple1.getTupleMatches()).get(f3),matches.getFirstTupleMatch());
  assertSame(((Map)tuple2.getTupleMatches()).get(f3),matches.getFirstTupleMatch().getNext());
}
