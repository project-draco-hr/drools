{
  boolean hasDialect=false;
  for (int i=0; i < model.attributes.length; i++) {
    RuleAttribute attr=model.attributes[i];
    buf.append("\t");
    buf.append(attr);
    buf.append("\n");
    if (attr.attributeName.equals("dialect")) {
      hasDialect=true;
    }
  }
  if (!hasDialect) {
    RuleAttribute attr=new RuleAttribute("dialect","mvel");
    buf.append("\t");
    buf.append(attr);
    buf.append("\n");
  }
}
