{
  config.setOption(AssertBehaviorOption.EQUALITY);
  assertEquals(AssertBehaviorOption.EQUALITY,config.getOption(AssertBehaviorOption.class));
  assertEquals("equality",config.getProperty(AssertBehaviorOption.PROPERTY_NAME));
  config.setProperty(AssertBehaviorOption.PROPERTY_NAME,"identity");
  assertEquals(AssertBehaviorOption.IDENTITY,config.getOption(AssertBehaviorOption.class));
  assertEquals("identity",config.getProperty(AssertBehaviorOption.PROPERTY_NAME));
}
