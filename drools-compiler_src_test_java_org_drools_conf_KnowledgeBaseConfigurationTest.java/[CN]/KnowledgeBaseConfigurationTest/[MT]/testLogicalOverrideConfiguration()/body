{
  config.setOption(LogicalOverrideOption.PRESERVE);
  assertEquals(LogicalOverrideOption.PRESERVE,config.getOption(LogicalOverrideOption.class));
  assertEquals("preserve",config.getProperty(LogicalOverrideOption.PROPERTY_NAME));
  config.setProperty(LogicalOverrideOption.PROPERTY_NAME,"discard");
  assertEquals(LogicalOverrideOption.DISCARD,config.getOption(LogicalOverrideOption.class));
  assertEquals("discard",config.getProperty(LogicalOverrideOption.PROPERTY_NAME));
}
