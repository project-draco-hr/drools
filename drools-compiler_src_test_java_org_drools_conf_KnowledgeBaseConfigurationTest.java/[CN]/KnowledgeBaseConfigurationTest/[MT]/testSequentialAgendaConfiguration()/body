{
  config.setOption(SequentialAgendaOption.DYNAMIC);
  assertEquals(SequentialAgendaOption.DYNAMIC,config.getOption(SequentialAgendaOption.class));
  assertEquals("dynamic",config.getProperty(SequentialAgendaOption.PROPERTY_NAME));
  config.setProperty(SequentialAgendaOption.PROPERTY_NAME,"sequential");
  assertEquals(SequentialAgendaOption.SEQUENTIAL,config.getOption(SequentialAgendaOption.class));
  assertEquals("sequential",config.getProperty(SequentialAgendaOption.PROPERTY_NAME));
}
