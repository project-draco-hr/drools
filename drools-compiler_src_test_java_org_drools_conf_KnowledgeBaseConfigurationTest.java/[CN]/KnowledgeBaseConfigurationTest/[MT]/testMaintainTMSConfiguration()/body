{
  config.setOption(MaintainTMSOption.YES);
  assertEquals(MaintainTMSOption.YES,config.getOption(MaintainTMSOption.class));
  assertEquals("true",config.getProperty(MaintainTMSOption.PROPERTY_NAME));
  config.setProperty(MaintainTMSOption.PROPERTY_NAME,"false");
  assertEquals(MaintainTMSOption.NO,config.getOption(MaintainTMSOption.class));
  assertEquals("false",config.getProperty(MaintainTMSOption.PROPERTY_NAME));
}
