{
  Function function=(Function)this.functions.get(name);
  if (function == null) {
    function=this.builtin.getFunction(name);
    if (function == null) {
      function=new FunctionDelegator(name);
    }
 else {
      function=new FunctionDelegator(function);
    }
    this.functions.put(name,function);
  }
  return function;
}
