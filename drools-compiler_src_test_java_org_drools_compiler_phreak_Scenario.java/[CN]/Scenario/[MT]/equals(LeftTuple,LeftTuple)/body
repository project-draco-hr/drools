{
  if (expected == actual) {
    return true;
  }
  if (expected == null) {
    if (actual == null) {
      return true;
    }
 else {
      return false;
    }
  }
 else   if (actual == null) {
    if (expected == null) {
      return true;
    }
 else {
      return false;
    }
  }
  while (actual.getLastHandle() == null) {
    actual=actual.getParent();
  }
  if (expected.hashCode() != actual.hashCode()) {
    return false;
  }
  if (expected.getLastHandle() != actual.getLastHandle()) {
    return false;
  }
  return equals(expected.getParent(),actual.getParent());
}
