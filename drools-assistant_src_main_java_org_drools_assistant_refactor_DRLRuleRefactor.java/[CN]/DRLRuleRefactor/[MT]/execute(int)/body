{
  this.offset=offset;
  RuleBasicContentInfo contentInfo=((DRLRuleRefactorInfo)ruleRefactorInfo).getContentAt(offset);
  if (contentInfo == null)   return this.options;
  if ((option=this.bindVariable(contentInfo)) != null)   this.options.add(option);
  if ((option=this.fixImports(contentInfo)) != null)   this.options.add(option);
  if ((option=this.renameVariable(contentInfo)) != null)   this.options.add(option);
  return this.options;
}
