{
  if (contentInfo == null)   return null;
  if (!contentInfo.getType().equals(DRLContentTypeEnum.RULE_LHS_LINE) && !contentInfo.getType().equals(DRLContentTypeEnum.RULE_RHS_LINE))   return null;
  String variable;
  if ((variable=VariableRename.isPossible(contentInfo,offset - contentInfo.getOffset())) != null)   return new RenameAssistantOption("rename variable",variable,contentInfo,contentInfo.getOffset());
  return null;
}
