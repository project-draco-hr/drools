{
  try {
    match(input,DRLLexer.ID,DroolsSoftKeywords.THEN,null,DroolsEditorType.KEYWORD);
    match(input,DRLLexer.LEFT_SQUARE,null,null,DroolsEditorType.SYMBOL);
    Token label=match(input,DRLLexer.ID,null,null,DroolsEditorType.SYMBOL);
    int first=input.index();
    match(input,DRLLexer.RIGHT_SQUARE,null,null,DroolsEditorType.SYMBOL);
    if (state.failed)     return;
    String name=label.getText();
    String chunk=getConsequenceCode(first);
    chunk=chunk.replaceFirst("^\\]\\s*\\r?\\n?","");
    rule.namedRhs(name,chunk);
  }
 catch (  RecognitionException re) {
    reportError(re);
  }
}
