{
  int cursor=0;
  int lastToken=input.LA(++cursor);
  while (true) {
    int nextToken=input.LA(++cursor);
switch (lastToken) {
case DRLLexer.ID:
      if (nextToken != DRLLexer.DOT && nextToken != DRLLexer.SHARP) {
        return -1;
      }
    break;
case DRLLexer.DOT:
  if (nextToken == DRLLexer.LEFT_PAREN) {
    return cursor;
  }
case DRLLexer.SHARP:
if (nextToken != DRLLexer.ID) {
  return -1;
}
break;
default :
return -1;
}
lastToken=nextToken;
}
}
