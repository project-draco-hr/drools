{
  try {
    FieldValue value=this.expression.evaluate(handle.getObject(),null,this.previousDeclarations,this.localDeclarations,workingMemory,null);
    return this.evaluator.evaluate(workingMemory,this.readAccessor,handle.getObject(),value);
  }
 catch (  final Exception e) {
    throw new RuntimeDroolsException(e);
  }
}
