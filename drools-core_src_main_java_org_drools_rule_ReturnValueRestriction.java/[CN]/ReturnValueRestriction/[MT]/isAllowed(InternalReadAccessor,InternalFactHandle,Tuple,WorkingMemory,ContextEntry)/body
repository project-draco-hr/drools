{
  try {
    return this.evaluator.evaluate(null,this.readAccessor,handle.getObject(),this.expression.evaluate(handle.getObject(),tuple,this.previousDeclarations,this.localDeclarations,workingMemory,((ReturnValueContextEntry)context).dialectContext));
  }
 catch (  final Exception e) {
    throw new RuntimeDroolsException(e);
  }
}
