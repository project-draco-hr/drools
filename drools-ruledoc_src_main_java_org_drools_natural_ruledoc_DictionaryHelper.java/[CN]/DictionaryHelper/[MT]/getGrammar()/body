{
  SimpleGrammar grammar=new SimpleGrammar();
  for (Iterator iter=props.keySet().iterator(); iter.hasNext(); ) {
    String key=(String)iter.next();
    if (key.equals("import") || key.equals("imports") || key.equals("functions")|| key.equals("application-data")|| key.equals("ruleset-name")) {
    }
 else {
      grammar.addToDictionary(key,props.getProperty(key));
    }
  }
  return grammar;
}
