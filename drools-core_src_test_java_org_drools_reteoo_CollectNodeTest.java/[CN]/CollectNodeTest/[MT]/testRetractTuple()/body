{
  final DefaultFactHandle f0=(DefaultFactHandle)this.workingMemory.getFactHandleFactory().newFactHandle("cheese");
  final ReteTuple tuple0=new ReteTuple(f0);
  this.node.assertTuple(tuple0,this.contextAssert,this.workingMemory);
  assertEquals(1,this.memory.getTupleMemory().size());
  assertEquals(0,this.memory.getFactHandleMemory().size());
  Assert.assertTrue("An empty collection should be propagated",((Collection)((DefaultFactHandle)((Tuple)((Object[])this.sink.getAsserted().get(0))[0]).get(1)).getObject()).isEmpty());
  this.node.retractTuple(tuple0,this.contextRetract,this.workingMemory);
  assertEquals(0,this.memory.getTupleMemory().size());
  assertEquals(1,this.sink.getRetracted().size());
  assertEquals(1,this.sink.getAsserted().size());
}
