{
  if (this.hasMemory) {
    ObjectHashMap map=(ObjectHashMap)workingMemory.getNodeMemory(this);
    LinkedList list=(LinkedList)map.get(handle);
    this.sink.createAndPropagateModifyTuple(list,context,workingMemory);
  }
 else {
    ReteTuple tuple=new ReteTuple(handle);
    this.sink.createAndPropagateModifyTuple(tuple,context,workingMemory);
  }
}
