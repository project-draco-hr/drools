{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final Illegal other=(Illegal)obj;
  if (basePoint != other.basePoint)   return false;
  if (labelId == null) {
    if (other.labelId != null)     return false;
  }
 else   if (!labelId.equals(other.labelId))   return false;
  return true;
}
