{
  try {
    if (!isPossibleToGenerateTheProxyFor(clazz)) {
      return null;
    }
    final String className=getInternalProxyClassNameForClass(clazz);
    final byte[] bytes=dump(clazz,className);
    return bytes;
  }
 catch (  final Exception e) {
    throw new RuntimeDroolsException(e);
  }
}
