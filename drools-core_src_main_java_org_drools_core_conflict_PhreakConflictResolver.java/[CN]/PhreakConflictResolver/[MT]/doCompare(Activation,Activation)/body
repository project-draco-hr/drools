{
  final int s1=existing.getSalience();
  final int s2=adding.getSalience();
  if (s1 > s2) {
    return 1;
  }
 else   if (s1 < s2) {
    return -1;
  }
  final int l1=existing.getRule().getLoadOrder();
  final int l2=adding.getRule().getLoadOrder();
  if (l1 < l2) {
    return 1;
  }
 else   if (l1 > l2) {
    return -1;
  }
 else {
    return 0;
  }
}
