{
  long id=handle.getId();
  FactEntry previous=this.first;
  FactEntry current=previous;
  while (current != null) {
    FactEntry next=(FactEntry)current.getNext();
    if (current.getFactHandle().getId() == id) {
      this.first=next;
      previous.setNext(next);
      current.setNext(null);
      return current;
    }
    previous=current;
    current=next;
  }
  return current;
}
