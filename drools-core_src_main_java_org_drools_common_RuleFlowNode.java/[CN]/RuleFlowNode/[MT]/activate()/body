{
  Activation[] activations=null;
  int i=0;
synchronized (this.lock) {
    activations=new Activation[this.queue.size()];
    while (!this.queue.isEmpty()) {
      activations[i++]=(Activation)this.queue.dequeue();
    }
  }
  final ExecuteRuleFlowNode execute=new ExecuteRuleFlowNode(this.agenda,(RuleFlowNode[])this.childNodes.toArray(new RuleFlowNode[this.childNodes.size()]),activations);
  final Thread thread=new Thread(execute);
  thread.start();
}
