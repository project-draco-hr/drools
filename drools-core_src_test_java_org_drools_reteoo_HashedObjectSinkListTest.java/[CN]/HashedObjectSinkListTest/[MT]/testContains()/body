{
  final MockObjectSink mock=new MockObjectSink();
  final MockObjectSource source=new MockObjectSource(1);
  final AlphaNode node=new AlphaNode(10,null,source);
  this.list.add(mock);
  this.list.add(node);
  Assert.assertTrue("List should contain the added sink",this.list.contains(mock));
  Assert.assertTrue("List should contain the added sink",this.list.contains(node));
}
