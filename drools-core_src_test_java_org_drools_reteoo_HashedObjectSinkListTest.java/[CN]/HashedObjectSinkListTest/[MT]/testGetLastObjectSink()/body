{
  final MockObjectSink mock=new MockObjectSink();
  final MockObjectSource source=new MockObjectSource(1);
  final AlphaNode node=new AlphaNode(10,null,source);
  Assert.assertNull("Invalid last added sink",this.list.getLastObjectSink());
  this.list.add(mock);
  Assert.assertSame("Invalid last added sink",mock,this.list.getLastObjectSink());
  this.list.add(node);
  Assert.assertSame("Invalid last added sink",node,this.list.getLastObjectSink());
  this.list.remove(mock);
  Assert.assertSame("Invalid last added sink",node,this.list.getLastObjectSink());
  this.list.remove(node);
  Assert.assertNull("Invalid last added sink",this.list.getLastObjectSink());
}
