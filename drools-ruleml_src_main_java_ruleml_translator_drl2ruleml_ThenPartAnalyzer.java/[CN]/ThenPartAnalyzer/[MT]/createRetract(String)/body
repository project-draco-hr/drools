{
  List<CommonToken> tokens=getTokensFromThenPart(retract);
  if (tokens.size() != 1) {
    throw new RuntimeException("The retract statement in the Then-part does not match the pattern: retract ($Var)");
  }
  JAXBElement<OidType> oid=Drools2RuleMLTranslator.builder.createOid(tokens.get(0).getText());
  return Drools2RuleMLTranslator.builder.createAtom(new JAXBElement<?>[]{oid});
}
