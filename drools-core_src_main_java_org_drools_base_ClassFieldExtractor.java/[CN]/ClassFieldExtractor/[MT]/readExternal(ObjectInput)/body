{
  clazz=(Class)is.readObject();
  fieldName=(String)is.readObject();
  if (is instanceof DroolsObjectInput) {
    DroolsObjectInput droolsInput=(DroolsObjectInput)is;
    extractor=droolsInput.getExtractorFactory().getExtractor(clazz,fieldName,droolsInput.getClassLoader());
  }
 else   extractor=ClassFieldExtractorCache.getInstance().getExtractor(clazz,fieldName,getClass().getClassLoader());
}
