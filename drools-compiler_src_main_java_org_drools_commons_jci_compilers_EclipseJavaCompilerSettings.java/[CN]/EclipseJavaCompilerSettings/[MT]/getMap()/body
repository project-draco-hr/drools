{
  final Map map=new HashMap(defaultEclipseSettings);
  map.put(CompilerOptions.OPTION_ReportDeprecation,CompilerOptions.GENERATE);
  map.put(CompilerOptions.OPTION_TargetPlatform,(getTargetVersion() != null) ? getTargetVersion() : CompilerOptions.VERSION_1_4);
  map.put(CompilerOptions.OPTION_Source,(getSourceVersion() != null) ? getSourceVersion() : CompilerOptions.VERSION_1_4);
  map.put(CompilerOptions.OPTION_Encoding,(getSourceEncoding() != null) ? getSourceEncoding() : "UTF-8");
  return map;
}
