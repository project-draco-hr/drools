{
  InputStream inputStream=null;
  try {
    PackageBuilder builder=new PackageBuilder();
    builder.addPackageFromDrl(new InputStreamReader(RuleEngineTestBase.class.getResourceAsStream(bindUri)));
    Package pkg=builder.getPackage();
    RuleExecutionSet ruleExecutionSet=ruleSetProvider.createRuleExecutionSet(pkg,null);
    assertEquals("rule set name","SistersRules",ruleExecutionSet.getName());
    assertEquals("number of rules",1,ruleExecutionSet.getRules().size());
  }
 catch (  IOException e) {
    fail("Couldn't create the RuleExecutionSet. " + "Test threw an IOException.");
  }
 finally {
    if (inputStream != null) {
      try {
        inputStream.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
}
