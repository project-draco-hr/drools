{
  final List<String> list=new ArrayList<String>();
  list.addAll(Arrays.asList(classes));
  list.add(KieCDIExtension.class.getName());
  list.add(KBase.class.getName());
  list.add(KSession.class.getName());
  list.add(KGAV.class.getName());
  Weld weld=new Weld(){
    @Override protected Deployment createDeployment(    ResourceLoader resourceLoader,    Bootstrap bootstrap){
      return new TestWeldSEDeployment(resourceLoader,bootstrap,list);
    }
  }
;
  return weld;
}
