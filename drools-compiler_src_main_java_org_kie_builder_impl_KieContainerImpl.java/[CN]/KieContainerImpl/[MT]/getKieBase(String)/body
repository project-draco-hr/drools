{
  KieBase kBase=kBases.get(kBaseName);
  if (kBase == null) {
    if (kProject != null) {
      kBase=AbstractKieModules.createKieBase(kProject.getKieBaseModel(kBaseName),kProject);
      if (kBase != null) {
        kBases.put(kBaseName,kBase);
      }
    }
 else {
      return KnowledgeBaseFactory.newKnowledgeBase();
    }
  }
  return kBase;
}
