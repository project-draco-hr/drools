{
  int arrayDim=externalArrayDimSize(className);
  String prefix="L";
  String coreType=arrayDim == 0 ? className : className.substring(0,className.indexOf("["));
  if (arrayDim > 0) {
    for (int j=0; j < arrayDim; j++) {
      prefix="[" + prefix;
    }
  }
 else {
    return className;
  }
  return prefix + coreType + ";";
}
