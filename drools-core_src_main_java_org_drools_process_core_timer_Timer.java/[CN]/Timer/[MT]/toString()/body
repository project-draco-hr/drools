{
  String result="Timer";
  if (delay != null || period != null) {
    result+="[";
    if (delay != null) {
      result+="delay=" + delay;
      if (period != null) {
        result+=", ";
      }
    }
    if (period != null) {
      result+="period=" + period;
    }
    result+="]";
  }
  return result;
}
