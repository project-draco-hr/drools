{
  xmlPackageReader.startConfiguration(localName,attrs);
  final String evaluator=attrs.getValue("evaluator");
  emptyAttributeCheck(localName,"evaluator",evaluator,xmlPackageReader);
  final String text=attrs.getValue("value");
  final LiteralRestrictionDescr literalDescr=new LiteralRestrictionDescr(evaluator,text);
  return literalDescr;
}
