{
  src.addFile(className.replace('.','/') + ".java",text.toCharArray());
  this.results=compiler.compile(new String[]{className},src,dst);
  System.out.println("-------------");
  CompilationProblem[] problems=this.results.getErrors();
  for (int i=0, length=problems.length; i < length; i++) {
    System.out.println(problems[i]);
  }
  return results;
}
