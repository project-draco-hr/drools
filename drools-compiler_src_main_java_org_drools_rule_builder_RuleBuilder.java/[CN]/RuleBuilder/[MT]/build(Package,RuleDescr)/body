{
  this.context=new RuleBuildContext(pkg,ruleDescr);
  setAttributes(this.context.getRule(),ruleDescr.getAttributes());
  this.context.setDialect(this.dialect);
  final ConditionalElementBuilder builder=(ConditionalElementBuilder)context.getDialect().getBuilder(ruleDescr.getLhs().getClass());
  if (builder != null) {
    final GroupElement ce=(GroupElement)builder.build(this.context,ruleDescr.getLhs());
    this.context.getRule().setLhs(ce);
  }
 else {
    throw new RuntimeDroolsException("BUG: builder not found for descriptor class " + ruleDescr.getLhs().getClass());
  }
  if (!(ruleDescr instanceof QueryDescr)) {
    this.dialect.getConsequenceBuilder().build(this.context,ruleDescr);
  }
  this.dialect.getRuleClassBuilder().buildRule(this.context,ruleDescr);
  return this.context.getRule();
}
