{
  final KieSessionConfiguration conf=KieSessionUtil.getKieSessionConfigurationWithClock(ClockTypeOption.get("pseudo"),getSessionProperties());
  session=KieBaseUtil.getKieBaseFromReleaseIdByName(RELEASE_ID,KIE_BASE_NAME).newKieSession(conf,KieServices.Factory.get().newEnvironment());
  clock=session.getSessionClock();
  eventFactType=session.getKieBase().getFactType(this.getClass().getPackage().getName(),"Event");
}
