{
  Object retval=execParser("compilation_unit","\nrule rule1 \n  auto-focus true\n  when\n  \tnot Cheese(type == \"stilton\")\n  then\n\tfunky();\nend",false);
  Object actual=examineParserExecResult(10,retval);
