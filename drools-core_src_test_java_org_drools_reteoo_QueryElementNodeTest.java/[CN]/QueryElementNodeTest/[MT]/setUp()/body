{
  this.ruleBase=(ReteooRuleBase)RuleBaseFactory.newRuleBase();
  this.buildContext=new BuildContext(ruleBase,((ReteooRuleBase)ruleBase).getReteooBuilder().getIdGenerator());
  this.context=new PropagationContextImpl(0,PropagationContext.ASSERTION,null,null,null);
  this.workingMemory=new InstrumentedWorkingMemory(0,(InternalRuleBase)this.ruleBase);
}
