{
  if (!INVOKER_REGISTRY.contains("invokers")) {
    ParserConfiguration pconf=new ParserConfiguration();
    pconf.setClassLoader(cl);
    ParserContext pctx=new ParserContext(pconf);
    INVOKER_REGISTRY.addNamedTemplate("invokers",TemplateCompiler.compileTemplate(AbstractJavaRuleBuilder.class.getResourceAsStream("javaInvokers.mvel"),pctx));
  }
  return INVOKER_REGISTRY;
}
