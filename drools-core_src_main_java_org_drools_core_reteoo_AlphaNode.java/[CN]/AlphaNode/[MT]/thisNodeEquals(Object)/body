{
  if (this == object) {
    return true;
  }
  if (!(object instanceof AlphaNode) || hashCode() != object.hashCode()) {
    return false;
  }
  final AlphaNode other=(AlphaNode)object;
  return this.nodeHashCode() == other.nodeHashCode() && constraint instanceof MvelConstraint ? ((MvelConstraint)constraint).equals(other.constraint,getKnowledgeBase()) : constraint.equals(other.constraint);
}
