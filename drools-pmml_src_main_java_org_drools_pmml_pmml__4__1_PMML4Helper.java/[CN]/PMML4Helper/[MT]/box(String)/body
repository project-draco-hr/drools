{
  if ("int".equalsIgnoreCase(s))   return Integer.class.getName();
 else   if ("float".equalsIgnoreCase(s))   return Float.class.getName();
 else   if ("double".equalsIgnoreCase(s))   return Double.class.getName();
 else   if ("boolean".equalsIgnoreCase(s))   return Boolean.class.getName();
 else   if ("short".equalsIgnoreCase(s))   return Short.class.getName();
 else   if ("long".equalsIgnoreCase(s))   return Long.class.getName();
 else   return s;
}
