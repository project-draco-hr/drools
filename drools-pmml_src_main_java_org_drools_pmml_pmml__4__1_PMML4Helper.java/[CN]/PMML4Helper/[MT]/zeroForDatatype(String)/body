{
  if (type == null) {
    return "null";
  }
  if (Integer.class.getName().equals(type) || "Integer".equalsIgnoreCase(type) || "int".equalsIgnoreCase(type)) {
    return "0";
  }
 else   if (Float.class.getName().equals(type) || "Float".equalsIgnoreCase(type) || "float".equalsIgnoreCase(type)) {
    return "0.0f";
  }
 else   if (Double.class.getName().equalsIgnoreCase(type) || "Double".equalsIgnoreCase(type) || "double".equalsIgnoreCase(type)) {
    return "0.0";
  }
 else   if (Long.class.getName().equalsIgnoreCase(type) || "Long".equalsIgnoreCase(type) || "long".equalsIgnoreCase(type)) {
    return "0L";
  }
 else   if (Short.class.getName().equalsIgnoreCase(type) || "Short".equalsIgnoreCase(type) || "short".equalsIgnoreCase(type)) {
    return "0";
  }
 else   if (Byte.class.getName().equalsIgnoreCase(type) || "Byte".equalsIgnoreCase(type) || "byte".equalsIgnoreCase(type)) {
    return "0";
  }
 else   if (Boolean.class.getName().equalsIgnoreCase(type) || "Boolean".equalsIgnoreCase(type) || "boolean".equalsIgnoreCase(type)) {
    return "false";
  }
 else   if (String.class.getName().equalsIgnoreCase(type) || "String".equalsIgnoreCase(type)) {
    return "null";
  }
 else   return "null";
}
