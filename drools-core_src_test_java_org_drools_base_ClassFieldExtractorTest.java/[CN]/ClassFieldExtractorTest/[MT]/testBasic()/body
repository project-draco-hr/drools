{
  final Object[] objArray=new Object[1];
  final TestBean obj=new TestBean();
  obj.setBlah(false);
  obj.setSomething("no");
  obj.setObjArray(objArray);
  final ClassFieldExtractor ext=ClassFieldExtractorCache.getExtractor(TestBean.class,"blah",getClass().getClassLoader());
  assertEquals(false,((Boolean)ext.getValue(obj)).booleanValue());
  final ClassFieldExtractor ext2=ClassFieldExtractorCache.getExtractor(TestBean.class,"fooBar",getClass().getClassLoader());
  assertEquals("fooBar",ext2.getValue(obj));
  final ClassFieldExtractor ext3=ClassFieldExtractorCache.getExtractor(TestBean.class,"objArray",getClass().getClassLoader());
  assertEquals(objArray,ext3.getValue(obj));
}
