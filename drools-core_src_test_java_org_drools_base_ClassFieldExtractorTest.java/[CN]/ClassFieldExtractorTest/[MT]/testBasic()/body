{
  final TestBean obj=new TestBean();
  obj.setBlah(false);
  obj.setSomething("no");
  final ClassFieldExtractor ext=new ClassFieldExtractor(TestBean.class,"blah");
  assertEquals(false,((Boolean)ext.getValue(obj)).booleanValue());
  final ClassFieldExtractor ext2=new ClassFieldExtractor(TestBean.class,"fooBar");
  assertEquals("fooBar",ext2.getValue(obj));
}
