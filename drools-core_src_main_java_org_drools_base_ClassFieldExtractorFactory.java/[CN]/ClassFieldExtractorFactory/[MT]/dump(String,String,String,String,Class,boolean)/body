 else {
    mv=cw.visitMethod(Opcodes.ACC_PUBLIC,"getValue","(Ljava/lang/Object;)Ljava/lang/Object;",null,null);
    mv.visitCode();
    Label l0=new Label();
    mv.visitLabel(l0);
    mv.visitLineNumber(15,l0);
    mv.visitVarInsn(Opcodes.ALOAD,1);
    mv.visitTypeInsn(Opcodes.CHECKCAST,originalClassName);
    if (isInterface) {
      mv.visitMethodInsn(Opcodes.INVOKEINTERFACE,originalClassName,getterName,"()L" + typeName + ";");
    }
 else {
      mv.visitMethodInsn(Opcodes.INVOKEVIRTUAL,originalClassName,getterName,"()L" + typeName + ";");
