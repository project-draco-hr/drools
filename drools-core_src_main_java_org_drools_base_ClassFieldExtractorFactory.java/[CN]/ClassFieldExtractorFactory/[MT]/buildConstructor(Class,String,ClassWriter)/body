{
  MethodVisitor mv;
{
    mv=cw.visitMethod(Opcodes.ACC_PUBLIC,"<init>",Type.getMethodDescriptor(Type.VOID_TYPE,new Type[]{Type.getType(Class.class),Type.getType(String.class)}),null,null);
    mv.visitCode();
    final Label l0=new Label();
    mv.visitLabel(l0);
    mv.visitVarInsn(Opcodes.ALOAD,0);
    mv.visitVarInsn(Opcodes.ALOAD,1);
    mv.visitVarInsn(Opcodes.ALOAD,2);
    mv.visitMethodInsn(Opcodes.INVOKESPECIAL,Type.getInternalName(superClazz),"<init>",Type.getMethodDescriptor(Type.VOID_TYPE,new Type[]{Type.getType(Class.class),Type.getType(String.class)}));
    final Label l1=new Label();
    mv.visitLabel(l1);
    mv.visitInsn(Opcodes.RETURN);
    final Label l2=new Label();
    mv.visitLabel(l2);
    mv.visitLocalVariable("this","L" + className + ";",null,l0,l2,0);
    mv.visitLocalVariable("clazz",Type.getDescriptor(Class.class),null,l0,l2,1);
    mv.visitLocalVariable("fieldName",Type.getDescriptor(String.class),null,l0,l2,2);
    mv.visitMaxs(0,0);
    mv.visitEnd();
  }
}
