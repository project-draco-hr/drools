{
  xmlPackageReader.startConfiguration(localName,attrs);
  String expression=attrs.getValue("expression");
  if (expression == null || expression.trim().equals("")) {
    throw new SAXParseException("<eval> must have some content",xmlPackageReader.getLocator());
  }
  EvalDescr evalDescr=new EvalDescr(expression);
  return evalDescr;
}
