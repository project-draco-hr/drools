{
  String rulesUri=RuleEngineTestBase.class.getResource(bindUri).toExternalForm();
  RuleExecutionSet testRuleSet=ruleSetProvider.createRuleExecutionSet(rulesUri,null);
  assertEquals("rule set name","SistersRules",testRuleSet.getName());
  assertEquals("number of rules",1,testRuleSet.getRules().size());
}
