{
  if (leftTuple != null) {
    int[] tuple=new int[leftTuple.size()];
    int i=tuple.length;
    for (LeftTuple entry=leftTuple; entry != null && i > 0; entry=entry.getParent()) {
      if (entry.getLastHandle() != null) {
        tuple[--i]=entry.getLastHandle().getId();
      }
    }
    return tuple;
  }
 else {
    return new int[0];
  }
}
