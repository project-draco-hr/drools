{
  grid.getCellAt(0,0).setCellState(CellState.LIVE);
  grid.getCellAt(0,1).setCellState(CellState.LIVE);
  grid.getCellAt(0,2).setCellState(CellState.LIVE);
  grid.getCellAt(1,0).setCellState(CellState.LIVE);
  grid.getCellAt(1,1).setCellState(CellState.LIVE);
  grid.nextGeneration();
  assertEquals("Overcrowded cell should have been killed",CellState.DEAD,grid.getCellAt(1,1).getCellState());
}
