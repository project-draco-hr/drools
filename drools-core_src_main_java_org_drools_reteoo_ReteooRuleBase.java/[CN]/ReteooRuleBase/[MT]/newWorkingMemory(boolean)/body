{
  final ReteooWorkingMemory workingMemory=new ReteooWorkingMemory(this.workingMemoryCounter++,this);
  super.addWorkingMemory(workingMemory,keepReference);
  final InitialFactHandle handle=new InitialFactHandle(workingMemory.getFactHandleFactory().newFactHandle(new InitialFactHandleDummyObject()));
  workingMemory.queueWorkingMemoryAction(workingMemory.new WorkingMemoryReteAssertAction(handle,false,true,null,null));
  return workingMemory;
}
