{
  ExecutorService executor=this.config.getExecutorService();
  final ReteooStatefulSession session=new ReteooStatefulSession(this.workingMemoryCounter++,this,executor);
  executor.setCommandExecutor(new CommandExecutor(session));
  if (keepReference) {
    super.addStatefulSession(session);
  }
  final InitialFactHandle handle=new InitialFactHandle(session.getFactHandleFactory().newFactHandle(new InitialFactHandleDummyObject()));
  session.queueWorkingMemoryAction(session.new WorkingMemoryReteAssertAction(handle,false,true,null,null));
  return session;
}
