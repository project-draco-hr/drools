{
  byte[] buf=new byte[1024];
  BufferedOutputStream bos=null;
  ByteArrayInputStream bais=null;
  try {
    bos=new BufferedOutputStream(new FileOutputStream(f));
    bais=new ByteArrayInputStream(data);
    int len;
    while ((len=bais.read(buf)) > 0) {
      bos.write(buf,0,len);
    }
  }
  finally {
    if (bos != null) {
      bos.close();
    }
    if (bais != null) {
      bais.close();
    }
  }
  bos.close();
  bais.close();
}
