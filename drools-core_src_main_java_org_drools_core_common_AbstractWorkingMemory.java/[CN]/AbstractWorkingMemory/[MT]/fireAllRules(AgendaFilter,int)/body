{
  if (this.firing.compareAndSet(false,true)) {
    try {
      startOperation();
      ruleBase.readLock();
      executeQueuedActions();
      int fireCount=0;
      fireCount=this.agenda.fireAllRules(agendaFilter,fireLimit);
      return fireCount;
    }
  finally {
      ruleBase.readUnlock();
      endOperation();
      this.firing.set(false);
    }
  }
  return 0;
}
