{
  boolean ret=false;
  if ((tuple != null) && (tuple.getChild() != null) && (tuple.getChild().getLinkedList() != null)) {
    ret=(tuple.getChild().getLinkedList() != this.noMatchList);
    if (ret && (this.innerMemory != null)) {
      ret=this.innerMemory.isPossibleMatch(tuple);
    }
  }
  return ret;
}
