{
  boolean ret=false;
  if ((tuple != null) && (tuple.getChild() != null) && (tuple.getChild().getLinkedList() != null)) {
    ret=((tuple.getChild().getLinkedList() != this.noMatchList) || (!isMatch(this.workingMemory,(ReteTuple)tuple.getNode())));
    if (ret && (this.childMemory != null)) {
      ret=this.childMemory.isPossibleMatch(tuple);
    }
  }
  return ret;
}
