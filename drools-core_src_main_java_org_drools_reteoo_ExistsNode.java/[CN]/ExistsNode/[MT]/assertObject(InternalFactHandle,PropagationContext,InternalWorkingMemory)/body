{
  final BetaMemory memory=(BetaMemory)workingMemory.getNodeMemory(this);
  memory.getRightTupleMemory().add(factHandle);
  if (!this.tupleMemoryEnabled) {
    return;
  }
  final Iterator it=memory.getLeftTupleMemory().iterator(factHandle);
  this.constraints.updateFromFactHandle(memory.getContext(),workingMemory,factHandle);
  for (LeftTuple tuple=(LeftTuple)it.next(); tuple != null; tuple=(LeftTuple)it.next()) {
    if (this.constraints.isAllowedCachedRight(memory.getContext(),tuple) && tuple.getMatch() == null) {
      tuple.setMatch(factHandle);
      this.sink.propagateAssertLeftTuple(tuple,context,workingMemory);
    }
  }
  this.constraints.resetFactHandle(memory.getContext());
}
