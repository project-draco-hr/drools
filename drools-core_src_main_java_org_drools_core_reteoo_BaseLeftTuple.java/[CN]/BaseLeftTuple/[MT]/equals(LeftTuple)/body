{
  if (other == this) {
    return true;
  }
 else   if (other == null) {
    return false;
  }
  if (this.hashCode() != other.hashCode()) {
    return false;
  }
  if (getFactHandle() != other.getFactHandle()) {
    return false;
  }
  if (this.parent == null) {
    return (other.getParent() == null);
  }
 else {
    return this.parent.equals(other.getParent());
  }
}
