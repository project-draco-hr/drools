{
  final From from=(From)rce;
  context.pushRuleComponent(from);
  BetaConstraints betaConstraints=utils.createBetaNodeConstraint(context,context.getBetaconstraints(),true);
  context.setTupleSource((LeftTupleSource)utils.attachNode(context,new FromNode(context.getNextId(),from.getDataProvider(),context.getTupleSource(),(AlphaNodeFieldConstraint[])context.getAlphaConstraints().toArray(new AlphaNodeFieldConstraint[context.getAlphaConstraints().size()]),betaConstraints,context.isTupleMemoryEnabled(),context,from)));
  context.setAlphaConstraints(null);
  context.setBetaconstraints(null);
  context.popRuleComponent();
}
