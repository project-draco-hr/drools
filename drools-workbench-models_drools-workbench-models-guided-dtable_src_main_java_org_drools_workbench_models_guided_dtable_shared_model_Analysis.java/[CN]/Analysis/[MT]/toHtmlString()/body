{
  StringBuilder htmlBuilder=new StringBuilder("<span>");
  boolean first=true;
  for (  String htmlEntry : list) {
    if (first) {
      first=false;
    }
 else {
      htmlBuilder.append("<br/> ");
    }
    htmlBuilder.append(htmlEntry);
  }
  htmlBuilder.append("</span>");
  return htmlBuilder.toString();
}
