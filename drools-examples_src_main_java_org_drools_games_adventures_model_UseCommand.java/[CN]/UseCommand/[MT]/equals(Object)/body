{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  UseCommand that=(UseCommand)o;
  if (!character.equals(that.character)) {
    return false;
  }
  if (!target.equals(that.target)) {
    return false;
  }
  if (!thing.equals(that.thing)) {
    return false;
  }
  return true;
}
