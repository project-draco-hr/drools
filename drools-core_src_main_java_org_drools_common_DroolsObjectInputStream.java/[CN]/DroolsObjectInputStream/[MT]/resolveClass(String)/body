{
  try {
    Class clazz=primClasses.get(className);
    if (clazz == null) {
      clazz=getClassLoader().loadClass(className);
      if (className.endsWith("ShadowProxy")) {
        System.out.println();
      }
    }
    return clazz;
  }
 catch (  ClassNotFoundException e) {
    return getClass().getClassLoader().loadClass(className);
  }
}
