{
  long id=handle.getId();
  FactEntry previous=this.first;
  FactEntry current=previous;
  while (current != null) {
    FactEntry next=(FactEntry)current.next;
    if (current.handle.getId() == id) {
      if (this.first == current) {
        this.first=next;
      }
 else {
        previous.next=next;
      }
      current.next=null;
      return current;
    }
    previous=current;
    current=next;
  }
  return current;
}
