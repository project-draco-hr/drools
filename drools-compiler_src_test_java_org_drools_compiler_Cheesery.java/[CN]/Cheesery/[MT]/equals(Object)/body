{
  if (this == obj) {
    return true;
  }
 else   if (obj instanceof Cheesery) {
    Cheesery that=(Cheesery)obj;
    return cheeses.equals(that.cheeses) && status == that.status && totalAmount == that.totalAmount && maturity == that.maturity || maturity != null && maturity.equals(that.maturity);
  }
  return false;
}
