{
  int hashCode=this.index.hashCodeOf(tuple);
  int index=indexOf(hashCode,this.table.length);
  TupleList entry=(TupleList)this.table[index];
  while (entry != null) {
    if (matches(entry,tuple,hashCode,factHandle)) {
      return entry;
    }
    entry=(TupleList)entry.getNext();
  }
  return entry;
}
