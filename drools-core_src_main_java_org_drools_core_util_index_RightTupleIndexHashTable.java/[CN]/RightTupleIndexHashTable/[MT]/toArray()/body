{
  Entry[] result=new Entry[this.factSize];
  int index=0;
  for (int i=0; i < this.table.length; i++) {
    TupleList bucket=(TupleList)this.table[i];
    while (bucket != null) {
      Entry entry=bucket.first;
      while (entry != null) {
        result[index++]=entry;
        entry=entry.getNext();
      }
      bucket=(TupleList)bucket.next;
    }
  }
  return result;
}
