{
  this.workingMemory=workingMemory;
  this.handle=handle;
  for (int i=0; i < alphas.length; i++) {
    if (alphas[i] != null) {
      alphas[i].updateFromFactHandle(workingMemory,handle);
    }
  }
  for (int i=0; i < betas.length; i++) {
    betas[i].updateFromFactHandle(workingMemory,handle);
  }
}
