{
  if (deleteFirst == null) {
    deleteFirst=leftTuple;
  }
 else {
    LeftTuple current=deleteFirst;
    LeftTuple last=null;
    while (current != null) {
      last=current;
      current=current.getStagedNext();
    }
    last.setStagedNext(leftTuple);
    leftTuple.setStagePrevious(leftTuple);
  }
}
