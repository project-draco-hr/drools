{
  if (insertFirst == null) {
    insertFirst=leftTuple;
  }
 else {
    LeftTuple current=insertFirst;
    LeftTuple last=null;
    while (current != null) {
      last=current;
      current=current.getStagedNext();
    }
    last.setStagedNext(leftTuple);
    leftTuple.setStagePrevious(leftTuple);
  }
}
