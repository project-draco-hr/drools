{
  return ((date.get(Calendar.HOUR_OF_DAY) < 10) ? "0" : "") + date.get(Calendar.HOUR_OF_DAY) + ":"+ ((date.get(Calendar.MINUTE) < 10) ? "0" : "")+ date.get(Calendar.MINUTE)+ ":"+ ((date.get(Calendar.SECOND) < 10) ? "0" : "")+ date.get(Calendar.SECOND)+ "."+ ((date.get(Calendar.MILLISECOND) < 10) ? "0" : "")+ ((date.get(Calendar.MILLISECOND) < 100) ? "0" : "")+ date.get(Calendar.MILLISECOND);
}
