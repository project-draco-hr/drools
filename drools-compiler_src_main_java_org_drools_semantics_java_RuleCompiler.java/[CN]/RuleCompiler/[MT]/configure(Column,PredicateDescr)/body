{
  Map root=new HashMap();
  List usedDeclarations=this.analyzer.analyze(predicate.getText(),this.declarations.keySet());
  predicate.setDeclarations((String[])usedDeclarations.toArray(new String[usedDeclarations.size()]));
  root.put("declaration",predicate.getDeclaration());
  root.put("declarations",usedDeclarations);
  root.put("globals",getUsedGlobals(predicate.getText()));
  Template template=this.cfg.getTemplate("predicateMethod.tfl");
  StringWriter string=new StringWriter();
  template.process(root,string);
  string.flush();
  this.invokerMethods.put(predicate,string);
}
