{
  RuleAgent ag=new RuleAgent(new RuleBaseConfiguration());
  Properties props=ag.loadFromProperties("/basic-authentication-sample-agent-config.properties");
  assertEquals("30",props.getProperty(RuleAgent.POLL_INTERVAL));
  assertEquals("http://localhost:8081/drools-guvnor/org.drools.guvnor.Guvnor/package/defaultPackage/LATEST",props.getProperty(RuleAgent.URLS));
  assertEquals("d:/drools",props.getProperty(RuleAgent.LOCAL_URL_CACHE));
  assertEquals("true",props.getProperty(RuleAgent.NEW_INSTANCE));
  assertEquals("admin",props.getProperty(RuleAgent.USER_NAME));
  assertEquals("admin",props.getProperty(RuleAgent.PASSWORD));
}
