{
  if (baseDescr instanceof ConstraintConnectiveDescr) {
    ConstraintConnectiveDescr con=(ConstraintConnectiveDescr)baseDescr;
    if (con.getConnective().equals(this.connective)) {
      for (      BaseDescr descr : con.getDescrs()) {
        addDescr(descr);
      }
    }
 else {
      addDescr(con);
    }
  }
 else {
    addDescr(baseDescr);
  }
}
