{
  AttributeDescr attributeDescr=new AttributeDescr(attributeName.getText());
  attributeDescr.setLocation(getLineLocation(attributeName),getColumnLocation(attributeName));
  attributeDescr.setStartCharacter(getStartOffsetLocation(attributeName));
  if (null != value) {
    attributeDescr.setValue(getCleanId(value));
    attributeDescr.setEndLocation(getLineLocation(value),getEndOffsetLocation(value));
    attributeDescr.setEndCharacter(getEndOffsetLocation(value));
  }
 else {
    attributeDescr.setEndLocation(getLineLocation(attributeName),getEndOffsetLocation(attributeName));
    attributeDescr.setEndCharacter(getEndOffsetLocation(attributeName));
    attributeDescr.setValue("true");
  }
  return attributeDescr;
}
