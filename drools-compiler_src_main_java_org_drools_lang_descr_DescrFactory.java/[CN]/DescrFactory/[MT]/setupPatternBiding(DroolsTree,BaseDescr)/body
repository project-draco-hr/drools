{
  fact.setStartCharacter(getStartOffsetLocation(label));
  if (fact instanceof OrDescr) {
    ((PatternDescr)((OrDescr)fact).getDescrs().get(0)).setIdentifier(label.getText());
    ((PatternDescr)((OrDescr)fact).getDescrs().get(1)).setIdentifier(label.getText());
  }
 else {
    ((PatternDescr)fact).setIdentifier(label.getText());
  }
  return fact;
}
