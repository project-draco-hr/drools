{
  FactTemplateDescr factTemplateDescr=new FactTemplateDescr(getCleanId(id));
  for (  FieldTemplateDescr fieldTemplateDescr : slotList) {
    factTemplateDescr.addFieldTemplate(fieldTemplateDescr);
  }
  factTemplateDescr.setLocation(getLineLocation(start),getColumnLocation(start));
  factTemplateDescr.setEndLocation(getLineLocation(end),getEndOffsetLocation(end));
  factTemplateDescr.setEndCharacter(getEndOffsetLocation(end));
  return factTemplateDescr;
}
