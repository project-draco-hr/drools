{
  if (!processRuntimeInitialized) {
synchronized (this) {
      if (!processRuntimeInitialized) {
        processRuntimeInitialized=true;
        this.processRuntime=createProcessRuntime();
      }
    }
  }
  if (this.processRuntime == null)   throw new RuntimeException("There is no ProcessRuntime available: are jBPM libraries missing on classpath?");
  return this.processRuntime;
}
