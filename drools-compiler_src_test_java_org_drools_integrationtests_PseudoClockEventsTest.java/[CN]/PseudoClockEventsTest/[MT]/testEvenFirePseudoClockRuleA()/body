{
  AgendaEventListener ael=mock(AgendaEventListener.class);
  processStocks(evalFirePseudoClockStockCount,ael,evalFirePseudoClockDeclaration + evalFirePseudoClockRuleA);
  verify(ael,times(evalFirePseudoClockStockCount * (evalFirePseudoClockStockCount - 1) / 2)).afterActivationFired(any(AfterActivationFiredEvent.class));
}
