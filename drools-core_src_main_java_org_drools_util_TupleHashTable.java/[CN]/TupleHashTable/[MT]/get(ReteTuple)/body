{
  int hashCode=tuple.hashCode();
  int index=indexOf(hashCode,table.length);
  ReteTuple current=(ReteTuple)this.table[index];
  while (current != null) {
    if (hashCode == current.hashCode() && tuple.equals(current)) {
      return current;
    }
    current=(ReteTuple)current.getNext();
  }
  return null;
}
