{
  if (mem != null) {
    SegmentMemory smem=mem.getSegmentMemory();
    if (smem != null && !smem.getStagedLeftTuples().isEmpty()) {
      for (      PathMemory pmem : smem.getPathMemories()) {
        if (pmem.getRuleAgendaItem() != null) {
          tnMems.pmemsToBeFlushed.add(pmem);
          break;
        }
      }
    }
  }
}
