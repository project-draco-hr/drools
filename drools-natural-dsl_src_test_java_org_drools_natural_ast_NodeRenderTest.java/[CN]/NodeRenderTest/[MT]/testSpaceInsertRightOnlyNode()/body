{
  LiteralNode lit1=new LiteralNode("bbb");
  RightInfix right=new RightInfix("blah","blah(${1})",1);
  LiteralNode lit2=new LiteralNode("ccc");
  lit1.next=right;
  right.prev=lit1;
  right.next=lit2;
  lit2.prev=right;
  lit1.processLeftToRight(RightInfix.class);
  assertEquals(" blah(ccc)",right.render());
  right.prev=null;
  assertEquals("blah(ccc)",right.render());
}
