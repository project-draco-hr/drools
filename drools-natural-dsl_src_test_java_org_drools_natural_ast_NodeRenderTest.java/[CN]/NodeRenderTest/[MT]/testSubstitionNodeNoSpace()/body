{
  SubstitutionNode sub=new SubstitutionNode("yeah","<<sub");
  LiteralNode lit=new LiteralNode("boo");
  LiteralNode lit2=new LiteralNode("boo");
  lit.next=sub;
  sub.prev=lit;
  sub.next=lit2;
  lit2.prev=sub;
  sub.processLeftToRight(SubstitutionNode.class);
  assertEquals("sub",sub.render());
}
