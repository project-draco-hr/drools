 else           if (((LA30_1 >= '\u0000' && LA30_1 <= '.') || (LA30_1 >= '0' && LA30_1 <= '\uFFFF'))) {
            alt30=1;
          }
        }
 else         if (((LA30_0 >= '\u0000' && LA30_0 <= ')') || (LA30_0 >= '+' && LA30_0 <= '\uFFFF'))) {
          alt30=1;
        }
