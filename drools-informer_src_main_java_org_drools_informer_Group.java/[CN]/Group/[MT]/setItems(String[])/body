{
  if (items == null) {
    this.items=null;
  }
 else {
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < items.length; i++) {
      String temp=items[i];
      if (validItemId(temp)) {
        if (sb.length() > 0) {
          sb.append(COMMA_SEPARATOR);
        }
        sb.append(items[i]);
      }
    }
    if (sb.length() > 0) {
      this.items=sb.toString();
    }
 else {
      this.items=null;
    }
  }
}
