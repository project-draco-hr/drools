{
  if (validItemId(itemId)) {
    List<String> items=new ArrayList<String>(Arrays.asList(getItems()));
    int pos=items.indexOf(itemId);
    if (pos >= 0) {
      if (items.size() == 1) {
        this.items=null;
      }
 else {
        items.remove(itemId);
        setItems(items.toArray());
      }
      return pos;
    }
  }
  return -1;
}
