{
  FactHandle handle=null;
  for (  WorkingMemoryEntryPoint ep : workingMemory.getEntryPoints().values()) {
    handle=(FactHandle)ep.getFactHandle(object);
    if (identityMap != null) {
      identityMap.put(object,handle);
    }
    if (handle != null) {
      break;
    }
  }
  return handle;
}
