{
  RuleTerminalNodeLeftTuple rtnLt1=(RuleTerminalNodeLeftTuple)existing;
  RuleTerminalNodeLeftTuple rtnLt2=(RuleTerminalNodeLeftTuple)adding;
  final int s1=rtnLt1.getSalience();
  final int s2=rtnLt2.getSalience();
  if (s1 > s2) {
    return 1;
  }
 else   if (s1 < s2) {
    return -1;
  }
 else {
    return 0;
  }
}
