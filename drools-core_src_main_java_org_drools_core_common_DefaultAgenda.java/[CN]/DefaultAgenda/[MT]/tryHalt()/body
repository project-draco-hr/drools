{
synchronized (this) {
    PropagationEntry head=workingMemory.takeAllPropagations();
    if (head == null) {
      currentState=ExecutionState.INACTIVE;
      notify();
    }
 else     if (currentState != ExecutionState.FORCE_HALTING) {
      currentState=ExecutionState.REST_HALTING;
    }
    return head;
  }
}
