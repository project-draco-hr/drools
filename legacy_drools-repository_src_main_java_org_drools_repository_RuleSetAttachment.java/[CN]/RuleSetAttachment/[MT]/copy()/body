{
  RuleSetAttachment copy=new RuleSetAttachment(this.getTypeOfAttachment(),this.getName(),this.getContent(),this.getOriginalFileName());
  copy.setTags(Tag.copyTags(this.tags));
  copy.setLastSavedByUser(this.getLastSavedByUser());
  copy.setLastSavedDate(this.getLastSavedDate());
  return copy;
}
