{
  Assert.assertNull("Empty list should return null on getLast()",list.getLast());
  list.add(node1);
  Assert.assertSame("List should return node1 on getLast()",list.getLast(),node1);
  list.add(node2);
  Assert.assertSame("List should return node2 on getLast()",list.getLast(),node2);
  list.add(node3);
  Assert.assertSame("List should return node3 on getLast()",list.getLast(),node3);
}
