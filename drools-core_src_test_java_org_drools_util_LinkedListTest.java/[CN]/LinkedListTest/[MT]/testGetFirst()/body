{
  Assert.assertNull("Empty list should return null on getFirst()",list.getFirst());
  list.add(node1);
  Assert.assertSame("List should return node1 on getFirst()",list.getFirst(),node1);
  list.add(node2);
  Assert.assertSame("List should return node1 on getFirst()",list.getFirst(),node1);
  list.add(node3);
  Assert.assertSame("List should return node1 on getFirst()",list.getFirst(),node1);
}
