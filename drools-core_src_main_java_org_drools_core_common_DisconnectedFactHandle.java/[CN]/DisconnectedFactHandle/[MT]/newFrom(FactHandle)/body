{
  if (handle instanceof DisconnectedFactHandle) {
    return (DisconnectedFactHandle)handle;
  }
 else {
    InternalFactHandle ifh=(InternalFactHandle)handle;
    return new DisconnectedFactHandle(ifh.getId(),ifh.getIdentityHashCode(),ifh.getObjectHashCode(),ifh.getRecency(),ifh.getEntryPoint() != null ? ifh.getEntryPoint().getEntryPointId() : null,null,ifh.isTraitOrTraitable());
  }
}
