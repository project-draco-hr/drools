{
  final Rete rete=ruleBase.getRete();
  final ObjectTypeNode objectTypeNode=new ObjectTypeNode(1,new ClassObjectType(Object.class),buildContext);
  objectTypeNode.attach();
  final ObjectTypeNode stringTypeNode=new ObjectTypeNode(2,new ClassObjectType(String.class),buildContext);
  stringTypeNode.attach();
  final Map map=rete.getObjectTypeNodes();
  assertEquals(2,map.size());
  assertNotNull(map.get(new ClassObjectType(Object.class)));
  assertNotNull(map.get(new ClassObjectType(String.class)));
}
