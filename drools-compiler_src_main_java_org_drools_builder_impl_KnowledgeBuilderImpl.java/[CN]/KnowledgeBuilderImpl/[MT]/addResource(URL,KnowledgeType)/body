{
  try {
switch (type) {
case DRL:
{
        pkgBuilder.addPackageFromDrl(url);
        break;
      }
case DSLR:
{
      break;
    }
case DSL:
{
    if (dslStore == null) {
      dslStore=new StringBuilder();
    }
    break;
  }
case XDRL:
{
  pkgBuilder.addPackageFromXml(url);
  break;
}
case DRF:
{
pkgBuilder.addProcessFromXml(url);
break;
}
case XLS:
{
}
}
}
 catch (Exception e) {
throw new RuntimeException(e);
}
}
