{
  String drl="" + "rule \"EXISTS\"\n" + "  dialect \"mvel\"\n"+ "  when\n"+ "    exists\n"+ " then\n"+ "end";
  RuleModel m=RuleModelDRLPersistenceImpl.getInstance().unmarshal(drl,dmo);
  assertEquals(1,m.lhs.length);
  assertTrue(m.lhs[0] instanceof CompositeFactPattern);
  CompositeFactPattern not=(CompositeFactPattern)m.lhs[0];
  assertEquals("exists",not.getType());
}
