{
  if (this.reseededStack) {
    this.setReseededStack(false);
    final PropagationContextImpl context=new PropagationContextImpl(workingMemory.nextPropagationIdCounter(),PropagationContext.ASSERTION,null,null);
    final DefaultFactHandle startFactHandle=new DefaultFactHandle(workingMemory.getIdLastFireAllAt(),new Object());
    for (final Iterator it=this.iteratorFromPositionToTableStart(startFactHandle,startFactHandle); it.hasNext(); ) {
      final LeapsFactHandle handle=(LeapsFactHandle)it.next();
      workingMemory.pushTokenOnStack(handle,new Token(workingMemory,handle,context));
    }
  }
}
