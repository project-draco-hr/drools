{
  Paragraph title=new Paragraph("\n\n\n\n\n" + packageData.packageName.toUpperCase(),RULE_PACKAGE_TITLE);
  title.setAlignment(Element.ALIGN_CENTER);
  document.add(title);
  Paragraph date=new Paragraph("Documentation created: " + currentDate,BODY_TEXT);
  date.setAlignment(Element.ALIGN_CENTER);
  document.add(date);
  document.add(new Paragraph("\n\n\n\n\nDescription: " + packageData.description,BODY_TEXT));
  document.add(createTable("Metadata: ",packageData.metadata));
  document.add(createTable("Globals: ",packageData.globals));
  createOtherItems(document,packageData.otherInformation);
}
