{
  if (hasMemory()) {
    Map memory=(Map)workingMemory.getNodeMemory(this);
    if (!memory.containsKey(tuple.getKey())) {
      boolean allowed=this.condition.isAllowed(tuple);
      workingMemory.getReteooNodeEventSupport().propagateReteooNode(this,tuple,allowed);
      if (allowed) {
        memory.put(tuple.getKey(),tuple);
        propagateAssertTuple(tuple,context,workingMemory);
      }
    }
  }
 else {
    boolean allowed=this.condition.isAllowed(tuple);
    workingMemory.getReteooNodeEventSupport().propagateReteooNode(this,tuple,allowed);
    if (allowed) {
      propagateAssertTuple(tuple,context,workingMemory);
    }
  }
}
