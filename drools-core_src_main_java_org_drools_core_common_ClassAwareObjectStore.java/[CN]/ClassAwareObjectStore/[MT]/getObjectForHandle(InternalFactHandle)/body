{
  try {
    this.lock.lock();
    InternalFactHandle internalHandle=isEqualityBehaviour ? (InternalFactHandle)equalityMap.get(handle) : (InternalFactHandle)getOrCreateConcreteClassStore(handle.getObject()).getAssertMap().get(handle);
    return internalHandle != null ? internalHandle.getObject() : null;
  }
  finally {
    this.lock.unlock();
  }
}
