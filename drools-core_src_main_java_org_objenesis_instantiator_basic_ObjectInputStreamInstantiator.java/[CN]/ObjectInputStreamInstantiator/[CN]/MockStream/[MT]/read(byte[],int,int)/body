{
  int left=len;
  int remaining=data.length - pointer;
  while (remaining <= left) {
    System.arraycopy(data,pointer,b,off,remaining);
    off+=remaining;
    left-=remaining;
    advanceBuffer();
    remaining=data.length - pointer;
  }
  if (left > 0) {
    System.arraycopy(data,pointer,b,off,left);
    pointer+=left;
  }
  return len;
}
