{
  if (this.top != frame.top) {
    throw new AnalyzerException("Incompatible stack heights");
  }
  boolean changes=false;
  for (int i=0; i < this.locals + this.top; ++i) {
    final Value v=interpreter.merge(this.values[i],frame.values[i]);
    if (v != this.values[i]) {
      this.values[i]=v;
      changes|=true;
    }
  }
  return changes;
}
