{
  List result=new ArrayList();
  Matcher natural1=invalidPattern1.matcher(item.getNaturalTemplate());
  Matcher natural2=invalidPattern2.matcher(item.getNaturalTemplate());
  Matcher target1=invalidPattern1.matcher(item.getTargetTemplate());
  Matcher target2=invalidPattern2.matcher(item.getTargetTemplate());
  while (natural1.find()) {
    String token=natural1.group();
    result.add(new MappingError(MappingError.ERROR_INVALID_TOKEN,MappingError.TEMPLATE_NATURAL,natural1.start(),token));
  }
  while (natural2.find()) {
    String token=natural2.group();
    result.add(new MappingError(MappingError.ERROR_UNMATCHED_BRACES,MappingError.TEMPLATE_NATURAL,natural2.start(),token));
  }
  while (target1.find()) {
    String token=target1.group();
    result.add(new MappingError(MappingError.ERROR_INVALID_TOKEN,MappingError.TEMPLATE_TARGET,target1.start(),token));
  }
  while (target2.find()) {
    String token=target2.group();
    result.add(new MappingError(MappingError.ERROR_UNMATCHED_BRACES,MappingError.TEMPLATE_TARGET,target2.start(),token));
  }
  return result;
}
