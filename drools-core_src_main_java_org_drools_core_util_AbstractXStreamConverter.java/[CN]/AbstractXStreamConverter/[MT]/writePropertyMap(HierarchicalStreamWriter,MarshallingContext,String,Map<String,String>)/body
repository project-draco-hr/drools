{
  if (map != null && !map.isEmpty()) {
    writer.startNode(mapName);
    for (    Map.Entry<String,String> entry : map.entrySet()) {
      writer.startNode("property");
      writer.addAttribute("key",entry.getKey());
      writer.setValue(entry.getValue());
      writer.endNode();
    }
    writer.endNode();
  }
}
