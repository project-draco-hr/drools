{
  return new AgendaFilter(){
    public boolean accept(    Activation activation){
      if (ruleNames.size() == 0)       return true;
      String ruleName=activation.getRule().getName();
      http:       if (inclusive) {
        if (ruleNames.contains(ruleName)) {
          return true;
        }
 else {
          return false;
        }
      }
 else {
        if (!ruleNames.contains(ruleName)) {
          return true;
        }
 else {
          return false;
        }
      }
    }
  }
;
}
