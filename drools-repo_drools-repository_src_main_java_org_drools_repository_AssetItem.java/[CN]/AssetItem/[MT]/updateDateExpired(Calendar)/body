{
  checkout();
  try {
    if (newDateExpired != null || this.node.hasProperty(DATE_EXPIRED_PROPERTY_NAME)) {
      this.node.setProperty(DATE_EXPIRED_PROPERTY_NAME,newDateExpired);
    }
  }
 catch (  Exception e) {
    log.error("Caught Exception",e);
    throw new RulesRepositoryException(e);
  }
}
