{
  FactHandleFactory handleFactory=context.ruleBase.newFactHandleFactory(_session.getRuleData().getLastId(),_session.getRuleData().getLastRecency());
  InternalAgenda agenda=context.ruleBase.getConfiguration().getComponentFactory().getAgendaFactory().createAgenda(context.ruleBase,false);
  readAgenda(context,_session.getRuleData(),agenda);
  WorkingMemoryFactory wmFactory=context.ruleBase.getConfiguration().getComponentFactory().getWorkingMemoryFactory();
  AbstractWorkingMemory session=(AbstractWorkingMemory)wmFactory.createWorkingMemory(id,context.ruleBase,handleFactory,null,1,config,agenda,environment);
  new StatefulKnowledgeSessionImpl(session);
  return session;
}
