{
  final String a="a";
  final String b="b";
  final String c="c";
  final String d="d";
  final And and1=new And();
  and1.addChild(a);
  and1.addChild(b);
  final And and2=new And();
  and2.addChild(c);
  and2.addChild(d);
  and1.addChild(and2);
  final Or or=new Or();
  and1.addChild(or);
  LogicTransformer.getInstance().checkForAndRemoveDuplicates(and1);
  assertLength(5,and1.getChildren());
  assertContains(a,and1.getChildren());
  assertContains(b,and1.getChildren());
  assertContains(c,and1.getChildren());
  assertContains(d,and1.getChildren());
  assertContains(or,and1.getChildren());
}
