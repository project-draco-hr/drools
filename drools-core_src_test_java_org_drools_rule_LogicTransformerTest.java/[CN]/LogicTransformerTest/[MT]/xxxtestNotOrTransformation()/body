{
  final String a="a";
  final String b="b";
  final Not not=new Not();
  final Or or=new Or();
  not.addChild(or);
  or.addChild(a);
  or.addChild(b);
  try {
    final And newAnd=(And)LogicTransformer.getInstance().applyOrTransformation(not,or);
    fail("This should fail as you cannot nest Ors under Nots");
  }
 catch (  final InvalidPatternException e) {
  }
}
