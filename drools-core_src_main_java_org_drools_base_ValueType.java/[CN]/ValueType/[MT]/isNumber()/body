{
  return (this.classType == Integer.TYPE) || (this.classType == Long.TYPE) || (this.classType == Float.TYPE)|| (this.classType == Double.TYPE)|| (this.classType == Byte.TYPE)|| (this.classType == Short.TYPE)|| (this.classType == Character.TYPE)|| (this.classType == Character.class)|| (Number.class.isAssignableFrom(this.classType));
}
