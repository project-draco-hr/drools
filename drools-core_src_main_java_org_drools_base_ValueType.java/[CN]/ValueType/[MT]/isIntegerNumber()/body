{
  return (this.classType == Integer.TYPE) || (this.classType == Long.TYPE) || (this.classType == Integer.class)|| (this.classType == Long.class)|| (this.classType == Character.class)|| (this.classType == Character.TYPE)|| (this.classType == Byte.TYPE)|| (this.classType == Short.TYPE)|| (this.classType == Byte.class)|| (this.classType == Short.class);
}
