{
  if (this.mDesc == null) {
    final StringBuffer b=new StringBuffer(36);
    for (int i=0; i < 16; ++i) {
switch (i) {
case 4:
case 6:
case 8:
case 10:
        b.append('-');
    }
    final int hex=this.mId[i] & 0xFF;
    b.append(UUID.kHexChars.charAt(hex >> 4));
    b.append(UUID.kHexChars.charAt(hex & 0x0f));
  }
  if (!UUID.sDescCaching) {
    return b.toString();
  }
  this.mDesc=b.toString();
}
return this.mDesc;
}
