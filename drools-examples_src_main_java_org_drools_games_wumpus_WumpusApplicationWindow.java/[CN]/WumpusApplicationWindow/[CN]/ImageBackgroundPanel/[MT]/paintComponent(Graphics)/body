{
  super.paintComponent(g);
  try {
    for (int i=0; i < 5; i++) {
      for (int j=0; j < 5; j++) {
        int row=i * 50;
        int col=j * 50;
        BufferedImage image=javax.imageio.ImageIO.read(getClass().getResource("pitt.png"));
        g.drawImage(image,row,col,50,50,this);
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
