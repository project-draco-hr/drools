{
  super(capacity,loadFactor);
  this.startResult=FactHandleIndexHashTable.PRIME;
  for (int i=0, length=index.length; i < length; i++) {
    this.startResult+=FactHandleIndexHashTable.PRIME * this.startResult + index[i].getExtractor().getIndex();
  }
switch (index.length) {
case 0:
    throw new IllegalArgumentException("FieldIndexHashTable cannot use an index[] of length  0");
case 1:
  this.index=new SingleIndex(index,this.startResult,this.comparator);
break;
case 2:
this.index=new DoubleCompositeIndex(index,this.startResult,this.comparator);
break;
case 3:
this.index=new TripleCompositeIndex(index,this.startResult,this.comparator);
break;
default :
throw new IllegalArgumentException("FieldIndexHashTable cannot use an index[] of length  great than 3");
}
}
