{
  final long id=handle.getId();
  FactEntryImpl previous=this.first;
  FactEntryImpl current=previous;
  while (current != null) {
    final FactEntryImpl next=(FactEntryImpl)current.next;
    if (current.handle.getId() == id) {
      if (this.first == current) {
        this.first=next;
      }
 else {
        previous.next=next;
      }
      current.next=null;
      return current;
    }
    previous=current;
    current=next;
  }
  return current;
}
