{
  this.xmlPackageReader.startConfiguration(localName,attrs);
  final String evaluator=attrs.getValue("evaluator");
  if (evaluator == null || evaluator.trim().equals("")) {
    throw new SAXParseException("<bound-variable> requires an 'evaluator' attribute",this.xmlPackageReader.getLocator());
  }
  final QualifiedIdentifierRestrictionDescr qualifiedIdentifierRestricionDescr=new QualifiedIdentifierRestrictionDescr(evaluator,null);
  return qualifiedIdentifierRestricionDescr;
}
