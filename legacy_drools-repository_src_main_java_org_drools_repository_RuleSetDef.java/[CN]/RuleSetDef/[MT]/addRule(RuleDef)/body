{
  RuleDef returnVal=rule;
  if (rule.getId() == null) {
    rule.setOwningRuleSetName(this.name);
    addAssetToSet(rule,this.rules);
  }
 else {
    RuleDef newRule=(RuleDef)rule.copy();
    newRule.setOwningRuleSetName(this.name);
    addAssetToSet(newRule,this.rules);
    returnVal=newRule;
  }
  return returnVal;
}
