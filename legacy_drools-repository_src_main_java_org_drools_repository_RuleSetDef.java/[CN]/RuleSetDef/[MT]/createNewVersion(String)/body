{
  this.workingVersionNumber++;
  addNewVersionHistory(comment);
  createAndAddNewVersions(this.rules,comment,this.workingVersionNumber);
  createAndAddNewVersions(this.attachments,comment,this.workingVersionNumber);
  createAndAddNewVersions(this.functions,comment,this.workingVersionNumber);
  createAndAddNewVersions(this.applicationData,comment,this.workingVersionNumber);
  createAndAddNewVersions(this.imports,comment,this.workingVersionNumber);
}
