{
  if (kSessionNames == null || kSessionNames.isEmpty()) {
    return ksessions.values().iterator().next();
  }
 else {
    String ksessionName=kSessionNames.iterator().next();
    if (ksessions.containsKey(ksessionName)) {
      return ksessions.get(ksessionName);
    }
 else {
      return ksessions.values().iterator().next();
    }
  }
}
