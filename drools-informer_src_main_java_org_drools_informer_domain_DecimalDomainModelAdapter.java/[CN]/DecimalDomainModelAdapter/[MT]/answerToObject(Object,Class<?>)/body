{
  if (answer == null) {
    if (clazz.isPrimitive()) {
      answer=0;
    }
 else {
      return null;
    }
  }
  if (clazz.isPrimitive()) {
    if (clazz.equals(float.class)) {
      clazz=Float.class;
    }
 else     if (clazz.equals(double.class)) {
      clazz=Double.class;
    }
  }
  try {
    return clazz.getConstructor(String.class).newInstance(answer.toString());
  }
 catch (  Exception e) {
    throw new UnsupportedOperationException(e);
  }
}
