{
  config.setOption(LanguageLevelOption.DRL5);
  assertEquals(LanguageLevelOption.DRL5,config.getOption(LanguageLevelOption.class));
  assertEquals(LanguageLevelOption.DRL5,config.getOption(LanguageLevelOption.class));
  assertEquals("DRL5",config.getProperty(LanguageLevelOption.PROPERTY_NAME));
  config.setProperty(LanguageLevelOption.PROPERTY_NAME,"DRL6");
  assertEquals(LanguageLevelOption.DRL6,config.getOption(LanguageLevelOption.class));
  assertEquals("DRL6",config.getOption(LanguageLevelOption.class).toString());
  assertEquals(LanguageLevelOption.DRL6,config.getOption(LanguageLevelOption.class));
  assertEquals("DRL6",config.getProperty(LanguageLevelOption.PROPERTY_NAME));
}
