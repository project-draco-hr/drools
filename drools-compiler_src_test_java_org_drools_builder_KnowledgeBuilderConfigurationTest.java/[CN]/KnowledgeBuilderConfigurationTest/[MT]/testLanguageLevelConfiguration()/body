{
  config.setOption(LanguageLevelOption.get(5));
  assertEquals(LanguageLevelOption.get(5),config.getOption(LanguageLevelOption.class));
  assertEquals(5,config.getOption(LanguageLevelOption.class).getLanguageLevel());
  assertEquals("5",config.getProperty(LanguageLevelOption.PROPERTY_NAME));
  config.setProperty(LanguageLevelOption.PROPERTY_NAME,"6");
  assertEquals(LanguageLevelOption.get(6),config.getOption(LanguageLevelOption.class));
  assertEquals("LanguageLevelOption( languageLevel=6 )",config.getOption(LanguageLevelOption.class).toString());
  assertEquals(6,config.getOption(LanguageLevelOption.class).getLanguageLevel());
  assertEquals("6",config.getProperty(LanguageLevelOption.PROPERTY_NAME));
}
