{
  this.node.assertTuple(new ReteTuple((DefaultFactHandle)workingMemory.getFactHandleFactory().newFactHandle("cheese")),context,workingMemory);
  this.node.assertTuple(new ReteTuple((DefaultFactHandle)workingMemory.getFactHandleFactory().newFactHandle("other cheese")),context,workingMemory);
  Assert.assertEquals("Two tuples should have been propagated",2,this.sink.getAsserted().size());
  final Tuple t1=(Tuple)((Object[])this.sink.getAsserted().get(0))[0];
  final Tuple t2=(Tuple)((Object[])this.sink.getAsserted().get(1))[0];
  List propagated=this.node.getPropagatedTuples(workingMemory,sink);
  Assert.assertEquals("Wrong Tuple propagated",t1,propagated.get(0));
  Assert.assertEquals("Wrong Tuple propagated",t2,propagated.get(1));
}
