{
  final DefaultFactHandle f0=(DefaultFactHandle)this.workingMemory.getFactHandleFactory().newFactHandle("cheese",null,null);
  final LeftTuple tuple0=new LeftTuple(f0,null,true);
  this.node.assertLeftTuple(tuple0,this.context,this.workingMemory);
  assertEquals(1,this.memory.getLeftTupleMemory().size());
  assertEquals(0,this.memory.getRightTupleMemory().size());
  Assert.assertTrue("An empty matching objects list should be propagated",this.accumulator.getMatchingObjects().isEmpty());
  final DefaultFactHandle f1=(DefaultFactHandle)this.workingMemory.getFactHandleFactory().newFactHandle("other cheese",null,null);
  final LeftTuple tuple1=new LeftTuple(f1,null,true);
  this.node.assertLeftTuple(tuple1,this.context,this.workingMemory);
  assertEquals(2,this.memory.getLeftTupleMemory().size());
  Assert.assertTrue("An empty matching objects list should be propagated",this.accumulator.getMatchingObjects().isEmpty());
  final LeftTupleMemory memory=this.memory.getLeftTupleMemory();
  assertTrue(memory.contains(tuple0));
  assertTrue(memory.contains(tuple1));
  Assert.assertEquals("Two tuples should have been propagated",2,this.sink.getAsserted().size());
}
