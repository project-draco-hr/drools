{
  loadFactor=in.readFloat();
  int capacity=in.readInt();
  int size=in.readInt();
  init();
  data=new HashEntry[capacity];
  for (int i=0; i < size; i++) {
    Object key=in.readObject();
    Object value=in.readObject();
    put(key,value);
  }
  threshold=calculateThreshold(data.length,loadFactor);
}
