{
  if (key < 0) {
    throw new IllegalArgumentException("-ve keys not supported: " + key);
  }
  if (value == null) {
    value=PrimitiveLongMap.NULL;
  }
  final Page page=findPage(key);
  final Object oldValue=page.put(key,value);
  if (oldValue == null) {
    this.totalSize++;
  }
  return oldValue;
}
