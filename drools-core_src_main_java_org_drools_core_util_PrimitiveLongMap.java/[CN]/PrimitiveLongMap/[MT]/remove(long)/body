{
  if (key > this.maxKey || key < 0) {
    return null;
  }
  final Page page=findPage(key);
  final Object oldValue=page.put(key,null);
  if (this.lastPageId != 0 && this.lastPage.isEmpty()) {
    shrinkPages(this.lastPageId);
  }
  if (oldValue != null) {
    this.totalSize--;
  }
  return oldValue;
}
