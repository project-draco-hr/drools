{
  int i=1;
  int size=2;
  while (size < tableSize) {
    size<<=1;
    ++i;
  }
  this.tableSize=size;
  this.shifts=i;
  this.previousSibling=previousSibling;
  if (this.previousSibling != null) {
    this.previousSibling.setNextSibling(this);
  }
  this.pageId=pageId;
  this.pageSize=tableSize << this.shifts;
}
