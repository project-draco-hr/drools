        gameData.getWumpusWorld().setData(gameData);
        updateCave();
        updateSensors();
      }
 else {
        updateCave();
        updateSensors();
      }
    }
  }
);
  movePanel.add(btnNewButton_1);
  JPanel panel_10=new JPanel();
  panel_10.setBackground(Color.WHITE);
  movePanel.add(panel_10);
  return movePanel;
}
