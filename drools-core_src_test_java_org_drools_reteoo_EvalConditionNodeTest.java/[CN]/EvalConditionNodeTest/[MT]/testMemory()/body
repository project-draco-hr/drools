{
  final ReteooWorkingMemory workingMemory=new ReteooWorkingMemory(1,(ReteooRuleBase)RuleBaseFactory.newRuleBase());
  final MockTupleSource source=new MockTupleSource(12);
  final EvalConditionNode node=new EvalConditionNode(18,source,new MockEvalCondition(true));
  final LinkedList memory=(LinkedList)workingMemory.getNodeMemory(node);
  assertNotNull(memory);
}
