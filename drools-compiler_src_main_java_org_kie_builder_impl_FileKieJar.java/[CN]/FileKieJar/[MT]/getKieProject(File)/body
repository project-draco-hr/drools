{
  ZipFile zipFile;
  try {
    zipFile=new ZipFile(kJar);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  ZipEntry zipEntry=zipFile.getEntry(KieProjectImpl.KPROJECT_JAR_PATH);
  if (zipEntry != null) {
    InputStream zipStream=null;
    try {
      zipStream=zipFile.getInputStream(zipEntry);
      return fromXML(zipStream);
    }
 catch (    IOException e) {
      new RuntimeException("Cannot open kproject.xml",e);
    }
 finally {
      if (zipStream != null) {
        try {
          zipStream.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
  throw new RuntimeException("Invalid kjar: doesn't have a kproject.xml file");
}
