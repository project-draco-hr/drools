{
  if (type instanceof ParameterizedType) {
    ParameterizedType pt=(ParameterizedType)type;
    Type goodType=null;
    for (    Type t : pt.getActualTypeArguments()) {
      goodType=t;
    }
    int index=goodType.toString().lastIndexOf(".");
    return goodType.toString().substring(index + 1);
  }
  return null;
}
