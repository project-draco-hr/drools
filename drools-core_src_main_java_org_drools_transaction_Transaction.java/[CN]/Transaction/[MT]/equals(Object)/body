{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Transaction other=(Transaction)obj;
  if (xid == null) {
    if (other.xid != null)     return false;
  }
 else   if (!xid.equals(other.xid))   return false;
  return true;
}
