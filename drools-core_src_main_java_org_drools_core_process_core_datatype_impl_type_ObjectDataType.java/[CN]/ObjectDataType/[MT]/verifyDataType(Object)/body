{
  if (value == null) {
    return true;
  }
  try {
    Class<?> clazz=Class.forName(className);
    if (clazz.isInstance(value)) {
      return true;
    }
  }
 catch (  ClassNotFoundException e) {
    throw new IllegalArgumentException("Could not find data type " + className);
  }
  return false;
}
