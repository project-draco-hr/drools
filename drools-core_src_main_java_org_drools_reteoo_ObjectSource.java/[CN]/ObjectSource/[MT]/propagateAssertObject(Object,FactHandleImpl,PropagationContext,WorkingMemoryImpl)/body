{
  if (!this.attachingNewNode) {
    for (int i=0, size=this.objectSinks.size(); i < size; i++) {
      ((ObjectSink)this.objectSinks.get(i)).assertObject(object,handle,context,workingMemory);
    }
  }
 else {
    ((ObjectSink)this.objectSinks.get(this.objectSinks.size() - 1)).assertObject(object,handle,context,workingMemory);
  }
}
