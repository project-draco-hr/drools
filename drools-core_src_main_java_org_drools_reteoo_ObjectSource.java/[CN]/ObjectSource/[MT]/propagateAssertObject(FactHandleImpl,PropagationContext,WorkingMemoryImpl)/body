{
  if (!this.attachingNewNode) {
    for (Iterator i=this.objectSinks.iterator(workingMemory,handle); i.hasNext(); ) {
      ((ObjectSink)i.next()).assertObject(handle,context,workingMemory);
    }
  }
 else {
    ObjectSink lastNode=this.objectSinks.getLastObjectSink();
    if (lastNode != null) {
      this.objectSinks.getLastObjectSink().assertObject(handle,context,workingMemory);
    }
 else {
      throw new RuntimeException("Possible BUG: trying to propagate an assert to a node that was the last added node");
    }
  }
}
