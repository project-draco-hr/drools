{
  String descrString="";
  for (final Iterator ite=descr.iterator(); ite.hasNext(); ) {
    Object obj=ite.next();
    visit(obj);
    descrString+=this.template;
    if (obj.getClass().equals(PatternDescr.class) || obj.getClass().equals(CollectDescr.class) || obj.getClass().equals(EvalDescr.class)|| obj.getClass().equals(AccumulateDescr.class)|| obj.getClass().equals(FromDescr.class)) {
      descrString+=DrlDumper.eol;
    }
 else     if (ite.hasNext()) {
      descrString+=" && ";
    }
  }
  return descrString;
}
