{
  String descrString="";
  for (final Iterator iterator=descr.iterator(); iterator.hasNext(); ) {
    visit(iterator.next());
    descrString+=this.template;
    if (descrString.endsWith(this.eol)) {
      descrString=descrString.substring(0,descrString.indexOf(this.eol));
    }
    descrString+=" || ";
  }
  return descrString.substring(0,descrString.length() - 4);
}
