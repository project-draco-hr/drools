{
  this.template=new String();
  final String parameterTemplate=processParameters(functionDescr.getParameterNames(),functionDescr.getParameterTypes());
  this.template="function " + functionDescr.getReturnType() + " "+ functionDescr.getName()+ "("+ parameterTemplate+ "){"+ functionDescr.getText()+ this.eol+ "}"+ this.eol;
}
