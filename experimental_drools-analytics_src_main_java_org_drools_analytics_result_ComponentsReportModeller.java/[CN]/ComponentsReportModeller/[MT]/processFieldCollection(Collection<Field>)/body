{
  if (fields != null && !fields.isEmpty()) {
    for (    Field field : fields) {
      str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&nbsp;<a href=\"#Field_id");
      str.append(field.getId());
      str.append("\">");
      str.append(field.getName());
      str.append("</a><br />\n");
    }
  }
 else {
    str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;No fields found<br />\n");
  }
}
