{
  if (classes != null && !classes.isEmpty()) {
    for (    AnalyticsClass clazz : classes) {
      str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&nbsp;<a href=\"#Pattern_id");
      str.append(clazz.getId());
      str.append("\">");
      str.append(clazz.getName());
      str.append("</a><br />\n");
    }
  }
 else {
    str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;No patterns found<br />\n");
  }
}
