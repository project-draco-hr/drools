{
  if (rules != null && !rules.isEmpty()) {
    for (    AnalyticsRule rule : rules) {
      str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&nbsp;<a href=\"#Rule_id");
      str.append(rule.getId());
      str.append("\">");
      str.append(rule.getRuleName());
      str.append("</a><br />\n");
    }
  }
 else {
    str.append("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;No rules found<br />\n");
  }
}
