{
  String rule="package org.kie\n" + "import org.kie.integrationtests.RuleMetadataTest.B\n" + "global java.util.List list;\n"+ "function A getA(A a) {\n"+ "    return a;\n"+ "}"+ "declare A\n"+ "    a : int\n"+ "    b : int\n"+ "    c : String\n"+ "end\n";
  int i=0;
  for (  String str : consequences) {
    rule+="rule R" + (i++) + "\n"+ "when\n"+ "   $a : A( $aa : a, $ab : b, $ac : c )\n"+ "   $b : B( $ba : a, $bb : b, $bc : c )\n"+ "then\n"+ str+ "\nend\n";
  }
  return loadKnowledgeBaseFromString(rule);
}
