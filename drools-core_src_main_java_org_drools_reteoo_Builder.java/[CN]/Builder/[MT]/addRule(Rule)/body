{
  And[] and=rule.getTransformedLhs();
  for (int i=0; i < and.length; i++) {
    addRule(and[i],rule);
  }
  if (!(rule instanceof Query)) {
    TerminalNode node=new TerminalNode(this.id++,this.tupleSource,rule);
  }
 else {
    QueryTerminalNode node=new QueryTerminalNode(this.id++,this.tupleSource,rule);
  }
}
