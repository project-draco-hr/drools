{
  Map.Entry<String,String> castEntry=context.getInferredCast(expr);
  if (castEntry == null) {
    return expr;
  }
  String castedExpr="((" + castEntry.getValue() + ")"+ castEntry.getKey()+ ")"+ expr.substring(castEntry.getKey().length());
  atomicExpr.setRewrittenExpression(castedExpr);
  return castedExpr;
}
