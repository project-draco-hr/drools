{
  Pattern pattern=context.getLastBuiltPatterns() != null ? context.getLastBuiltPatterns()[0] : null;
  if (pattern == null) {
    return -1L;
  }
  ObjectType objectType=pattern.getObjectType();
  if (!(objectType instanceof ClassObjectType)) {
    return -1L;
  }
  Class objectClass=((ClassWireable)objectType).getClassType();
  return isPropertyReactive(context,objectClass) ? calculatePositiveMask(pattern.getListenedProperties(),getSettableProperties(context.getRuleBase(),objectClass)) : -1L;
}
