{
  if (object == this) {
    return true;
  }
  if (object == null || !(object instanceof LeftInputAdapterNode)) {
    return false;
  }
  final LeftInputAdapterNode other=(LeftInputAdapterNode)object;
  return this.sinkMask.equals(other.sinkMask) && this.objectSource.equals(other.objectSource);
}
