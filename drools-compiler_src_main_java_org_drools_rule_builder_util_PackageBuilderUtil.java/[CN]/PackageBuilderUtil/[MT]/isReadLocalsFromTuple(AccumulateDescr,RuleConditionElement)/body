{
  final boolean readLocalsFromTuple=accumDescr.isMultiPattern() || (accumDescr.getInputPattern().getSource() != null && !(accumDescr.getInputPattern().getSource() instanceof WindowReferenceDescr) && !(accumDescr.getInputPattern().getSource() instanceof EntryPointDescr)) || source instanceof QueryElement|| (source.getNestedElements().size() == 1 && source.getNestedElements().get(0) instanceof QueryElement);
  return readLocalsFromTuple;
}
