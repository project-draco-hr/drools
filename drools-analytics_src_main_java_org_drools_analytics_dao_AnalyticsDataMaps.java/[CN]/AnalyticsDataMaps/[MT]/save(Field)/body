{
  AnalyticsClass clazz=classesById.get(Integer.valueOf(field.getClassId()));
  fieldsByClassAndFieldName.put(clazz.getName() + "." + field.getName(),field);
  fieldsById.put(field.getId(),field);
  if (fieldsByClassId.containsKey(field.getClassId())) {
    Set<Field> set=fieldsByClassId.get(field.getClassId());
    set.add(field);
  }
 else {
    Set<Field> set=new HashSet<Field>();
    set.add(field);
    fieldsByClassId.put(field.getClassId(),set);
  }
}
