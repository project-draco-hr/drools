{
  if (this.lhs == null) {
    return null;
  }
  for (int i=0; i < this.lhs.length; i++) {
    if (this.lhs[i] instanceof FactPattern) {
      final FactPattern p=(FactPattern)this.lhs[i];
      for (      FieldConstraint z : p.getFieldConstraints()) {
        return giveFieldBinding(z,var);
      }
    }
  }
  return null;
}
