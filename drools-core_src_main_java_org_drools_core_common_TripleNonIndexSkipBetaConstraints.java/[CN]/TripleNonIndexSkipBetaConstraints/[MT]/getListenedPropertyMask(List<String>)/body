{
  if (constraint0 instanceof MvelConstraint && constraint1 instanceof MvelConstraint && constraint2 instanceof MvelConstraint) {
    return ((MvelConstraint)constraint0).getListenedPropertyMask(settableProperties).setAll(((MvelConstraint)constraint1).getListenedPropertyMask(settableProperties)).setAll(((MvelConstraint)constraint2).getListenedPropertyMask(settableProperties));
  }
  return allSetButTraitBitMask();
}
