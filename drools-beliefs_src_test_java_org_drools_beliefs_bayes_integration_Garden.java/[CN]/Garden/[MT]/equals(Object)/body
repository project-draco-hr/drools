{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Garden garden=(Garden)o;
  if (cloudy != garden.cloudy) {
    return false;
  }
  if (rain != garden.rain) {
    return false;
  }
  if (sprinkler != garden.sprinkler) {
    return false;
  }
  if (wetGrass != garden.wetGrass) {
    return false;
  }
  return true;
}
