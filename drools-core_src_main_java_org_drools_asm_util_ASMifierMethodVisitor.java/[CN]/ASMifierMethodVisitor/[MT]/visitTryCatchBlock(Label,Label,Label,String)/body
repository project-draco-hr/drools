{
  this.buf.setLength(0);
  declareLabel(start);
  declareLabel(end);
  declareLabel(handler);
  this.buf.append("mv.visitTryCatchBlock(");
  appendLabel(start);
  this.buf.append(", ");
  appendLabel(end);
  this.buf.append(", ");
  appendLabel(handler);
  this.buf.append(", ");
  appendConstant(type);
  this.buf.append(");\n");
  this.text.add(this.buf.toString());
}
