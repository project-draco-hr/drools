{
  this.buf.setLength(0);
  this.buf.append("mv.visitLocalVariable(");
  appendConstant(name);
  this.buf.append(", ");
  appendConstant(desc);
  this.buf.append(", ");
  appendConstant(signature);
  this.buf.append(", ");
  appendLabel(start);
  this.buf.append(", ");
  appendLabel(end);
  this.buf.append(", ").append(index).append(");\n");
  this.text.add(this.buf.toString());
}
