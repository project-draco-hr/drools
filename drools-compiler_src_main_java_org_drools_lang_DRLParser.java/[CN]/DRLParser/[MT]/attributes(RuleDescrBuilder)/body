{
  if (helper.validateIdentifierKey(DroolsSoftKeywords.ATTRIBUTES)) {
    match(input,DRLLexer.ID,DroolsSoftKeywords.ATTRIBUTES,null,DroolsEditorType.IDENTIFIER);
    if (state.failed)     return;
    match(input,DRLLexer.COLON,null,null,DroolsEditorType.SYMBOL);
    if (state.failed)     return;
  }
  if (helper.validateAttribute(1)) {
    attribute();
    if (state.failed)     return;
    if (input.LA(1) == DRLLexer.COMMA) {
      match(input,DRLLexer.COMMA,null,null,DroolsEditorType.SYMBOL);
      if (state.failed)       return;
    }
    while (helper.validateAttribute(1)) {
      attribute();
      if (state.failed)       return;
    }
  }
}
