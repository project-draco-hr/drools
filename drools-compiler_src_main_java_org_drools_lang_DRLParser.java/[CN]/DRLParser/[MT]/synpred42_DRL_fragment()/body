{
{
    if ((input.LA(1) >= INCR && input.LA(1) <= DECR)) {
      input.consume();
      state.errorRecovery=false;
      state.failed=false;
    }
 else {
      if (state.backtracking > 0) {
        state.failed=true;
        return;
      }
      MismatchedSetException mse=new MismatchedSetException(null,input);
      throw mse;
    }
  }
}
