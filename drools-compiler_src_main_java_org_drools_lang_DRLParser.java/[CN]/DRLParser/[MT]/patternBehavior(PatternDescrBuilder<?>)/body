{
  match(input,DRLLexer.ID,DroolsSoftKeywords.OVER,null,DroolsEditorType.KEYWORD);
  if (state.failed)   return;
  behaviorDef(pattern);
  if (state.failed)   return;
  while (input.LA(1) == DRLLexer.COMMA) {
    match(input,DRLLexer.COMMA,null,null,DroolsEditorType.SYMBOL);
    if (state.failed)     return;
    behaviorDef(pattern);
    if (state.failed)     return;
  }
}
