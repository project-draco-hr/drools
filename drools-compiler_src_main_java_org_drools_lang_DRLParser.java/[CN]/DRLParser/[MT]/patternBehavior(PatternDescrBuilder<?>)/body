{
  if (input.LA(1) == DRLLexer.PIPE) {
    while (input.LA(1) == DRLLexer.PIPE) {
      match(input,DRLLexer.PIPE,null,null,DroolsEditorType.SYMBOL);
      if (state.failed)       return;
      behaviorDef(pattern);
      if (state.failed)       return;
    }
  }
 else {
    match(input,DRLLexer.ID,DroolsSoftKeywords.OVER,null,DroolsEditorType.KEYWORD);
    if (state.failed)     return;
    behaviorDef(pattern);
    if (state.failed)     return;
  }
}
