{
  try {
    String key=null;
    if (input.LA(1) == DRLLexer.ID && input.LA(2) == DRLLexer.EQUALS_ASSIGN) {
      Token id=match(input,DRLLexer.ID,null,null,DroolsEditorType.IDENTIFIER);
      if (state.failed)       return;
      key=id.getText();
      match(input,DRLLexer.EQUALS_ASSIGN,null,null,DroolsEditorType.SYMBOL);
      if (state.failed)       return;
    }
    String value=elementValue();
    if (value.startsWith("\"") && value.endsWith("\"")) {
      value=StringUtils.unescapeJava(value);
    }
    if (state.failed)     return;
    if (state.backtracking == 0) {
      annotation.keyValue(key != null ? key : "value",value);
    }
  }
 catch (  RecognitionException re) {
    reportError(re);
  }
}
