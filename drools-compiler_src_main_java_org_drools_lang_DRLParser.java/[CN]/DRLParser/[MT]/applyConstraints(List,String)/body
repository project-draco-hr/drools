{
  if (constraints == null) {
    return block;
  }
  StringBuffer list=new StringBuffer();
  for (Iterator iter=constraints.iterator(); iter.hasNext(); ) {
    String con=(String)iter.next();
    list.append("\n\t\t");
    list.append(con);
    if (iter.hasNext()) {
      list.append(",");
    }
  }
  if (block.endsWith("()")) {
    return block.substring(0,block.length() - 2) + "(" + list.toString()+ ")";
  }
 else {
    return block + "(" + list.toString()+ ")";
  }
}
