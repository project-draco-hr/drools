{
  String label=null;
  if (input.LA(1) == DRLLexer.ID && input.LA(2) == DRLLexer.COLON && !helper.validateCEKeyword(1)) {
    label=label(DroolsEditorType.IDENTIFIER_VARIABLE);
    if (state.failed)     return;
  }
  Token function=match(input,DRLLexer.ID,null,null,DroolsEditorType.KEYWORD);
  if (state.failed)   return;
  List<String> parameters=parameters();
  if (state.failed)   return;
  if (state.backtracking == 0) {
    accumulate.function(function.getText(),label,parameters.toArray(new String[parameters.size()]));
  }
}
