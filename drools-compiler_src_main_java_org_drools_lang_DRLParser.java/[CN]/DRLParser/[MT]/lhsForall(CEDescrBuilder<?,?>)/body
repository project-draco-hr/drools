{
  ForallDescrBuilder<?> forall=helper.start(ce,ForallDescrBuilder.class,null);
  try {
    match(input,DRLLexer.ID,DroolsSoftKeywords.FORALL,null,DroolsEditorType.KEYWORD);
    if (state.failed)     return null;
    match(input,DRLLexer.LEFT_PAREN,null,null,DroolsEditorType.SYMBOL);
    if (state.failed)     return null;
    do {
      lhsPatternBind(forall,false);
      if (state.failed)       return null;
      if (input.LA(1) == DRLLexer.COMMA) {
        match(input,DRLLexer.COMMA,null,null,DroolsEditorType.SYMBOL);
        if (state.failed)         return null;
      }
    }
 while (input.LA(1) != DRLLexer.EOF && input.LA(1) != DRLLexer.RIGHT_PAREN);
    match(input,DRLLexer.RIGHT_PAREN,null,null,DroolsEditorType.SYMBOL);
    if (state.failed)     return null;
  }
  finally {
    helper.end(ForallDescrBuilder.class,forall);
  }
  return forall != null ? forall.getDescr() : null;
}
