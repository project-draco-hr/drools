{
  if (rightBinding == null) {
    return evaluator.evaluate(workingMemory,leftBinding != null ? leftExtractor : new ConstantValueReader(left),leftHandle,new ObjectFieldImpl(right));
  }
  return evaluator.evaluate(workingMemory,leftBinding != null ? leftExtractor : new ConstantValueReader(left),leftHandle,rightBinding != null ? rightExtractor : new ConstantValueReader(right),rightHandle);
}
