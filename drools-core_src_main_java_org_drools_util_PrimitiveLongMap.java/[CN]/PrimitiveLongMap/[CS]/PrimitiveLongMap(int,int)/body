{
  int i=1;
  int size=2;
  while (size < indexIntervals) {
    size<<=1;
    ++i;
  }
  this.indexIntervals=size;
  this.intervalShifts=i;
  i=1;
  size=2;
  while (size < tableSize) {
    size<<=1;
    ++i;
  }
  this.tableSize=size;
  this.shifts=i;
  this.doubleShifts=this.shifts << 1;
  this.midIntervalPoint=((this.tableSize << this.shifts) << this.intervalShifts) >> 1;
  this.lastPageId=0;
  init();
}
