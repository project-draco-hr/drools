{
  if (clazz == null) {
    return false;
  }
  for (  ImportDeclaration imp : this.imports.values()) {
    if (imp.isEvent() && imp.matches(clazz)) {
      return true;
    }
  }
  if (this.isEvent(clazz.getSuperclass())) {
    return true;
  }
  for (  Class interf : clazz.getInterfaces()) {
    if (this.isEvent(interf)) {
      return true;
    }
  }
  return false;
}
