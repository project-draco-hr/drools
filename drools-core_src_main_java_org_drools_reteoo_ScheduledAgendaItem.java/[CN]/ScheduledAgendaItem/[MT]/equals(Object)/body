{
  if (object == this) {
    return true;
  }
  if ((object == null) || !(object instanceof ScheduledAgendaItem)) {
    return false;
  }
  ScheduledAgendaItem otherItem=(ScheduledAgendaItem)object;
  return (this.rule.equals(otherItem.getRule()) && this.tuple.getKey().equals(otherItem.getKey()));
}
