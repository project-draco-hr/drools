{
  AnnotationDescr annotationDescr=typeDescr.getAnnotation(name);
  if (annotationDescr != null && annotationDescr.isDuplicated()) {
    kbuilder.addBuilderResult(new TypeDeclarationError(typeDescr,"Duplicated annotation '" + name + "' for type declaration of '"+ typeDescr.getTypeName()+ "'"));
    return null;
  }
  return annotationDescr;
}
