{
  try {
    final ReturnValueContextEntry ctx=(ReturnValueContextEntry)context;
    return this.restriction.isAllowed(this.readAccessor,ctx.getHandle(),tuple,ctx.getWorkingMemory(),ctx);
  }
 catch (  final Exception e) {
    throw new RuntimeDroolsException("Exception executing ReturnValue constraint " + this.restriction + " : "+ e.getMessage(),e);
  }
}
