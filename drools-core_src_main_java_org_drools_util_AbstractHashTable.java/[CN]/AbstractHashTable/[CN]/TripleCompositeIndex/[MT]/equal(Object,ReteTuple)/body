{
  Object object12=tuple.get(this.index0.declaration).getObject();
  Object object22=tuple.get(this.index1.declaration).getObject();
  Object object32=tuple.get(this.index2.declaration).getObject();
  return this.index0.evaluator.evaluate(this.index0.extractor,object1,object12) && this.index1.evaluator.evaluate(this.index1.extractor,object1,object22) && this.index2.evaluator.evaluate(this.index2.extractor,object1,object32);
}
