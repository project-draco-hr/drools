{
  if (isLiteral()) {
    return evaluator.evaluate(((LiteralContextEntry)context).workingMemory,((LiteralContextEntry)context).getFieldExtractor(),((LiteralContextEntry)context).getObject(),field);
  }
  return evaluator.evaluateCachedRight(((VariableContextEntry)context).workingMemory,(VariableContextEntry)context,evaluator.prepareLeftObject(tuple.get(declarations[0])));
}
