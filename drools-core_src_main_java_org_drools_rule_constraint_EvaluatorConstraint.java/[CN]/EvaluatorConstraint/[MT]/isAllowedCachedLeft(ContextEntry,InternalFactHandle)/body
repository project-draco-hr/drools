{
  if (isLiteral()) {
    return evaluator.evaluate(((LiteralContextEntry)context).workingMemory,((LiteralContextEntry)context).getFieldExtractor(),handle.getObject(),field);
  }
  return evaluator.evaluateCachedLeft(((VariableContextEntry)context).workingMemory,(VariableContextEntry)context,evaluator.prepareRightObject(handle));
}
