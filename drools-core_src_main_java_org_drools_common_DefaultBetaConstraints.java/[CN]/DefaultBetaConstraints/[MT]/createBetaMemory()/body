{
  BetaMemory memory;
  if (this.indexed) {
    Constraint constraint=(Constraint)this.constraints.getFirst();
    VariableConstraint variableConstraint=(VariableConstraint)constraint;
    memory=new BetaMemory(new TupleHashTable(),new FieldIndexHashTable(variableConstraint.getFieldExtractor(),variableConstraint.getRequiredDeclarations()[0]));
  }
 else {
    memory=new BetaMemory(new TupleHashTable(),new FactHashTable());
  }
  return memory;
}
