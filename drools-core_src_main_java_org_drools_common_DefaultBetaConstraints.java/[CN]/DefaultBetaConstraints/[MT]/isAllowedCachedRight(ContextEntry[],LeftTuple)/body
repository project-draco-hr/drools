{
  LinkedListEntry entry=(LinkedListEntry)findNode(this.indexed + 1);
  int i=1;
  while (entry != null) {
    if (!((BetaNodeFieldConstraint)entry.getObject()).isAllowedCachedRight(tuple,context[this.indexed + i])) {
      return false;
    }
    entry=(LinkedListEntry)entry.getNext();
    i++;
  }
  return true;
}
