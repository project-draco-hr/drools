{
  try {
    File resource=new File(file,pResourceName);
    if (resource.exists()) {
      return IoUtils.readBytesFromInputStream(new FileInputStream(resource));
    }
 else {
      return null;
    }
  }
 catch (  Exception e) {
    throw new RuntimeException("Unable to get bytes for: " + new File(file,pResourceName));
  }
}
