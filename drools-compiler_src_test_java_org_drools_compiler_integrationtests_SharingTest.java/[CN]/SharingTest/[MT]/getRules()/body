{
  String drl="";
  drl+="package org.drools.compiler.test\n";
  drl+="import " + Person.class.getCanonicalName() + "\n";
  drl+="import " + Cheese.class.getCanonicalName() + "\n";
  drl+="rule r1\n";
  drl+="when\n";
  drl+="   Person(name == \"Mark\")\n";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r2\n";
  drl+="when\n";
  drl+="   Person(name == \"Mark\")\n";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r3\n";
  drl+="when\n";
  drl+="   Person(name == \"Mark\", age == 50)\n";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r4\n";
  drl+="when\n";
  drl+="   Person(name == \"Mark\", age == 50)\n";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r5\n";
  drl+="when\n";
  drl+="   Person(name == \"John\", age == 50)\n";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r6\n";
  drl+="when\n";
  drl+=" Person(name == \"John\",$personCheese: cheese)\n";
  drl+="Cheese(this == $personCheese )";
  drl+="then\n";
  drl+="end\n";
  drl+="rule r7\n";
  drl+="when\n";
  drl+=" Person(name == \"John\",$personCheese: cheese)\n";
  drl+="Cheese(this == $personCheese )";
  drl+="then\n";
  drl+="end\n";
  return drl;
}
