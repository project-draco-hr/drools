{
  String value=null;
  for (Iterator it=this.props.iterator(); it.hasNext(); ) {
    Properties props=(Properties)it.next();
    value=props.getProperty(key);
    if (value != null) {
      break;
    }
  }
  if (value == null) {
    for (Iterator it=this.defaultProps.iterator(); it.hasNext(); ) {
      Properties props=(Properties)it.next();
      value=props.getProperty(key);
      if (value != null) {
        break;
      }
    }
  }
  return (value != null) ? value : defaultValue;
}
