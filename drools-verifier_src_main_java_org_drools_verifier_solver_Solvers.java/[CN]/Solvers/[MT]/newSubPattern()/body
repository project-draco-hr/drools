{
  SubPattern subPattern=new SubPattern();
  subPattern.setRuleGuid(ruleSolver.getRule().getGuid());
  subPattern.setRuleName(ruleSolver.getRule().getRuleName());
  subPattern.setRuleGuid(ruleSolver.getRule().getGuid());
  subPattern.setPatternGuid(patternSolver.getPattern().getGuid());
  return subPattern;
}
