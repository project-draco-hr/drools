{
  this.ruleBase.executeQueuedActions();
  this.wm.executeQueuedActions();
  if (activation != null) {
    activation.getPropagationContext().releaseResources();
  }
  final PropagationContext propagationContext=new PropagationContextImpl(this.wm.getNextPropagationIdCounter(),PropagationContext.ASSERTION,rule,(activation == null) ? null : (LeftTuple)activation.getTuple(),handle,this.wm.agenda.getActiveActivations(),this.wm.agenda.getDormantActivations(),entryPoint);
  this.entryPointNode.assertObject(handle,propagationContext,typeConf,this.wm);
  this.wm.executeQueuedActions();
  this.wm.workingMemoryEventSupport.fireObjectInserted(propagationContext,handle,object,this.wm);
}
