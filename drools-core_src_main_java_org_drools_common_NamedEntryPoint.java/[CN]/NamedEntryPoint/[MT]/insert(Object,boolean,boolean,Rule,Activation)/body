{
  if (object == null) {
    return null;
  }
  ObjectTypeConf typeConf=this.typeConfReg.getObjectTypeConf(this.entryPoint,object);
  InternalFactHandle handle=this.handleFactory.newFactHandle(object,typeConf.isEvent(),wm);
  this.objectStore.addHandle(handle,object);
  if (dynamic) {
    addPropertyChangeListener(object);
  }
  try {
    this.lock.lock();
    insert(handle,object,rule,activation);
  }
  finally {
    this.lock.unlock();
  }
  return handle;
}
