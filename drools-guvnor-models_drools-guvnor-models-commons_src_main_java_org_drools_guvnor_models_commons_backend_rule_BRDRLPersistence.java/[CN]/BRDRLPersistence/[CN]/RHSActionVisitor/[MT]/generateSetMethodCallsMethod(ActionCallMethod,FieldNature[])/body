{
  buf.append(indentation);
  if (isDSLEnhanced) {
    buf.append(">");
  }
  buf.append(action.getVariable());
  buf.append(".");
  buf.append(action.getMethodName());
  buf.append("( ");
  boolean isFirst=true;
  for (int i=0; i < fieldValues.length; i++) {
    ActionFieldFunction valueFunction=(ActionFieldFunction)fieldValues[i];
    if (isFirst == true) {
      isFirst=false;
    }
 else {
      buf.append(", ");
    }
    constraintValueBuilder.buildRHSFieldValue(buf,valueFunction.getType(),valueFunction.getValue());
  }
  buf.append(" );\n");
}
