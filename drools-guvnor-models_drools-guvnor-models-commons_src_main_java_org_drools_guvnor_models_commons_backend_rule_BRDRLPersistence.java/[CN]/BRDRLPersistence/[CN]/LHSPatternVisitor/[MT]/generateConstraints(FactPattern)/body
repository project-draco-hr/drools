{
  int printedCount=0;
  for (int i=0; i < pattern.getFieldConstraints().length; i++) {
    StringBuilder buffer=new StringBuilder();
    generateConstraint(pattern.getConstraintList().getConstraints()[i],false,buffer);
    if (buffer.length() > 0) {
      if (printedCount > 0) {
        buf.append(", ");
      }
      buf.append(buffer);
      printedCount++;
    }
  }
}
