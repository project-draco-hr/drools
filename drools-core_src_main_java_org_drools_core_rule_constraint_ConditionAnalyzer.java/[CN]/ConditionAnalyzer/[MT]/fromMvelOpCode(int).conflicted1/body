{
switch (opCode) {
case Operator.ADD:
    return ADD;
case Operator.SUB:
  return SUB;
case Operator.MULT:
return MUL;
case Operator.DIV:
return DIV;
case Operator.MOD:
return MOD;
case Operator.BW_AND:
return BW_AND;
case Operator.BW_OR:
return BW_OR;
case Operator.BW_SHIFT_RIGHT:
return BW_SHIFT_RIGHT;
case Operator.BW_SHIFT_LEFT:
return BW_SHIFT_LEFT;
case Operator.BW_USHIFT_RIGHT:
return BW_USHIFT_RIGHT;
case Operator.BW_USHIFT_LEFT:
return BW_USHIFT_LEFT;
}
throw new RuntimeException("Unknown boolean operator");
}
