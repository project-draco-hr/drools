{
  boolean value=false;
  if (object1 instanceof Integer) {
    value=compare(((Integer)object1).intValue(),((Integer)object2).intValue());
  }
 else   if (object1 instanceof Float) {
    value=compare(((Float)object1).intValue(),((Float)object2).intValue());
  }
 else   if (object1 instanceof Double) {
    value=compare(((Double)object1).intValue(),((Double)object2).intValue());
  }
  return value;
}
