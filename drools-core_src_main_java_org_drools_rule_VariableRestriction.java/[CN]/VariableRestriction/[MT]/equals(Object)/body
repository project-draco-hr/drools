{
  if (this == object) {
    return true;
  }
  if (object == null || getClass() != object.getClass()) {
    return false;
  }
  final VariableRestriction other=(VariableRestriction)object;
  return (this.column == other.column) && this.declaration.equals(other.declaration) && this.evaluator.equals(other.evaluator)&& Arrays.equals(this.requiredDeclarations,other.requiredDeclarations);
}
