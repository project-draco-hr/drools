{
  this.object=evaluator.prepareLeftObject(handle);
  this.workingMemory=workingMemory;
  this.rightNull=this.extractor.isNullValue(workingMemory,evaluator.prepareLeftObject(handle));
  if (!rightNull) {
    this.right=this.extractor.getCharValue(workingMemory,evaluator.prepareLeftObject(handle));
  }
 else {
    this.right=0;
  }
}
