{
  this.reteTuple=tuple;
  this.workingMemory=workingMemory;
  this.leftNull=this.declaration.getExtractor().isNullValue(workingMemory,evaluator.prepareLeftObject(tuple.get(this.declaration)));
  if (!leftNull) {
    this.left=this.declaration.getExtractor().getCharValue(workingMemory,evaluator.prepareLeftObject(tuple.get(this.declaration)));
  }
 else {
    this.left=0;
  }
}
