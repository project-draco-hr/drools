{
  String tagName=tag.getTagName();
  boolean isEnding=tag.isEndTag();
  if (tagName.equals("I")) {
    if (!isEnding) {
      listener.startComment();
    }
 else {
      listener.endComment();
    }
  }
 else   if (tagName.equals("P")) {
    listener.handleText("\n");
  }
 else   if (tagName.equals("BR")) {
    listener.handleText("\n");
  }
}
