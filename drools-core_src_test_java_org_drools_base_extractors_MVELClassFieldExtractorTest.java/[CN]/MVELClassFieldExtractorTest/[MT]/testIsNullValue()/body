{
  try {
    Assert.assertFalse(this.extractor.isNullValue(this.person));
    Extractor nullExtractor=ClassFieldExtractorFactory.getClassFieldExtractor(Person.class,"addresses['business'].phone");
    Assert.assertTrue(nullExtractor.isNullValue(this.person));
  }
 catch (  final Exception e) {
    fail("Should not throw an exception");
  }
}
