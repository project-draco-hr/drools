{
  try {
    assertFalse(this.extractor.isNullValue(null,this.person[0]));
    InternalReadAccessor nullExtractor=store.getReader(Person.class,"addresses['business'].phone",getClass().getClassLoader());
    assertTrue(nullExtractor.isNullValue(null,this.person[0]));
  }
 catch (  final Exception e) {
    fail("Should not throw an exception");
  }
}
