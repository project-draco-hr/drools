{
  fieldBoolean.set(dcv,true);
  fieldDate.set(dcv,now);
  fieldNumeric.set(dcv,1L);
  dcv.setStringValue("woot");
  assertEquals(DataType.DataTypes.STRING,dcv.getDataType());
  assertNull(dcv.getBooleanValue());
  assertNull(dcv.getDateValue());
  assertNull(dcv.getNumericValue());
  assertEquals("woot",dcv.getStringValue());
}
