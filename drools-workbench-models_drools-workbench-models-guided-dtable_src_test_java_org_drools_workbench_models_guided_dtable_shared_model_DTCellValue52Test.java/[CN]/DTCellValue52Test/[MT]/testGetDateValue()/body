{
  fieldBoolean.set(dcv,true);
  dcv.setDateValue(now);
  fieldNumeric.set(dcv,1L);
  fieldString.set(dcv,"woot");
  assertEquals(DataType.DataTypes.DATE,dcv.getDataType());
  assertNull(dcv.getBooleanValue());
  assertEquals(now,dcv.getDateValue());
  assertNull(dcv.getNumericValue());
  assertNull(dcv.getStringValue());
}
