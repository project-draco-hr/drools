{
  PrimitiveLongMap map=new PrimitiveLongMap(8,4);
  map.put(0,new Integer(0));
  assertEquals(new Integer(0),map.get(0));
  assertNull(map.remove(1));
  assertEquals(new Integer(0),map.get(0));
  assertNotNull(map.remove(0));
  assertNull(map.get(0));
  map.put(1,new Integer(1));
  assertEquals(new Integer(1),map.get(1));
  assertNull(map.remove(2));
  assertEquals(new Integer(1),map.get(1));
  assertNotNull(map.remove(1));
  assertNull(map.get(1));
  map.put(127,new Integer(127));
  assertEquals(new Integer(127),map.get(127));
  assertNull(map.remove(128));
  assertEquals(new Integer(127),map.get(127));
  assertNotNull(map.remove(127));
  assertNull(map.get(127));
  map.put(128,new Integer(128));
  assertEquals(new Integer(128),map.get(128));
  assertNull(map.remove(129));
  assertEquals(new Integer(128),map.get(128));
  assertNotNull(map.remove(128));
  assertNull(map.get(128));
}
