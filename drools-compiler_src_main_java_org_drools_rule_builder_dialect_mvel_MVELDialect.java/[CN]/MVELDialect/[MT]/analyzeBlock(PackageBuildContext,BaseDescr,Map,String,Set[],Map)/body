{
  Dialect.AnalysisResult result=null;
  try {
    result=this.analyzer.analyzeExpression(context,text,availableIdentifiers,localTypes);
  }
 catch (  final Exception e) {
    context.getErrors().add(new DescrBuildError(context.getParentDescr(),descr,e,"Unable to determine the used declarations.\n" + e.getMessage()));
  }
  return result;
}
