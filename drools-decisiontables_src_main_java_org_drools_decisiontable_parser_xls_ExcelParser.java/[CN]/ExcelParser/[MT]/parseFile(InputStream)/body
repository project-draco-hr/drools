{
  try {
    Workbook workbook=WorkbookFactory.create(inStream);
    if (_useFirstSheet) {
      Sheet sheet=workbook.getSheetAt(0);
      processSheet(sheet,_listeners.get(DEFAULT_RULESHEET_NAME));
    }
 else {
      for (      String sheetName : _listeners.keySet()) {
        Sheet sheet=workbook.getSheet(sheetName);
        if (sheet == null) {
          throw new IllegalStateException("Could not find the sheetName (" + sheetName + ") in the workbook sheetNames.");
        }
        processSheet(sheet,_listeners.get(sheetName));
      }
    }
  }
 catch (  InvalidFormatException e) {
    throw new DecisionTableParseException("An error occurred opening the workbook. It is possible that the encoding of the document did not match the encoding of the reader.",e);
  }
catch (  IOException e) {
    throw new DecisionTableParseException("Failed to open Excel stream, " + "please check that the content is xls97 format.",e);
  }
}
