{
  if (this == obj) {
    return true;
  }
  if (obj == null || !(obj instanceof XpathChunk)) {
    return false;
  }
  XpathChunk other=(XpathChunk)obj;
  return field.equals(other.field) && index == other.index && iterate == other.iterate && lazy == other.lazy && array == other.array && areNullSafeEquals(declaration,other.declaration);
}
