{
  this.clazz=clazz;
  this.field=field;
  this.index=index;
  this.iterate=iterate;
  this.lazy=lazy;
  this.accessor=accessor;
  this.accessor.setAccessible(true);
  Class<?> lastReturnedClass=accessor.getReturnType();
  this.returnedClass=iterate && Iterable.class.isAssignableFrom(lastReturnedClass) ? getParametricType() : lastReturnedClass;
}
