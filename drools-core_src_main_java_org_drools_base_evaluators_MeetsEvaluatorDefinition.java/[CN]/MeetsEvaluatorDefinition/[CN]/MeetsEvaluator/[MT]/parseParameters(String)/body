{
  if (parameters == null || parameters.trim().length() == 0) {
    this.finalRange=0;
    return;
  }
  try {
    String[] ranges=parameters.split(",");
    if (ranges.length == 1) {
      this.finalRange=Long.parseLong(ranges[0]);
    }
 else {
      throw new RuntimeDroolsException("[Meets Evaluator]: Not possible to parse parameters: '" + parameters + "'");
    }
  }
 catch (  NumberFormatException e) {
    throw new RuntimeDroolsException("[Meets Evaluator]: Not possible to parse parameters: '" + parameters + "'",e);
  }
}
