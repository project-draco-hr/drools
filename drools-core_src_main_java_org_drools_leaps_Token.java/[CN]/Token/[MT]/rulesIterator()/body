{
  if (this.rules == null) {
    FactTable factTable=this.workingMemory.getFactTable(this.dominantFactHandle.getObject().getClass());
    if (this.tupleType == Token.ASSERTED) {
      this.rules=factTable.getPositiveRulesIterator();
    }
 else {
      this.rules=factTable.getNegativeRulesIterator();
    }
  }
  return this.rules;
}
