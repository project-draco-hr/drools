{
  if (this.reseededStack) {
    this.setReseededStack(false);
    PropagationContextImpl context=new PropagationContextImpl(workingMemory.nextPropagationIdCounter(),PropagationContext.ASSERTION,null,null);
    Handle startFactHandle=new FactHandleImpl(workingMemory.getIdLastFireAllAt(),null);
    for (Iterator it=this.tailIterator(startFactHandle,startFactHandle); it.hasNext(); ) {
      FactHandleImpl handle=(FactHandleImpl)it.next();
      workingMemory.pushTokenOnStack(handle,new Token(workingMemory,handle,context));
    }
  }
}
