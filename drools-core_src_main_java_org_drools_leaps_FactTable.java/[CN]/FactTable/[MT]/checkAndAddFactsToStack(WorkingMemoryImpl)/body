{
  if (this.reseededStack) {
    this.setReseededStack(false);
    PropagationContextImpl context=new PropagationContextImpl(workingMemory.nextPropagationIdCounter(),PropagationContext.ASSERTION,null,null);
    Handle factHandle=new FactHandleImpl(workingMemory.getIdLastFireAllAt(),null);
    for (Iterator it=this.tailIterator(factHandle,factHandle); it.hasNext(); ) {
      workingMemory.pushTokenOnStack(new Token(workingMemory,(FactHandleImpl)it.next(),context));
    }
  }
}
