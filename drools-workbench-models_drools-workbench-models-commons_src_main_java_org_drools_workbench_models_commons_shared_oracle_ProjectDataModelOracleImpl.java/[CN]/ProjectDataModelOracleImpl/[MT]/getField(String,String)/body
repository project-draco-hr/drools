{
  final String shortName=getFactNameFromType(modelClassName);
  final ModelField[] fields=projectModelFields.get(shortName);
  if (fields == null) {
    return null;
  }
  for (  ModelField modelField : fields) {
    if (modelField.getName().equals(fieldName)) {
      return modelField;
    }
  }
  return null;
}
