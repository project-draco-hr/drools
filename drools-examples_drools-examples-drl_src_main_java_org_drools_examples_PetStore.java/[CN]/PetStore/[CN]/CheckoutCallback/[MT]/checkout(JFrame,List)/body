{
  Order order=new Order();
  for (int i=0; i < items.size(); i++) {
    order.addItem(new Purchase(order,(Product)items.get(i)));
  }
  StatefulKnowledgeSession statefulKnowledgeSession=knowledgeBase.newStatefulKnowledgeSession();
  statefulKnowledgeSession.setGlobal("frame",frame);
  statefulKnowledgeSession.setGlobal("textArea",this.output);
  statefulKnowledgeSession.insert(new Product("Gold Fish",5));
  statefulKnowledgeSession.insert(new Product("Fish Tank",25));
  statefulKnowledgeSession.insert(new Product("Fish Food",2));
  statefulKnowledgeSession.insert(new Product("Fish Food Sample",0));
  statefulKnowledgeSession.insert(order);
  statefulKnowledgeSession.fireAllRules();
  return order.toString();
}
