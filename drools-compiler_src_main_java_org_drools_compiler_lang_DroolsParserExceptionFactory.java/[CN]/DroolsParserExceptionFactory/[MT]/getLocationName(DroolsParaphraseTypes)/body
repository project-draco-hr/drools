{
switch (type) {
case PACKAGE:
    return "package";
case IMPORT:
  return "import";
case FUNCTION_IMPORT:
return "function import";
case GLOBAL:
return "global";
case FUNCTION:
return "function";
case QUERY:
return "query";
case TEMPLATE:
return "template";
case RULE:
return "rule";
case RULE_ATTRIBUTE:
return "rule attribute";
case PATTERN:
return "pattern";
case EVAL:
return "eval";
default :
return "";
}
}
