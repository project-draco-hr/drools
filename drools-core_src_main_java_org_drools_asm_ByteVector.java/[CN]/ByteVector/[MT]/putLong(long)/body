{
  int length=this.length;
  if (length + 8 > this.data.length) {
    enlarge(8);
  }
  final byte[] data=this.data;
  int i=(int)(l >>> 32);
  data[length++]=(byte)(i >>> 24);
  data[length++]=(byte)(i >>> 16);
  data[length++]=(byte)(i >>> 8);
  data[length++]=(byte)i;
  i=(int)l;
  data[length++]=(byte)(i >>> 24);
  data[length++]=(byte)(i >>> 16);
  data[length++]=(byte)(i >>> 8);
  data[length++]=(byte)i;
  this.length=length;
  return this;
}
