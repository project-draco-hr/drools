{
  if (baseDescr instanceof AndDescr) {
    AndDescr and=(AndDescr)baseDescr;
    for (    BaseDescr descr : and.getDescrs()) {
      addDescr(descr);
      for (      String annKey : and.getAnnotationNames()) {
        addAnnotation(and.getAnnotation(annKey));
      }
    }
  }
 else {
    addDescr(baseDescr);
  }
}
