{
  if (this.store == null) {
    return new String[0];
  }
  final List names=new ArrayList();
  for (final Iterator it=this.store.keySet().iterator(); it.hasNext(); ) {
    final String name=(String)it.next();
    names.add(name);
  }
  return (String[])names.toArray(new String[this.store.size()]);
}
