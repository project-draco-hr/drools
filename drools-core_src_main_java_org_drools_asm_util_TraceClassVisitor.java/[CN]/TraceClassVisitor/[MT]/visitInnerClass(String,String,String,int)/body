{
  this.buf.setLength(0);
  this.buf.append(this.tab).append("// access flags ").append(access & ~Opcodes.ACC_SUPER).append('\n');
  this.buf.append(this.tab);
  appendAccess(access);
  this.buf.append("INNERCLASS ");
  if ((access & Opcodes.ACC_ENUM) != 0) {
    this.buf.append("enum ");
  }
  appendDescriptor(TraceAbstractVisitor.INTERNAL_NAME,name);
  this.buf.append(' ');
  appendDescriptor(TraceAbstractVisitor.INTERNAL_NAME,outerName);
  this.buf.append(' ');
  appendDescriptor(TraceAbstractVisitor.INTERNAL_NAME,innerName);
  this.buf.append('\n');
  this.text.add(this.buf.toString());
  if (this.cv != null) {
    this.cv.visitInnerClass(name,outerName,innerName,access);
  }
}
