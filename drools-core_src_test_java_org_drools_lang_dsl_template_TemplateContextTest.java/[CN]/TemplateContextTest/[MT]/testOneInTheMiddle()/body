{
  Chunk chunk1=new Chunk("yeah ");
  Chunk chunk2=new Chunk("{abc}");
  Chunk chunk3=new Chunk("one more");
  chunk1.next=chunk2;
  chunk2.next=chunk3;
  String nl="yeah exp1 one more ";
  chunk1.process(nl);
  HashMap map=new HashMap();
  chunk1.buildValueMap(map);
  assertEquals("exp1",map.get("{abc}"));
}
