{
  Or or1=new Or();
  Column column=new Column(0,null);
  or1.addChild(column);
  assertEquals(1,or1.getChildren().size());
  assertSame(column,or1.getChildren().get(0));
  And and1=new And();
  and1.addChild(or1);
  assertEquals(1,and1.getChildren().size());
  assertSame(column,and1.getChildren().get(0));
}
