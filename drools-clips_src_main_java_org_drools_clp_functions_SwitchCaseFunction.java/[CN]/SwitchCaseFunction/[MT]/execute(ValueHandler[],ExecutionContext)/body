{
  ValueHandler result=null;
  if (args[0].getBooleanValue(context)) {
    for (int i=2, length=args.length; i < length; i++) {
      result=args[i].getValue(context);
      if (result == BaseValueHandler.BREAK) {
        break;
      }
    }
  }
 else {
    result=new BooleanValueHandler(false);
  }
  return result;
}
