{
  String str="import org.drools.integrationtests.MiscTest.PrimitiveBean;\n" + "rule R when\n" + "   PrimitiveBean(0.1 + (primitive/typed) > 0.8)\n"+ "then\n"+ "end";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  ksession.insert(new PrimitiveBean(0.9,1.1));
  ksession.insert(new PrimitiveBean(0.9,null));
  assertEquals(1,ksession.fireAllRules());
  ksession.dispose();
}
