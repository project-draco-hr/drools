{
  String str="import org.kie.integrationtests.MiscTest.*\n" + "declare\n" + "   Parent @typesafe(false)\n"+ "end\n"+ "rule R1\n"+ "when\n"+ "   $a : Parent( x == 1 )\n"+ "then\n"+ "end\n";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  for (int i=0; i < 20; i++) {
    ksession.insert(new ChildA(i % 10));
    ksession.insert(new ChildB(i % 10));
  }
  assertEquals(4,ksession.fireAllRules());
  Thread.sleep(100);
  ksession.insert(new ChildA(1));
  ksession.insert(new ChildB(1));
  assertEquals(2,ksession.fireAllRules());
}
