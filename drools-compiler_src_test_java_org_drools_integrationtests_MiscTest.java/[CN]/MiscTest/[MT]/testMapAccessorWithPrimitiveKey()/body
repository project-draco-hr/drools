{
  String str="package com.sample\n" + "import org.kie.integrationtests.MiscTest.MapContainerBean\n" + "rule R1 when\n"+ "  MapContainerBean( map[1] == \"one\" )\n"+ "then\n"+ "end\n"+ "rule R2 when\n"+ "  MapContainerBean( map[1+1] == \"two\" )\n"+ "then\n"+ "end\n"+ "rule R3 when\n"+ "  MapContainerBean( map[this.get3()] == \"three\" )\n"+ "then\n"+ "end\n"+ "rule R4 when\n"+ "  MapContainerBean( map[4] == null )\n"+ "then\n"+ "end\n";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  ksession.insert(new MapContainerBean());
  assertEquals(4,ksession.fireAllRules());
  ksession.dispose();
}
