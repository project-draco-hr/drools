{
  String str="package org.kie\n" + "rule X\n" + "when\n"+ "    Primitives( bigInteger == 10I, bigInteger < (50I), bigDecimal == 10B, bigDecimal < (50B) )\n"+ "then\n"+ "end\n";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=createKnowledgeSession(kbase);
  Primitives p=new Primitives();
  p.setBigDecimal(BigDecimal.valueOf(10));
  p.setBigInteger(BigInteger.valueOf(10));
  ksession.insert(p);
  int rulesFired=ksession.fireAllRules();
  assertEquals(1,rulesFired);
}
