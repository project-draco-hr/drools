{
  ExpiringResourceCopy res=new ExpiringResourceCopy(1);
  assertTrue(res.isExpired());
  byte[] data="boo".getBytes();
  res.setData(data);
  assertFalse(res.isExpired());
  assertEquals(data,res.getData());
  Thread.sleep(2000);
  assertTrue(res.isExpired());
  res.setData(data);
  assertFalse(res.isExpired());
}
