{
  final FieldExtractor extractor=new ClassFieldExtractor(Cheese.class,"type");
  final Declaration declaration=new Declaration("typeOfCheese",extractor,5);
  assertEquals("typeOfCheese",declaration.getIdentifier());
  assertSame(String.class,declaration.getExtractor().getExtractToClass());
  assertSame(extractor,declaration.getExtractor());
  assertEquals(5,declaration.getColumn());
}
