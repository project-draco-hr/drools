{
  final String className="action";
  JavaDialect dialect=(JavaDialect)context.getDialect();
  Dialect.AnalysisResult analysis=dialect.analyzeBlock(context,actionDescr,actionDescr.getText(),new Set[]{Collections.EMPTY_SET,context.getPkg().getGlobals().keySet()});
  if (analysis == null) {
    return;
  }
  final List[] usedIdentifiers=analysis.getBoundIdentifiers();
  final Map map=createVariableContext(className,actionDescr.getText(),(ProcessBuildContext)context,(String[])usedIdentifiers[1].toArray(new String[usedIdentifiers[1].size()]));
  map.put("text",actionDescr.getText());
  generatTemplates("actionMethod","actionInvoker",(ProcessBuildContext)context,className,map,actionNode,actionDescr);
}
