{
  for (  String drl : drls) {
    final KnowledgeBuilder kBuilder;
    if (reimportAllRules) {
      kBuilder=createKnowledgeBuilder(session.getKieBase(),drl);
    }
 else {
      kBuilder=createKnowledgeBuilder(null,drl);
    }
    session.getKieBase().addKnowledgePackages(kBuilder.getKnowledgePackages());
  }
}
