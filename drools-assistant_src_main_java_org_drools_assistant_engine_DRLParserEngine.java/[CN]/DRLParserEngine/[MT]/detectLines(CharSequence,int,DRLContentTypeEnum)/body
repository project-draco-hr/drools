{
  List<RuleLineContentInfo> ruleLines=new ArrayList<RuleLineContentInfo>();
  pattern=Pattern.compile(".*");
  matcher=pattern.matcher(rule);
  while (matcher.find()) {
    if (matcher.start() != matcher.end())     ruleLines.add(new RuleLineContentInfo(matcher.start() + lineOffset,matcher.group(),type));
  }
  return ruleLines;
}
