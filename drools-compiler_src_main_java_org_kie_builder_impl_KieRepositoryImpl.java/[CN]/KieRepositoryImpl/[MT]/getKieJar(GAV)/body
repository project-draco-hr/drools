{
  KieJar kieJar=kieJars.get(gav);
  if (kieJar == null) {
    log.debug("KieJar Lookup. GAV {} was not in cache, checking classpath",gav.toExternalForm());
    kieJar=checkClasspathForKieJar(gav);
  }
  if (kieJar == null) {
    log.debug("KieJar Lookup. GAV {} was not in cache, checking maven repository",gav.toExternalForm());
    kieJar=loadKieJarFromMavenRepo(gav);
  }
  return kieJar;
}
