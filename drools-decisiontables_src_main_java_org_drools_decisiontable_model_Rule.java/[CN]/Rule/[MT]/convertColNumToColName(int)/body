{
  String result;
  int div=i / 26;
  int mod=i % 26;
  if (div == 0) {
    byte[] c=new byte[1];
    c[0]=(byte)(mod + 65);
    result=byteToString(c);
  }
 else {
    byte[] firstChar=new byte[1];
    firstChar[0]=(byte)((div - 1) + 65);
    byte[] secondChar=new byte[1];
    secondChar[0]=(byte)(mod + 65);
    String first=byteToString(firstChar);
    String second=byteToString(secondChar);
    result=first + second;
  }
  return result;
}
