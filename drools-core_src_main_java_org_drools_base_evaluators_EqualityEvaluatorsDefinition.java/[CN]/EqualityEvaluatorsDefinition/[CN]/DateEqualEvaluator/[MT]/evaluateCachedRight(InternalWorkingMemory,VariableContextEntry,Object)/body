{
  final Date value1=(Date)context.declaration.getExtractor().getValue(workingMemory,left);
  final Object value2=((ObjectVariableContextEntry)context).right;
  if (value1 == null) {
    return value2 == null;
  }
  if (value2 == null) {
    return false;
  }
  return value1.compareTo(DateUtils.getRightDate(value2)) == 0;
}
