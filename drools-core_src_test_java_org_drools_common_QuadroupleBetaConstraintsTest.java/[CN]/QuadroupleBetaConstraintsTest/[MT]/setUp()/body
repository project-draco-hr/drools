{
  conf=new RuleBaseConfiguration();
  constraints=new BetaNodeFieldConstraint[4];
  Class clazz=Person.class;
  constraints[0]=getConstraint("street",clazz);
  constraints[1]=getConstraint("city",clazz);
  constraints[2]=getConstraint("state",clazz);
  constraints[3]=getConstraint("country",clazz);
  super.setUp();
}
