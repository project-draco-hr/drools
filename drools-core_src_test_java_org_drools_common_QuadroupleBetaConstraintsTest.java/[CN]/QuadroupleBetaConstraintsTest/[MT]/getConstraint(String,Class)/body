{
  FieldExtractor extractor=ClassFieldExtractorFactory.getClassFieldExtractor(clazz,fieldName);
  Declaration declaration=new Declaration(fieldName,extractor,new Column(0,new ClassObjectType(clazz)));
  Evaluator evaluator=StringFactory.getInstance().getEvaluator(Operator.EQUAL);
  return new VariableConstraint(extractor,declaration,evaluator);
}
