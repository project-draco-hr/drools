{
  FieldExtractor extractor=ClassFieldExtractorCache.getExtractor(clazz,fieldName);
  Declaration declaration=new Declaration(fieldName,extractor,new Pattern(0,new ClassObjectType(clazz)));
  Evaluator evaluator=StringFactory.getInstance().getEvaluator(Operator.EQUAL);
  return new VariableConstraint(extractor,declaration,evaluator);
}
