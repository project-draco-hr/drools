{
  if (lines == null) {
    return;
  }
  if (lineEnding == null) {
    lineEnding=LINE_SEPARATOR;
  }
  for (Iterator it=lines.iterator(); it.hasNext(); ) {
    Object line=it.next();
    if (line != null) {
      output.write(line.toString().getBytes());
    }
    output.write(lineEnding.getBytes());
  }
}
