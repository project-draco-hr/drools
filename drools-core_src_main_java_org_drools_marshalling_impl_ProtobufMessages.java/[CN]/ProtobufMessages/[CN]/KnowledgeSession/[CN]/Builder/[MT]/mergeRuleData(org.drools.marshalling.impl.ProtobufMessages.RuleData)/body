{
  if (ruleDataBuilder_ == null) {
    if (((bitField0_ & 0x00000004) == 0x00000004) && ruleData_ != org.drools.marshalling.impl.ProtobufMessages.RuleData.getDefaultInstance()) {
      ruleData_=org.drools.marshalling.impl.ProtobufMessages.RuleData.newBuilder(ruleData_).mergeFrom(value).buildPartial();
