{
  RuleDRLContentInfo contentInfo=new RuleDRLContentInfo(offset,content,type,ruleName,lhs,rhs);
  for (  RuleLineContentInfo ruleLine : contentInfo.getLHSRuleLines())   ruleLine.setRule(contentInfo);
  for (  RuleLineContentInfo ruleLine : contentInfo.getRHSRuleLines())   ruleLine.setRule(contentInfo);
  this.contents.put(offset,contentInfo);
}
