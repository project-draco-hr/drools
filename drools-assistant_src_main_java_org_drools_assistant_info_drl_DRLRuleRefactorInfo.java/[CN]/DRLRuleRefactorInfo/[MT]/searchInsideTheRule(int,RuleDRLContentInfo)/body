{
  if (offset <= info.getRuleNameLength() + info.getOffset())   return null;
  for (  RuleLineContentInfo ruleLine : info.getLHSRuleLines()) {
    if (offset <= ruleLine.getOffset() + ruleLine.getContentLength())     return ruleLine;
  }
  for (  RuleLineContentInfo ruleLine : info.getRHSRuleLines()) {
    if (offset <= ruleLine.getOffset() + ruleLine.getContentLength())     return ruleLine;
  }
  return null;
}
