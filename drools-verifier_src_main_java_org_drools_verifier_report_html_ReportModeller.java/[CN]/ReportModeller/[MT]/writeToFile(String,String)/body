{
  zout.putNextEntry(new JarEntry(fileName));
  ByteArrayInputStream i=new ByteArrayInputStream(text.getBytes());
  int len=0;
  byte[] copyBuf=new byte[1024];
  while (len != -1) {
    len=i.read(copyBuf,0,copyBuf.length);
    if (len > 0) {
      zout.write(copyBuf,0,len);
    }
  }
  i.close();
  zout.closeEntry();
}
