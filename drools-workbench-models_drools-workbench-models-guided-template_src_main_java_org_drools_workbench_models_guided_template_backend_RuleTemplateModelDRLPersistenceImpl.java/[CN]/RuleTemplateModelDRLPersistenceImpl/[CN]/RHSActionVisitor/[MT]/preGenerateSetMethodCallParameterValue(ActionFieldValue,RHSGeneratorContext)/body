{
  if (fieldValue.getNature() == FieldNatureType.TYPE_TEMPLATE) {
    buf.append("@if{" + fieldValue.getValue() + " != empty}");
    buf.append("@code{hasOutput" + gctx.getOffset() + " = true}");
    super.preGenerateSetMethodCallParameterValue(fieldValue,gctx);
    buf.append("@end{}");
  }
 else {
    buf.append("@code{hasOutput" + gctx.getOffset() + " = true}");
    super.preGenerateSetMethodCallParameterValue(fieldValue,gctx);
  }
}
