{
  if (gctx.getParent() == null) {
    return;
  }
  buf.append("@if{hasOutput" + gctx.getOffset() + " && (");
  RHSGeneratorContext parent=gctx.getParent();
  while (parent != null) {
    buf.append("hasOutput" + parent.getOffset() + (parent.getParent() != null ? " || " : ""));
    parent=parent.getParent();
  }
  buf.append(")}");
  buf.append(", \n");
  buf.append("@end{}");
}
