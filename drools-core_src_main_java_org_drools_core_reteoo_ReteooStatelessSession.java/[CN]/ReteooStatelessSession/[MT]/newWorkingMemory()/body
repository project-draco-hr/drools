{
  this.ruleBase.readLock();
  try {
    InternalWorkingMemory wm=wmFactory.createWorkingMemory(this.ruleBase.nextWorkingMemoryCounter(),this.ruleBase,ruleBase.newFactHandleFactory(),null,1,this.sessionConf,EnvironmentFactory.newEnvironment(),this.workingMemoryEventSupport,this.agendaEventSupport,this.ruleEventListenerSupport,null);
    wm.setGlobalResolver(this.globalResolver);
    return wm;
  }
  finally {
    this.ruleBase.readUnlock();
  }
}
