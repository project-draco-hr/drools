{
  if (lock.writeLock().tryLock()) {
    try {
      internalRemoveRule(packageName,ruleName);
    }
  finally {
      unlock();
    }
  }
 else {
    kbaseModificationsQueue.offer(new Runnable(){
      @Override public void run(){
        internalRemoveRule(packageName,ruleName);
      }
    }
);
  }
}
