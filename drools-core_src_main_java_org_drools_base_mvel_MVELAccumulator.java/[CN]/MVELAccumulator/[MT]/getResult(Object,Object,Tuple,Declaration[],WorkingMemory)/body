{
  DroolsMVELFactory factory=(DroolsMVELFactory)workingMemoryContext;
  factory.setContext(leftTuple,null,null,workingMemory,(Map)context);
  Package pkg=workingMemory.getRuleBase().getPackage("MAIN");
  if (pkg != null) {
    MVELDialectData data=(MVELDialectData)pkg.getDialectDatas().getDialectData("mvel");
    factory.setNextFactory(data.getFunctionFactory());
  }
  final Object result=MVEL.executeExpression(this.result,null,factory);
  return result;
}
