{
  DroolsMVELFactory factory=(DroolsMVELFactory)workingMemoryContext;
  factory.setContext(leftTuple,null,handle.getObject(),workingMemory,((MVELAccumulatorContext)context).variables);
  if (reverse != null) {
    takeSnapshot(context,factory,handle);
  }
  MVEL.executeExpression(this.action,null,factory);
}
