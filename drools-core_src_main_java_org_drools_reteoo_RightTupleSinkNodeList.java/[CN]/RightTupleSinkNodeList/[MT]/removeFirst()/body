{
  if (this.firstNode == null) {
    return null;
  }
  final RightTupleSinkNode node=this.firstNode;
  this.firstNode=node.getNextRightTupleSinkNode();
  node.setNextRightTupleSinkNode(null);
  if (this.firstNode != null) {
    this.firstNode.setPreviousRightTupleSinkNode(null);
  }
 else {
    this.lastNode=null;
  }
  this.size--;
  return node;
}
