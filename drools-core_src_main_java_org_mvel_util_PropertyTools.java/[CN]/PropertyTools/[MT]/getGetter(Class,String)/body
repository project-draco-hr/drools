{
  String get=ReflectionUtil.getGetter(property);
  String isGet=ReflectionUtil.getIsGetter(property);
  for (  Method meth : clazz.getDeclaredMethods()) {
    if ((meth.getModifiers() & PUBLIC) == 0 || meth.getParameterTypes().length != 0) {
    }
 else     if (get.equals(meth.getName()) || isGet.equals(meth.getName())) {
      return meth;
    }
  }
  return null;
}
