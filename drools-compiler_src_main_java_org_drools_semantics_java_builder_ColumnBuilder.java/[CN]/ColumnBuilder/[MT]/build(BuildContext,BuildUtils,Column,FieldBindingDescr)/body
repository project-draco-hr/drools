{
  Declaration declaration=(Declaration)context.getDeclarationResolver().getDeclaration(fieldBindingDescr.getIdentifier());
  if (declaration != null) {
    context.getErrors().add(new RuleError(context.getRule(),fieldBindingDescr,null,"Duplicate declaration for variable '" + fieldBindingDescr.getIdentifier() + "' in the rule '"+ context.getRule().getName()+ "'"));
    return;
  }
  final FieldExtractor extractor=getFieldExtractor(context,utils,fieldBindingDescr,column.getObjectType(),fieldBindingDescr.getFieldName());
  if (extractor == null) {
    return;
  }
  declaration=column.addDeclaration(fieldBindingDescr.getIdentifier(),extractor);
}
