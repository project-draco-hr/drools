{
  reader.mark(1024);
  String line=null;
  while (line == null) {
    line=reader.readLine();
    if (line == null) {
      reader.reset();
      return null;
    }
    String trimLine=line.trim();
    if (trimLine.length() == 0 || trimLine.startsWith("#") || trimLine.startsWith("//")) {
      line=null;
    }
  }
  reader.reset();
  return line;
}
