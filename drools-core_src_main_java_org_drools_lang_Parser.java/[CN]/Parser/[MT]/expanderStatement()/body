{
  String line=laDiscard();
  if (line == null) {
    return false;
  }
  Matcher matcher=EXPANDER_STATEMENT.matcher(line);
  if (matcher.matches()) {
    consumeDiscard();
    String expanderName=matcher.group(1);
    expander=ExpanderContext.getInstance().getExpander(expanderName);
    return true;
  }
  return false;
}
