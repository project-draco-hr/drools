{
  String line=laDiscard();
  if (line == null) {
    return false;
  }
  line=line.trim();
  if (line.equals("then") || line.equals("end")) {
    return false;
  }
  Matcher matcher=FACT_BINDING.matcher(line.trim());
  String name=null;
  String pattern=null;
  consumeDiscard();
  if (matcher.matches()) {
    name=matcher.group(1);
    pattern=matcher.group(2);
  }
 else {
    pattern=line;
  }
  pattern=maybeExpand(pattern);
  pattern(pattern);
  return true;
}
