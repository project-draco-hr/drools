{
  if (this == obj)   return true;
  if (obj instanceof Fact) {
    Fact temp=(Fact)obj;
    if (this.getType() != null) {
      if (temp.getType() == null)       return false;
 else       if (!(this.getType().equals(temp.getType())))       return false;
    }
 else     if (temp.getType() != null)     return false;
    return true;
  }
  return false;
}
