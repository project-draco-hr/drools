{
  addMetaInfBuilder();
  if (pomXml != null) {
    ReleaseIdImpl g=(ReleaseIdImpl)releaseId;
    trgMfs.write(g.getPomXmlPath(),pomXml,true);
    trgMfs.write(g.getPomPropertiesPath(),generatePomProperties(releaseId).getBytes(),true);
  }
  if (kModuleModelXml != null) {
    trgMfs.write(KieModuleModelImpl.KMODULE_JAR_PATH,kModuleModel.toXML().getBytes(),true);
  }
}
