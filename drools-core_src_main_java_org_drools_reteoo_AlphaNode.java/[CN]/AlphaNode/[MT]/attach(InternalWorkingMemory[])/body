{
  attach();
  for (int i=0, length=workingMemories.length; i < length; i++) {
    final InternalWorkingMemory workingMemory=workingMemories[i];
    final PropagationContext propagationContext=new PropagationContextImpl(workingMemory.getNextPropagationIdCounter(),PropagationContext.RULE_ADDITION,null,null);
    this.objectSource.updateNewNode(workingMemory,propagationContext);
  }
}
