{
  Set memory=(Set)workingMemory.getNodeMemory(this);
  if (this.constraint.isAllowed(handle,null,workingMemory)) {
    if (memory.add(handle)) {
      propagateAssertObject(handle,context,workingMemory);
    }
 else {
      propagateModifyObject(handle,context,workingMemory);
    }
  }
 else {
    if (memory.remove(handle)) {
      propagateRetractObject(handle,context,workingMemory);
    }
  }
}
