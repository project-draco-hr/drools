{
  if (this.storeMemory) {
    Set memory=(Set)workingMemory.getNodeMemory(this);
    if (!memory.contains(handle)) {
      if (constraint.isAllowed(object,handle,null)) {
        memory.add(handle);
        propagateAssertObject(object,handle,context,workingMemory);
      }
    }
  }
 else {
    if (constraint.isAllowed(object,handle,null)) {
      propagateAssertObject(object,handle,context,workingMemory);
    }
  }
}
