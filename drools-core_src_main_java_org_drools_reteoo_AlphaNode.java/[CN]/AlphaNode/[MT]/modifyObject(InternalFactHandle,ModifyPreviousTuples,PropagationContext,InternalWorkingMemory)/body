{
  long propagationMask=context.getPropagationMask();
  if (context.getModificationMask() == Long.MAX_VALUE || intersect(context.getModificationMask(),propagationMask | networkMask)) {
    context.setPropagationMask(propagationMask | declaredMask);
    final AlphaMemory memory=(AlphaMemory)workingMemory.getNodeMemory(this);
    if (this.constraint.isAllowed(factHandle,workingMemory,memory.context)) {
      this.sink.propagateModifyObject(factHandle,modifyPreviousTuples,context,workingMemory);
    }
    context.setPropagationMask(propagationMask);
  }
 else {
    byPassModifyToBetaNode(modifyPreviousTuples);
  }
}
