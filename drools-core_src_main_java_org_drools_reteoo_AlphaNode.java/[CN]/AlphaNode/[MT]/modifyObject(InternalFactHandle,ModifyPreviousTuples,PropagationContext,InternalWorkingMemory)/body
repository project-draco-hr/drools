{
  if (context.getModificationMask() == Long.MAX_VALUE || intersect(context.getModificationMask(),getListenedPropertyMask(workingMemory))) {
    final AlphaMemory memory=(AlphaMemory)workingMemory.getNodeMemory(this);
    if (this.constraint.isAllowed(factHandle,workingMemory,memory.context)) {
      this.sink.propagateModifyObject(factHandle,modifyPreviousTuples,context,workingMemory);
    }
  }
}
