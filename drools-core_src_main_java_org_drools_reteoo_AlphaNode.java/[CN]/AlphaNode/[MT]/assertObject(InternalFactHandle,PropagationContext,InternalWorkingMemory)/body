{
  final AlphaMemory memory=(AlphaMemory)workingMemory.getNodeMemory(this);
  if (this.constraint.isAllowed(handle,workingMemory,memory.context)) {
    if (isObjectMemoryEnabled()) {
      memory.facts.add(handle,false);
    }
    this.sink.propagateAssertObject(handle,context,workingMemory);
  }
}
