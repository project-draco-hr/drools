{
  boolean propagate=true;
  final AlphaMemory memory=(AlphaMemory)workingMemory.getNodeMemory(this);
  if (isObjectMemoryEnabled()) {
    propagate=memory.facts.remove(handle);
  }
 else {
    propagate=this.constraint.isAllowed(handle,workingMemory,memory.context);
  }
  if (propagate) {
    this.sink.propagateRetractObject(handle,context,workingMemory,true);
  }
}
