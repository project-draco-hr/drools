{
  if (hasMemory()) {
    Set memory=(Set)workingMemory.getNodeMemory(this);
    if (this.constraint.isAllowed(handle,null,workingMemory)) {
      memory.add(handle);
      propagateAssertObject(handle,context,workingMemory);
    }
  }
 else {
    if (this.constraint.isAllowed(handle,null,workingMemory)) {
      propagateAssertObject(handle,context,workingMemory);
    }
  }
}
