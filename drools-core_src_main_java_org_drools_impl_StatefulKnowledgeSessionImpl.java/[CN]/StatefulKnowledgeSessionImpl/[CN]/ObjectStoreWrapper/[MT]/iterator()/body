{
  Iterator it=null;
  if (type == OBJECT) {
    if (filter != null) {
      it=store.iterateObjects(filter);
    }
 else {
      it=store.iterateObjects();
    }
  }
 else {
    if (filter != null) {
      it=store.iterateFactHandles(filter);
    }
 else {
      it=store.iterateFactHandles();
    }
  }
  return it;
}
