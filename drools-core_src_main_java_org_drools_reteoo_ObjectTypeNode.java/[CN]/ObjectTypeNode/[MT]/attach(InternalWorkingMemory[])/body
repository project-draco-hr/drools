{
  attach();
  for (int i=0, length=workingMemories.length; i < length; i++) {
    final InternalWorkingMemory workingMemory=workingMemories[i];
    final PropagationContextImpl propagationContext=new PropagationContextImpl(workingMemory.getNextPropagationIdCounter(),PropagationContext.RULE_ADDITION,null,null);
    propagationContext.setEntryPoint(this.entryPoint);
    this.rete.updateSink(this,propagationContext,workingMemory);
  }
}
