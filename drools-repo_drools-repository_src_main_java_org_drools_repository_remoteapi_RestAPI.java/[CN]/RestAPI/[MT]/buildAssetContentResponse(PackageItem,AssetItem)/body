{
  if (asset.isBinary()) {
    Binary r=new Response.Binary();
    r.lastModified=asset.getLastModified();
    r.stream=asset.getBinaryContentAttachment();
    return r;
  }
 else {
    Text r=new Response.Text();
    r.lastModified=pkg.getLastModified();
    r.data=asset.getContent();
    return r;
  }
}
