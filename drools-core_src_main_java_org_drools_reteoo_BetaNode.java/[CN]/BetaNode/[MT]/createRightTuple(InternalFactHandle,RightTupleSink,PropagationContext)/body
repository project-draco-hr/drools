{
  if (!this.concurrentRightTupleMemory) {
    if (context.getActiveWindowTupleList() == null) {
      return new RightTuple(handle,sink);
    }
 else {
      return new WindowTuple(handle,sink,context.getActiveWindowTupleList());
    }
  }
 else {
    return new ConcurrentRightTuple(handle,sink);
  }
}
