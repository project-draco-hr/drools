{
  RightTuple rightTuple=removeRightTuple(modifyPreviousTuples);
  if (context.getModificationMask() == Long.MAX_VALUE || intersect(context.getModificationMask(),context.getPropagationMask() | declaredMask)) {
    if (rightTuple != null) {
      modifyRightTuple(rightTuple,context,workingMemory);
    }
 else {
      assertObject(factHandle,context,workingMemory);
    }
  }
}
