{
  super.readExternal(in);
  System.out.println(" ridden a " + this.getClass().getName() + " >> "+ object+ " ! "+ store);
  int N=in.readInt();
  for (int j=0; j < N; j++) {
    Triple t=(Triple)in.readObject();
    ((TripleImpl)t).setInstance(getObject());
    System.out.println("Inned " + t);
    store.put(t,false);
  }
}
