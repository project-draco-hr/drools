{
  final StringBuffer buf=new StringBuffer();
  buf.append((this.variableName != null) ? this.variableName : "");
  for (final Iterator it=this.invokers.iterator(); it.hasNext(); ) {
    if (buf.length() > 0) {
      buf.append(".");
    }
    buf.append(it.next().toString());
  }
  return buf.toString();
}
