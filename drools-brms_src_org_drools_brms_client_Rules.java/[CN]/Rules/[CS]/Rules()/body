{
  TabPanel tab=new TabPanel();
  tab.setWidth("100%");
  tab.setHeight("100%");
  setWidget(tab);
  HorizontalPanel explorePanel=doExplore(tab);
  RuleView ruleViewer=new RuleView();
  ruleViewer.setWidth("100%");
  ruleViewer.setHeight("100%");
  tab.add(explorePanel,"Explore");
  tab.add(ruleViewer,"Author");
  tab.selectTab(0);
}
