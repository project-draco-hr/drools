{
  final RuleModel m=new RuleModel();
  m.name="Complex Rule";
  final FactPattern pat=new FactPattern();
  pat.boundName="p1";
  pat.factType="Person";
  final Constraint con=new Constraint();
  con.fieldBinding="f1";
  con.fieldName="age";
  pat.addConstraint(con);
  m.addLhsItem(pat);
  return m;
}
