{
  if (kBaseModel == null) {
    return;
  }
  Set<String> incs=kBaseModel.getIncludes();
  if (incs != null && !incs.isEmpty()) {
    for (    String inc : incs) {
      if (!includes.contains(inc)) {
        includes.add(inc);
        getTransitiveIncludes(kieProject,(KieBaseModelImpl)kieProject.getKieBaseModel(inc),includes);
      }
    }
  }
}
