{
  ResourceConfiguration conf=getResourceConfiguration(kieModule,fileName);
  Resource resource=kieModule.getResource(fileName);
  if (resource != null) {
    if (conf == null) {
      ckbuilder.add(resource,ResourceType.determineResourceType(fileName));
    }
 else {
      ckbuilder.add(resource,ResourceType.determineResourceType(fileName),conf);
    }
    return true;
  }
  return false;
}
