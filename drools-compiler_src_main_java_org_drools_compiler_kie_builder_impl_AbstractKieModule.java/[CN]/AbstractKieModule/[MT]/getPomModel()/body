{
  if (pomModel == null) {
    try {
      byte[] pomXml=getPomXml();
      if (pomXml != null) {
        PomModel tempPomModel=MinimalPomParser.parse("pom.xml",new ByteArrayInputStream(pomXml));
        validatePomModel(tempPomModel);
        pomModel=tempPomModel;
      }
    }
 catch (    Exception e) {
    }
  }
  return pomModel;
}
