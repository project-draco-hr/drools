{
  assertion.actualResult=firingCounts.containsKey(assertion.ruleName) ? firingCounts.get(assertion.ruleName) : 0;
  if (assertion.expectedFire != null) {
    assertion.successResult=assertion.expectedFire ? assertion.actualResult > 0 : assertion.actualResult == 0;
  }
  if (assertion.expectedCount != null) {
    assertion.successResult=assertion.actualResult.equals(assertion.expectedCount);
  }
}
