{
  List<KProject> kProjects=new ArrayList<KProject>();
  final Enumeration<URL> e;
  try {
    e=classLoader.getResources(KPROJECT_JAR_PATH);
  }
 catch (  IOException exc) {
    return kProjects;
  }
  while (e.hasMoreElements()) {
    kProjects.add(fromXML(e.nextElement()));
  }
  return kProjects;
}
