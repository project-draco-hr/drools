{
  try {
    this.lock.lock();
    Object object=this.defaultGlobalResolver.resolveGlobal(identifier);
    if (object == null && this.delegateGlobalResolver != null) {
      object=this.delegateGlobalResolver.resolveGlobal(identifier);
    }
    return object;
  }
  finally {
    this.lock.unlock();
  }
}
