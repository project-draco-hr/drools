{
  try {
    this.lock.lock();
    InternalFactHandle internalHandle=(InternalFactHandle)handle;
    Object object=internalHandle.getObject();
    if (internalHandle.isShadowFact()) {
      object=((ShadowProxy)object).getShadowedObject();
    }
    return object;
  }
  finally {
    this.lock.unlock();
  }
}
