{
  this.ruleBase.executeQueuedActions();
  if (activation != null) {
    activation.getPropagationContext().releaseResources();
  }
  final PropagationContext propagationContext=new PropagationContextImpl(getNextPropagationIdCounter(),PropagationContext.ASSERTION,rule,activation,this.agenda.getActiveActivations(),this.agenda.getDormantActivations(),entryPoint);
  this.entryPointNode.assertObject(handle,propagationContext,typeConf,this);
  executeQueuedActions();
  this.workingMemoryEventSupport.fireObjectInserted(propagationContext,handle,object,this);
}
