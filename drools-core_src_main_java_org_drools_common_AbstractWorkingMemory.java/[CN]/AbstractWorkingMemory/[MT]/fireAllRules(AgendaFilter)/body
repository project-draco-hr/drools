{
  if (!this.factQueue.isEmpty()) {
    propagateQueuedActions();
  }
  boolean noneFired=true;
  if (!this.firing) {
    try {
      this.firing=true;
      while (this.agenda.fireNextItem(agendaFilter)) {
        noneFired=false;
      }
    }
  finally {
      this.firing=false;
      if (noneFired) {
        doOtherwise(agendaFilter);
      }
    }
  }
}
