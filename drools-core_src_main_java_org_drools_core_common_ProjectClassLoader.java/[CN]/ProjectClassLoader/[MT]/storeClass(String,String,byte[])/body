{
  int lastDot=name.lastIndexOf('.');
  if (lastDot > 0) {
    String pkgName=name.substring(0,lastDot);
    if (getPackage(pkgName) == null) {
      definePackage(pkgName,"","","","","","",null);
    }
  }
  if (store == null) {
    store=new HashMap<String,byte[]>();
  }
  store.put(resourceName,bytecode);
}
