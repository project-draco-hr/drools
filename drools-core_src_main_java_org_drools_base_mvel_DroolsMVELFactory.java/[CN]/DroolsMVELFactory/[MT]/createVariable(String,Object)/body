{
  VariableResolver vr=getVariableResolver(name);
  if (vr != null) {
    vr.setValue(value);
    return vr;
  }
 else {
    if (this.variables == null) {
      this.variables=new HashMap();
    }
    addResolver(name,vr=new MapVariableResolver(this.variables,name));
    vr.setValue(value);
    return vr;
  }
}
