{
  if (tuple != null) {
    this.tupleObjects=((LeftTuple)tuple).toFactHandles();
  }
  this.object=object;
  this.kruntime=kruntime;
  if (variables == null) {
    if (this.localVariables == null) {
      this.localVariables=new HashMap();
    }
 else {
      this.localVariables.clear();
    }
  }
 else {
    this.localVariables=variables;
  }
}
