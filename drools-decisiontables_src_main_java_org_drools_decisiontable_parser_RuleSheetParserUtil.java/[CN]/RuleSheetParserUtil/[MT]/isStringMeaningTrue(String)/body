{
  if (property == null) {
    return false;
  }
 else {
    property=property.trim();
    if (property.equalsIgnoreCase("true")) {
      return true;
    }
 else     if (property.startsWith("Y")) {
      return true;
    }
 else     if (property.startsWith("y")) {
      return true;
    }
 else     if (property.equalsIgnoreCase("on")) {
      return true;
    }
 else {
      return false;
    }
  }
}
