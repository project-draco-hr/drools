{
  final String className="predicate" + context.getNextId();
  predicateDescr.setClassMethodName(className);
  final Map map=createVariableContext(className,(String)predicateDescr.getContent(),context,previousDeclarations,localDeclarations,(String[])usedIdentifiers[1].toArray(new String[usedIdentifiers[1].size()]));
  generatTemplates("predicateMethod","predicateInvoker",context,className,map,predicateConstraint,predicateDescr);
}
