{
  if (isHole()) {
    if (next == null || next.text == null) {
      value=expression.trim();
    }
 else {
      value=StringUtils.substringBefore(expression,next.text).trim();
    }
  }
 else {
    value=text;
  }
  if (next != null) {
    next.process(StringUtils.substringAfter(expression,value));
  }
}
