      matchAny();
      if (failed)       return;
    }
  break;
default :
break loop9;
}
}
 while (true);
match('\'');
if (failed) return;
}
}
break;
}
if (token == null) {
emit(type,line,charPosition,channel,start,getCharIndex() - 1);
}
}
  finally {
if (backtracking > 0) {
memoize(input,53,STRING_StartIndex);
