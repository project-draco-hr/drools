                if (failed)                 return;
              }
            break;
default :
          break loop7;
      }
    }
 while (true);
    match('"');
    if (failed)     return;
  }
}
break;
case 2:
{
{
match('\'');
if (failed) return;
loop8: do {
  int alt8=2;
  int LA8_0=input.LA(1);
  if (LA8_0 == '\'') {
    alt8=2;
  }
 else   if ((LA8_0 >= '\u0000' && LA8_0 <= '&') || (LA8_0 >= '(' && LA8_0 <= '\uFFFE')) {
