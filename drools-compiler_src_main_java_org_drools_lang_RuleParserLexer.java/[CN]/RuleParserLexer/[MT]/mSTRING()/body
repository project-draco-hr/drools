switch (alt8) {
case 1:
{
      matchAny();
      if (failed)       return;
    }
  break;
default :
break loop8;
}
}
 while (true);
match('\'');
if (failed) return;
}
}
break;
}
if (token == null) {
emit(type,line,charPosition,channel,start,getCharIndex() - 1);
}
}
  finally {
if (backtracking > 0) {
