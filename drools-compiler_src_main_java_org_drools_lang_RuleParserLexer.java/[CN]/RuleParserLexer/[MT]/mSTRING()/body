                if (failed)                 return;
              }
            break;
default :
          break loop8;
      }
    }
 while (true);
    match('"');
    if (failed)     return;
  }
}
break;
case 2:
{
{
match('\'');
if (failed) return;
loop9: do {
  int alt9=2;
  int LA9_0=input.LA(1);
  if (LA9_0 == '\'') {
    alt9=2;
  }
 else   if ((LA9_0 >= '\u0000' && LA9_0 <= '&') || (LA9_0 >= '(' && LA9_0 <= '\uFFFE')) {
