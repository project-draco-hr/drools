      match("package");
      if (failed)       return;
    }
    if (token == null) {
      emit(type,line,charPosition,channel,start,getCharIndex() - 1);
    }
  }
  finally {
    if (backtracking > 0) {
      memoize(input,2,T16_StartIndex);
    }
  }
}
