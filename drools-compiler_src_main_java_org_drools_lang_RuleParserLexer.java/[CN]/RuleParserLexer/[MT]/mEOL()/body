{
  int type=EOL;
  int start=getCharIndex();
  int line=getLine();
  int charPosition=getCharPositionInLine();
  int channel=Token.DEFAULT_CHANNEL;
{
    int alt1=3;
    int LA1_0=input.LA(1);
    if (LA1_0 == '\r') {
      int LA1_1=input.LA(2);
      if (LA1_1 == 'n') {
        alt1=1;
      }
 else {
        alt1=2;
      }
    }
 else     if (LA1_0 == '\n') {
      alt1=3;
    }
 else {
      NoViableAltException nvae=new NoViableAltException("42:7: ( \'\\r\\n\' | \'\\r\' | \'\\n\' )",1,0,input);
      throw nvae;
    }
switch (alt1) {
case 1:
{
        match("\r\n");
      }
    break;
case 2:
{
    match('\r');
  }
break;
case 3:
{
match('\n');
}
break;
}
}
if (token == null) {
emit(type,line,charPosition,channel,start,getCharIndex() - 1);
}
}
