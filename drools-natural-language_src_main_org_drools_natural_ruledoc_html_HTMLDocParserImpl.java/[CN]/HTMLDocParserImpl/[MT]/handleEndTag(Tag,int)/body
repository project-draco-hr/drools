{
  System.out.println("Breaksflow: " + tag.breaksFlow());
  if (tag == Tag.I) {
    listener.endComment();
  }
 else   if (tag == Tag.TABLE) {
    listener.endTable();
  }
 else   if (tag == Tag.TH) {
    listener.endRow();
  }
 else   if (tag == Tag.TR) {
    listener.endRow();
  }
 else   if (tag == Tag.TD) {
    listener.endColumn();
  }
 else   if (tag == Tag.BR) {
    System.out.println("BREAK");
  }
 else   if (tag == Tag.P) {
    System.out.println("PARA");
  }
}
