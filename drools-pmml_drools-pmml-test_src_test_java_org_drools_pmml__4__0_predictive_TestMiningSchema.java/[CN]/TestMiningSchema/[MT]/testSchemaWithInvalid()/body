{
  setKSession(getModelSession(source,VERBOSE));
  setKbase(getKSession().getKnowledgeBase());
  getKSession().getWorkingMemoryEntryPoint("in_PetalLength").insert(-37.0);
  getKSession().fireAllRules();
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"PetalLength"),false,false,null,-37.0);
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"PetalLength"),true,false,"IRIS_MLP",3.95);
  refreshKSession();
  getKSession().getWorkingMemoryEntryPoint("in_PetalNumber").insert(-1);
  getKSession().fireAllRules();
  System.err.println(reportWMObjects(getKSession()));
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"PetalNumber"),false,false,null,-1);
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"PetalNumber"),true,false,"IRIS_MLP",5);
}
