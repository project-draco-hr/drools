{
  if (node == smem.getTipNode()) {
    if (smem.isEmpty()) {
synchronized (smem) {
        if (smem.isEmpty()) {
          SegmentUtilities.createChildSegments(wm,smem,((LeftTupleSource)node).getSinkPropagator());
        }
        return smem.getFirst().getStagedLeftTuples();
      }
    }
 else {
      return smem.getFirst().getStagedLeftTuples();
    }
  }
 else {
    return null;
  }
}
