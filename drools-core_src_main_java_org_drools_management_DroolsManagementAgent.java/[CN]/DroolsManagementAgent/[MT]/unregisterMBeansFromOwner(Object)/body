{
  List<ObjectName> mbl=mbeans.remove(owner);
  if (mbl != null) {
    MBeanServer mbs=getMBeanServer();
    for (    ObjectName name : mbl) {
      try {
        mbs.unregisterMBean(name);
      }
 catch (      Exception e) {
        System.err.println("Exception unregistering mbean: " + name);
        e.printStackTrace();
      }
    }
  }
}
