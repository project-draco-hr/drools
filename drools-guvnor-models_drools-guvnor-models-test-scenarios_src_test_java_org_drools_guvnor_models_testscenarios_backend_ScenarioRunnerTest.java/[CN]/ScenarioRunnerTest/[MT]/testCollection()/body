{
  KieSession ksession=getKieSession("test_rules2.drl");
  ScenarioRunner run=new ScenarioRunner(ksession);
  Scenario scenario=new Scenario();
  scenario.getImports().addImport(new Import("org.drools.guvnor.models.testscenarios.backend.Cheese"));
  scenario.getImports().addImport(new Import("org.drools.guvnor.models.testscenarios.backend.Cheesery"));
  run.run(scenario);
}
