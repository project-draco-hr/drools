{
  StringBuffer sb=new StringBuffer();
  list.clear();
  int i=0;
  if (line.length() == 0) {
    list.add(line);
    return list;
  }
  do {
    sb.setLength(0);
    if (i < line.length() && line.charAt(i) == '"')     i=advQuoted(line,sb,++i);
 else     i=advPlain(line,sb,i);
    list.add(sb.toString());
    i++;
  }
 while (i < line.length());
  return list;
}
