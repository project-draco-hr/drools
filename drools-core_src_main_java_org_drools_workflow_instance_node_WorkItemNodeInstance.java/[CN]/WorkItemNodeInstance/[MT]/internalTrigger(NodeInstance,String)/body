{
  super.internalTrigger(from,type);
  WorkItemNode workItemNode=getWorkItemNode();
  createWorkItem(workItemNode);
  if (workItemNode.isWaitForCompletion()) {
    addWorkItemListener();
  }
  if (isInversionOfControl()) {
    ((InternalProcessInstance)getProcessInstance()).getWorkingMemory().update(((InternalProcessInstance)getProcessInstance()).getWorkingMemory().getFactHandle(this),this);
  }
 else {
    ((InternalWorkItemManager)((InternalProcessInstance)getProcessInstance()).getWorkingMemory().getWorkItemManager()).internalExecuteWorkItem(workItem);
  }
  if (!workItemNode.isWaitForCompletion()) {
    triggerCompleted();
  }
 else {
    this.workItemId=workItem.getId();
  }
}
