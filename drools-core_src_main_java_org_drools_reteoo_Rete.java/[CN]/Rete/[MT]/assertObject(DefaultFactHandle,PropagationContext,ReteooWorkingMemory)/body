{
  final HashMap memory=(HashMap)workingMemory.getNodeMemory(this);
  final Object object=handle.getObject();
  Object key=null;
  if (object.getClass() == FactImpl.class) {
    key=((Fact)object).getFactTemplate().getName();
  }
 else {
    key=object.getClass();
  }
  ObjectTypeNode[] cachedNodes=(ObjectTypeNode[])memory.get(key);
  if (cachedNodes == null) {
    cachedNodes=getMatchingNodes(object);
    memory.put(key,cachedNodes);
  }
  for (int i=0, length=cachedNodes.length; i < length; i++) {
    cachedNodes[i].assertObject(handle,context,workingMemory);
  }
}
