{
  if (argumentStack % 2 == 0) {
    ++argumentStack;
    declaration.append("<");
  }
 else {
    declaration.append(", ");
  }
  if (tag == SignatureVisitor.EXTENDS) {
    declaration.append("? extends ");
  }
 else   if (tag == SignatureVisitor.SUPER) {
    declaration.append("? super ");
  }
  startType();
  return this;
}
