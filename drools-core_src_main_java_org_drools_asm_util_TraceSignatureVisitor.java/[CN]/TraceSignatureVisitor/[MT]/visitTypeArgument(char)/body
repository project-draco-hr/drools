{
  if (this.argumentStack % 2 == 0) {
    ++this.argumentStack;
    this.declaration.append("<");
  }
 else {
    this.declaration.append(", ");
  }
  if (tag == SignatureVisitor.EXTENDS) {
    this.declaration.append("? extends ");
  }
 else   if (tag == SignatureVisitor.SUPER) {
    this.declaration.append("? super ");
  }
  startType();
  return this;
}
