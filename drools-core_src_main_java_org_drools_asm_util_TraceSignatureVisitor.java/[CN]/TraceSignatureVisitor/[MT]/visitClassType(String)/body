{
  if (!"java/lang/Object".equals(name)) {
    declaration.append(separator).append(name.replace('/','.'));
  }
 else {
    boolean needObjectClass=argumentStack % 2 == 1 || seenParameter;
    if (needObjectClass) {
      declaration.append(separator).append(name.replace('/','.'));
    }
  }
  separator="";
  argumentStack*=2;
}
