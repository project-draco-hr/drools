{
  if (!"java/lang/Object".equals(name)) {
    this.declaration.append(this.separator).append(name.replace('/','.'));
  }
 else {
    final boolean needObjectClass=this.argumentStack % 2 == 1 || this.seenParameter;
    if (needObjectClass) {
      this.declaration.append(this.separator).append(name.replace('/','.'));
    }
  }
  this.separator="";
  this.argumentStack*=2;
}
