{
  if (availableItems == null) {
    this.availableItems=null;
  }
 else {
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < availableItems.length; i++) {
      if (sb.length() > 0) {
        sb.append(COMMA_SEPARATOR);
      }
      sb.append(availableItems[i]);
    }
    if (sb.length() > 0) {
      this.availableItems=sb.toString();
    }
 else {
      this.availableItems=null;
    }
  }
}
