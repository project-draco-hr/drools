{
  if (array == null)   return false;
  if (obj instanceof ShadowProxy) {
    for (int i=0; i < array.length; i++) {
      if (array[i] == null && obj == null || obj != null && obj.equals(array[i])) {
        return true;
      }
    }
  }
 else {
    for (int i=0; i < array.length; i++) {
      if (array[i] == null && obj == null || array[i] != null && array[i].equals(obj)) {
        return true;
      }
    }
  }
  return false;
}
