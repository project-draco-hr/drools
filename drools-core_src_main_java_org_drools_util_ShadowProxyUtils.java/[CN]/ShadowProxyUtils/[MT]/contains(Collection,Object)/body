{
  if (col == null)   return false;
  if (obj instanceof ShadowProxy) {
    return col.contains(obj);
  }
 else {
    for (Iterator it=col.iterator(); it.hasNext(); ) {
      Object next=it.next();
      if (next == null && obj == null || next != null && next.equals(obj)) {
        return true;
      }
    }
  }
  return false;
}
