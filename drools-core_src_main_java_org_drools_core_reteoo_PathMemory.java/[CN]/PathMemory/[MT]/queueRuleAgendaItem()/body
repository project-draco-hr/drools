{
  if (!agendaItem.isQueued()) {
    InternalRuleFlowGroup rfg=agendaItem.getRuleFlowGroup();
    InternalAgendaGroup ag=agendaItem.getAgendaGroup();
    if (rfg != null) {
      rfg.addActivation(agendaItem);
    }
 else {
      ag.add(agendaItem);
    }
  }
  agendaItem.getRuleExecutor().setDirty(true);
}
