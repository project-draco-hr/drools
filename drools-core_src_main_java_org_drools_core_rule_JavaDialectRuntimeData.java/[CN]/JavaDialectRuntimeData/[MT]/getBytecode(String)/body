{
  byte[] bytecode=null;
  if (store != null) {
    bytecode=store.get(resourceName);
  }
  if (bytecode == null && rootClassLoader instanceof ProjectClassLoader) {
    bytecode=((ProjectClassLoader)rootClassLoader).getBytecode(resourceName);
    if (bytecode != null) {
      store.put(resourceName,bytecode);
    }
  }
  return bytecode;
}
