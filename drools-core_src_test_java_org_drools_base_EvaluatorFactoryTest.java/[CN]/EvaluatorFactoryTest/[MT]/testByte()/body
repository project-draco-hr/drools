  final Object[][] data={{new Byte("1"),"==",new Byte("1"),Boolean.TRUE},{new Byte("1"),"==",new Byte("2"),Boolean.FALSE},{new Byte("1"),"!=",new Byte("2"),Boolean.TRUE},{new Byte("1"),"!=",new Byte("1"),Boolean.FALSE},{new Byte("1"),"<=",new Byte("1"),Boolean.TRUE},{new Byte("1"),"==",null,Boolean.FALSE},{null,"==",null,Boolean.TRUE},{null,"!=",null,Boolean.FALSE},{null,"!=",new Byte("1"),Boolean.TRUE},{null,"==",new Byte("1"),Boolean.FALSE}};
