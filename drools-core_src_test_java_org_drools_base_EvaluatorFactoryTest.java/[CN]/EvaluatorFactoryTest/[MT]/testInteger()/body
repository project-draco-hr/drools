  final Object[][] data={{new Integer(42),"==",new Integer(42),Boolean.TRUE},{new Integer(42),"<",new Integer(43),Boolean.TRUE},{new Integer(42),">=",new Integer(41),Boolean.TRUE},{new Integer(42),"!=",new Integer(41),Boolean.TRUE},{new Integer(42),">",new Integer(41),Boolean.TRUE},{new Integer(42),"<=",new Integer(42),Boolean.TRUE},{new Integer(42),">",new Integer(100),Boolean.FALSE}};
