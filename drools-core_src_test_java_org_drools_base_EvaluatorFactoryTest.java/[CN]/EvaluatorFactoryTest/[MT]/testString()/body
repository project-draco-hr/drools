  Object[][] data={{"foo","==","bar",Boolean.FALSE},{"foo","==","foo",Boolean.TRUE},{"foo","!=","bar",Boolean.TRUE},{"something foo","matches",".*foo",Boolean.TRUE},{"foo","matches",".*foo",Boolean.TRUE},{"foo","matches","bar",Boolean.FALSE},{null,"matches",".*foo",Boolean.FALSE},{"foo","==",null,Boolean.FALSE}};
