{
  final ValueType type=ValueType.determineValueType(BigInteger.class);
  assertSame(type,ValueType.BIG_INTEGER_TYPE);
  Collection col=new ArrayList();
  col.add(new BigInteger("42"));
  col.add(new BigInteger("45"));
  final Object[][] data={{new BigInteger("42"),"==",new BigInteger("42"),Boolean.TRUE},{new BigInteger("42"),"<",new BigInteger("43"),Boolean.TRUE},{new BigInteger("42"),">=",new BigInteger("41"),Boolean.TRUE},{new BigInteger("42"),"!=",new BigInteger("41"),Boolean.TRUE},{new BigInteger("42"),">",new BigInteger("41"),Boolean.TRUE},{new BigInteger("42"),"<=",new BigInteger("42"),Boolean.TRUE},{new BigInteger("42"),">",new BigInteger("100"),Boolean.FALSE},{new BigInteger("42"),"==",null,Boolean.FALSE},{new BigInteger("42"),"!=",null,Boolean.TRUE},{null,"==",new BigInteger("42"),Boolean.FALSE},{null,"!=",new BigInteger("42"),Boolean.TRUE},{new BigInteger("42"),"memberOf",col,Boolean.TRUE},{new BigInteger("43"),"memberOf",col,Boolean.FALSE},{null,"memberOf",col,Boolean.FALSE},{new BigInteger("42"),"memberOf",null,Boolean.FALSE},{new BigInteger("42"),"not memberOf",col,Boolean.FALSE},{new BigInteger("43"),"not memberOf",col,Boolean.TRUE},{null,"not memberOf",col,Boolean.TRUE},{new BigInteger("42"),"not memberOf",null,Boolean.FALSE}};
  runEvaluatorTest(data,ValueType.BIG_INTEGER_TYPE);
}
