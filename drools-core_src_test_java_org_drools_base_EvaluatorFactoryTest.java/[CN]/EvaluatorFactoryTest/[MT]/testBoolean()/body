  final Object[][] data={{new Boolean(true),"==",new Boolean(true),Boolean.TRUE},{new Boolean(false),"!=",new Boolean(true),Boolean.TRUE},{new Boolean(true),"==",new Boolean(false),Boolean.FALSE},{new Boolean(true),"!=",new Boolean(false),Boolean.TRUE},{new Boolean(true),"==",null,Boolean.FALSE},{new Boolean(true),"!=",null,Boolean.TRUE},{null,"==",null,Boolean.TRUE}};
