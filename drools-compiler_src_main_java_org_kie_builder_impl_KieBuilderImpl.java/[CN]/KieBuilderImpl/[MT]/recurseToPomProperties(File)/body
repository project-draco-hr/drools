{
  for (  java.io.File child : file.listFiles()) {
    if (child.isDirectory()) {
      File returnedFile=recurseToPomProperties(child);
      if (returnedFile != null) {
        return returnedFile;
      }
    }
 else     if (child.getName().endsWith("pom.properties")) {
      return child;
    }
  }
  return null;
}
