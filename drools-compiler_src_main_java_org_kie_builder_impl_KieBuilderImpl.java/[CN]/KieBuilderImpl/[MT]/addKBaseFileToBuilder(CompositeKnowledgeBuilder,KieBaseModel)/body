{
  for (  String fileName : srcMfs.getFileNames()) {
    if (filterFileInKBase(kieBase,fileName)) {
      byte[] bytes=srcMfs.getBytes(fileName);
      ckbuilder.add(ResourceFactory.newByteArrayResource(srcMfs.getBytes(fileName)),ResourceType.determineResourceType(fileName));
      trgMfs.write(fileName,bytes,true);
    }
  }
}
