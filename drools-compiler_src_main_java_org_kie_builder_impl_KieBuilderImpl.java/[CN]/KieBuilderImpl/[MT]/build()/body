{
  if (!isBuilt()) {
    trgMfs=new MemoryFileSystem();
    writePomAndKProject();
    kieJar=new MemoryKieJar(kieProject,trgMfs);
    compileJavaClasses();
    compileKieFiles();
    if (!hasMessages(Level.ERROR)) {
      KieServices.Factory.get().getKieRepository().addKieJar(kieJar);
    }
  }
  return new MessagesImpl(messages,null);
}
