{
  System.out.println("registering compiler services");
  this.kbuilderReg=bc.registerService(new String[]{KnowledgeBuilderProvider.class.getName(),Service.class.getName()},new KnowledgeBuilderProviderImpl(),new Hashtable());
  this.dtableTracker=new ServiceTracker(bc,DecisionTableProvider.class.getName(),new DroolsServiceTracker(bc,this));
  this.dtableTracker.open();
  System.out.println("compiler services registered");
}
