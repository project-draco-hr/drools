{
  if (this.handle == handle) {
    if (this.instances == null) {
      this.handle=null;
    }
 else {
      this.handle=(InternalFactHandle)this.instances.remove(0);
      if (this.instances.isEmpty()) {
        this.instances=null;
      }
    }
  }
 else {
    this.instances.remove(handle);
    if (this.instances.isEmpty()) {
      this.instances=null;
    }
  }
}
