{
  final RuleModel m=getComplexModel(false);
  String expected="rule \"Complex Rule\"\n" + "\tno-loop true\n" + "\tsalience -10\n"+ "\tagenda-group \"aGroup\"\n"+ "\tdialect \"mvel\"\n"+ "\twhen\n"+ "\t\tp1 : Person( f1 : age < 42 )\n"+ "\t\tnot (Cancel( )) \n"+ "\tthen\n"+ "\t\tp1.setStatus( \"rejected\" );\n"+ "\t\tupdate( p1 );\n"+ "\t\tretract( p1 );\n"+ "end\n";
