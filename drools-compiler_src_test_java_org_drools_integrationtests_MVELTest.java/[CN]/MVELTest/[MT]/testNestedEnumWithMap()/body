{
  String str="" + "package org.test \n" + "import " + DMap.class.getCanonicalName() + " \n"+ "import "+ Triangle.class.getCanonicalName()+ "\n"+ "global java.util.List list \n"+ "rule \"show\" \n"+ "when  \n"+ "    $m : DMap( this[Triangle.Type.ACUTE] == 'xxx') \n"+ "then \n"+ "    list.add('r1'); \n"+ "end \n";
  KnowledgeBuilder kbuilder=KnowledgeBuilderFactory.newKnowledgeBuilder();
  kbuilder.add(ResourceFactory.newByteArrayResource(str.getBytes()),ResourceType.DRL);
  if (kbuilder.hasErrors()) {
    fail(kbuilder.getErrors().toString());
  }
  KnowledgeBase kbase=KnowledgeBaseFactory.newKnowledgeBase();
  kbase.addKnowledgePackages(kbuilder.getKnowledgePackages());
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  List list=new ArrayList();
  ksession.setGlobal("list",list);
  DMap m=new DMap();
  m.put(Triangle.Type.ACUTE,"xxx");
  ksession.insert(m);
  ksession.fireAllRules();
  assertEquals("r1",list.get(0));
}
