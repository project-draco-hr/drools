{
  final Class clazz=((ClassObjectType)pattern.getObjectType()).getClassType();
  final FieldExtractor extractor=ClassFieldExtractorCache.getInstance().getExtractor(clazz,fieldName,getClass().getClassLoader());
  final FieldValue field=new BooleanFieldImpl(fieldValue);
  return new LiteralConstraint(extractor,evaluator,field);
}
