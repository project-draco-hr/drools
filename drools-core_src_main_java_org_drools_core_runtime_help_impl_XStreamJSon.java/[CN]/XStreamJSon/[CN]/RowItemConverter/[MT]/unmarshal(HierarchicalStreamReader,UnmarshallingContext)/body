{
  String externalForm=null;
  Object object=null;
  while (reader.hasMoreChildren()) {
    reader.moveDown();
    String nodeName=reader.getNodeName();
    if ("external-form".equals(nodeName)) {
      externalForm=reader.getValue();
    }
 else     if ("object".equals(nodeName)) {
      reader.moveDown();
      object=readItem(reader,context,null);
      reader.moveUp();
    }
    reader.moveUp();
  }
  return new RowItemContainer(DefaultFactHandle.createFromExternalFormat(externalForm),object);
}
