{
  if (e.equals("!=")) {
    return "==";
  }
 else   if (e.equals("==")) {
    return "!=";
  }
 else   if (e.equals(">")) {
    return "<=";
  }
 else   if (e.equals("<")) {
    return ">=";
  }
 else   if (e.equals(">=")) {
    return "<";
  }
 else   if (e.equals("<=")) {
    return ">";
  }
  return e;
}
