{
synchronized (context.getRuleBase().getPackagesMap()) {
    ObjectTypeNode otn=new ObjectTypeNode(context.getNextId(),objectType,context);
    InternalWorkingMemory[] wms=context.getWorkingMemories();
    if (wms.length > 0) {
      otn.attach(wms);
    }
 else {
      otn.attach();
    }
    return otn;
  }
}
