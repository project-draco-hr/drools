{
  String newName=name.replaceAll("(^[0-9]|[^\\w$])","_");
  int counter=-1;
  boolean exists=true;
  while (exists) {
    counter++;
    String fileName=packageName.replaceAll("\\.","/") + newName + "_"+ counter+ ext;
    this.ruleSetBundle.getMemoryResourceReader().isAvailable(fileName);
  }
  if (counter >= 0) {
    newName=newName + "_" + counter;
  }
  return newName;
}
