{
  MarshallerWriteContext context=new MarshallerWriteContext(stream,(InternalRuleBase)((KnowledgeBaseImpl)kbase).ruleBase,(InternalWorkingMemory)((StatefulKnowledgeSessionImpl)session).session,RuleBaseNodes.getNodeMap((InternalRuleBase)((KnowledgeBaseImpl)kbase).ruleBase),this.strategyStore,this.marshallingConfig.isMarshallProcessInstances(),this.marshallingConfig.isMarshallWorkItems());
  OutputMarshaller.writeSession(context);
  context.close();
}
