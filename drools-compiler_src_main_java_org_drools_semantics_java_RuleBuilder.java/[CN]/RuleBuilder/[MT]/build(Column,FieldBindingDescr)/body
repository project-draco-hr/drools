{
  Declaration declaration=(Declaration)this.declarations.get(fieldBindingDescr.getIdentifier());
  if (declaration != null) {
    build(column,new BoundVariableDescr(fieldBindingDescr.getFieldName(),"==",fieldBindingDescr.getIdentifier()));
    return;
  }
  final Class clazz=((ClassObjectType)column.getObjectType()).getClassType();
  final FieldExtractor extractor=getFieldExtractor(fieldBindingDescr,clazz,fieldBindingDescr.getFieldName());
  if (extractor == null) {
    return;
  }
  declaration=column.addDeclaration(fieldBindingDescr.getIdentifier(),extractor);
  this.declarations.put(declaration.getIdentifier(),declaration);
  if (this.notDeclarations != null) {
    this.notDeclarations.put(declaration.getIdentifier(),declaration);
  }
}
