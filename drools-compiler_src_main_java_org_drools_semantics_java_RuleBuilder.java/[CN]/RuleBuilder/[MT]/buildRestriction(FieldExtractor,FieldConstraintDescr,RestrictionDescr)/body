{
  Restriction restriction=null;
  if (restrictionDescr instanceof LiteralRestrictionDescr) {
    restriction=buildRestriction(extractor,fieldConstraintDescr,(LiteralRestrictionDescr)restrictionDescr);
  }
 else   if (restrictionDescr instanceof VariableRestrictionDescr) {
    restriction=buildRestriction(extractor,fieldConstraintDescr,(VariableRestrictionDescr)restrictionDescr);
  }
 else   if (restrictionDescr instanceof ReturnValueRestrictionDescr) {
    restriction=buildRestriction(extractor,fieldConstraintDescr,(ReturnValueRestrictionDescr)restrictionDescr);
  }
  return restriction;
}
