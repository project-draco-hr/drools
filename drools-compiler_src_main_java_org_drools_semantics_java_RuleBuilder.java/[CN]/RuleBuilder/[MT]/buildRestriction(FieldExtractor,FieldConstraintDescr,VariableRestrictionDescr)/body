{
  if (variableRestrictionDescr.getIdentifier() == null || variableRestrictionDescr.getIdentifier().equals("")) {
    this.errors.add(new RuleError(this.rule,variableRestrictionDescr,null,"Identifier not defined for binding field '" + fieldConstraintDescr.getFieldName() + "'"));
    return null;
  }
  final Declaration declaration=(Declaration)this.declarations.get(variableRestrictionDescr.getIdentifier());
  if (declaration == null) {
    this.errors.add(new RuleError(this.rule,variableRestrictionDescr,null,"Unable to return Declaration for identifier '" + variableRestrictionDescr.getIdentifier() + "'"));
    return null;
  }
  final Evaluator evaluator=getEvaluator(variableRestrictionDescr,extractor.getValueType(),variableRestrictionDescr.getEvaluator());
  if (evaluator == null) {
    return null;
  }
  return new VariableRestriction(declaration,evaluator);
}
