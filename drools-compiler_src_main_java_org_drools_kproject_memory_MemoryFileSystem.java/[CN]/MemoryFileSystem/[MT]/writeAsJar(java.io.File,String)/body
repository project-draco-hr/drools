{
  ZipOutputStream out=null;
  try {
    java.io.File jarFile=new java.io.File(folder,jarName + ".jar");
    out=new ZipOutputStream(new FileOutputStream(jarFile));
    writeJarEntries(getRootFolder(),out);
    out.close();
    return jarFile;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    try {
      out.close();
    }
 catch (    IOException e) {
    }
  }
}
