{
  if (t.getSort() == Type.OBJECT) {
    return Type.getType("L" + map(t.getInternalName()) + ";");
  }
 else   if (t.getSort() == Type.ARRAY) {
    String s=fix(t.getElementType()).getDescriptor();
    for (int i=0; i < t.getDimensions(); ++i) {
      s="[" + s;
    }
    return Type.getType(s);
  }
 else {
    return t;
  }
}
