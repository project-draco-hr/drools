{
  if (this._isShared) {
    clearShared();
    return;
  }
  for (Entry e=this._head, end=this._tail; (e=e._next) != end; ) {
    e._key=null;
    e._value=null;
    final Entry[][] table=e._table;
    table[(e._keyHash >> FastMap.R0) & (table.length - 1)][e._keyHash & FastMap.M0]=null;
  }
  this._tail=this._head._next;
  this._size=0;
  this._oldEntries=null;
}
