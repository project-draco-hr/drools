{
  if (obj == this) {
    return true;
  }
 else   if (obj instanceof Map) {
    final Map that=(Map)obj;
    if (this.size() == that.size()) {
      final Set thatEntrySet=that.entrySet();
      for (Entry e=this._head, end=this._tail; (e=e._next) != end; ) {
        if (!thatEntrySet.contains(e)) {
          return false;
        }
      }
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
