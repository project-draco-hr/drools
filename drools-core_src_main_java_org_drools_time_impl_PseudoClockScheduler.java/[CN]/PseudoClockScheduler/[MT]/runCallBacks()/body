{
  ScheduledJob item=queue.peek();
  while (item != null && (item.getTrigger().hasNextFireTime().getTime() <= this.timer)) {
    queue.remove();
    item.getTrigger().nextFireTime();
    if (item.getTrigger().hasNextFireTime() != null) {
      queue.add(item);
    }
    try {
      item.call();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    item=queue.peek();
  }
}
