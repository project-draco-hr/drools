{
  final FieldBindingDescr implicitBinding=new FieldBindingDescr(identifier,identifier);
  final FieldExtractor extractor=getFieldExtractor(context,implicitBinding,pattern.getObjectType(),implicitBinding.getFieldName(),false);
  if (extractor == null) {
    return null;
  }
  final Declaration declaration=new Declaration(identifier,extractor,pattern);
  return declaration;
}
