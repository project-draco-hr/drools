{
  final FieldBindingDescr implicitBinding=new FieldBindingDescr(identifier,identifier);
  final Declaration declaration=new Declaration(identifier,pattern);
  final InternalReadAccessor extractor=getFieldReadAccessor(context,implicitBinding,pattern.getObjectType(),implicitBinding.getFieldName(),declaration,false);
  if (extractor == null) {
    return null;
  }
  return declaration;
}
