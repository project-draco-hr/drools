{
  final BindingDescr implicitBinding=new BindingDescr(identifier,identifier);
  final Declaration declaration=new Declaration(identifier,pattern);
  final InternalReadAccessor extractor=getFieldReadAccessor(context,implicitBinding,pattern.getObjectType(),implicitBinding.getExpression(),declaration,false);
  if (extractor == null) {
    return null;
  }
  return declaration;
}
