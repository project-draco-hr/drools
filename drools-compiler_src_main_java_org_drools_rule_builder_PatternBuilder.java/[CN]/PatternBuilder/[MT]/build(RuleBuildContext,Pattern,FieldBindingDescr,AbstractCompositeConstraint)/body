{
  if (context.getDeclarationResolver().isDuplicated(context.getRule(),fieldBindingDescr.getIdentifier())) {
    FieldConstraintDescr descr=new FieldConstraintDescr(fieldBindingDescr.getFieldName());
    descr.addRestriction(new VariableRestrictionDescr("==",fieldBindingDescr.getIdentifier()));
    build(context,pattern,(FieldConstraintDescr)descr,container);
    return;
  }
  Declaration declr=pattern.addDeclaration(fieldBindingDescr.getIdentifier());
  final InternalReadAccessor extractor=getFieldReadAccessor(context,fieldBindingDescr,pattern.getObjectType(),fieldBindingDescr.getFieldName(),declr,true);
}
