{
  if (context.getDeclarationResolver().isDuplicated(fieldBindingDescr.getIdentifier())) {
    context.getErrors().add(new DescrBuildError(context.getParentDescr(),fieldBindingDescr,null,"Duplicate declaration for variable '" + fieldBindingDescr.getIdentifier() + "' in the rule '"+ context.getRule().getName()+ "'"));
    return;
  }
  final FieldExtractor extractor=getFieldExtractor(context,fieldBindingDescr,pattern.getObjectType(),fieldBindingDescr.getFieldName(),true);
  if (extractor == null) {
    return;
  }
  pattern.addDeclaration(fieldBindingDescr.getIdentifier(),extractor);
}
