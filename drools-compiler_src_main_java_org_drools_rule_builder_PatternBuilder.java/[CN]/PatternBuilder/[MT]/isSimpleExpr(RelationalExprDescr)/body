{
  boolean simple=false;
  if (relDescr != null) {
    if ((relDescr.getLeft() instanceof AtomicExprDescr || relDescr.getLeft() instanceof BindingDescr) && (relDescr.getRight() instanceof AtomicExprDescr || relDescr.getRight() instanceof BindingDescr)) {
      simple=true;
    }
  }
  return simple;
}
