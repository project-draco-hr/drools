{
  return (indexed[0] || constraints[0].isAllowedCachedLeft(context[0],handle)) && (indexed[1] || constraints[1].isAllowedCachedLeft(context[1],handle)) && (indexed[2] || constraints[2].isAllowedCachedLeft(context[2],handle))&& (indexed[3] || constraints[3].isAllowedCachedLeft(context[3],handle));
}
