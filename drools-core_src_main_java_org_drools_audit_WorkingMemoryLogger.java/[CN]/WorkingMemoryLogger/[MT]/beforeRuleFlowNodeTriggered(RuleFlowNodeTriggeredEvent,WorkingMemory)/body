{
  filterLogEvent(new RuleFlowNodeLogEvent(LogEvent.BEFORE_RULEFLOW_NODE_TRIGGERED,event.getRuleFlowNodeInstance().getId() + "",event.getRuleFlowNodeInstance().getProcessInstance().getRuleFlowProcess().getNode(event.getRuleFlowNodeInstance().getNodeId()).getName(),event.getRuleFlowProcessInstance().getProcess().getId(),event.getRuleFlowProcessInstance().getProcess().getName()));
}
