{
  filterLogEvent(new RuleFlowNodeLogEvent(LogEvent.BEFORE_RULEFLOW_NODE_EXITED,createNodeId(event.getRuleFlowNodeInstance()),event.getRuleFlowNodeInstance().getNodeName(),createNodeInstanceId(event.getRuleFlowNodeInstance()),event.getProcessInstance().getProcessId(),event.getProcessInstance().getProcessName(),event.getProcessInstance().getId()));
}
