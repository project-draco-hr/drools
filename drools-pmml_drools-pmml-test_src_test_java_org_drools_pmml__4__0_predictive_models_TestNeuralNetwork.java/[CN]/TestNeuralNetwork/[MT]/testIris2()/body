  Assert.assertEquals("Iris_setosa",queryStringField("NSPECIES_Iris_setosa","IRIS_MLP"));
  Assert.assertEquals("Iris_versicolor",queryStringField("NSPECIES_Iris_versicolor","IRIS_MLP"));
