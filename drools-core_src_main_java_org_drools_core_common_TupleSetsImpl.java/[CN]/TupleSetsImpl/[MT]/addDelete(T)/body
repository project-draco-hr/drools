{
switch (tuple.getStagedType()) {
case Tuple.INSERT:
    removeInsert(tuple);
  return deleteFirst == null;
case Tuple.UPDATE:
removeUpdate(tuple);
break;
}
tuple.setStagedType(Tuple.DELETE);
if (deleteFirst == null) {
deleteFirst=tuple;
return true;
}
tuple.setStagedNext(deleteFirst);
deleteFirst.setStagedPrevious(tuple);
deleteFirst=tuple;
return false;
}
