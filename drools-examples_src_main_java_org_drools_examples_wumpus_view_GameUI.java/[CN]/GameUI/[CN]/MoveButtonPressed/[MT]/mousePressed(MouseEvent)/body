{
  gameData.getKsession().insert(new MoveCommand(move));
  gameData.getKsession().fireAllRules();
  if (gameData.isPittDeath() || gameData.isWumpusDeath()) {
    gameData.getKlogger().close();
    gameData.setShowAllCells(true);
    updateCave();
    updateSensors();
    int answre=JOptionPane.showConfirmDialog(null,"Play Again1?","DEAD",JOptionPane.OK_OPTION);
    gameData.getWumpusWorld().setData(gameData);
    updateCave();
    updateSensors();
  }
 else   if (gameData.isGoldWin()) {
    gameData.getKlogger().close();
    gameData.setShowAllCells(true);
    updateCave();
    updateSensors();
    int answre=JOptionPane.showConfirmDialog(null,"Play Again1?","WIN",JOptionPane.OK_OPTION);
    gameData.getWumpusWorld().setData(gameData);
    updateCave();
    updateSensors();
  }
 else {
    updateCave();
    updateSensors();
  }
}
