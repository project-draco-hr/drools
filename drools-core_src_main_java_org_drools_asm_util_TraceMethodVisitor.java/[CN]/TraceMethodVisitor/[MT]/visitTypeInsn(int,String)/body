{
  this.buf.setLength(0);
  this.buf.append(this.tab2).append(AbstractVisitor.OPCODES[opcode]).append(' ');
  if (desc.startsWith("[")) {
    appendDescriptor(TraceAbstractVisitor.FIELD_DESCRIPTOR,desc);
  }
 else {
    appendDescriptor(TraceAbstractVisitor.INTERNAL_NAME,desc);
  }
  this.buf.append('\n');
  this.text.add(this.buf.toString());
  if (this.mv != null) {
    this.mv.visitTypeInsn(opcode,desc);
  }
}
