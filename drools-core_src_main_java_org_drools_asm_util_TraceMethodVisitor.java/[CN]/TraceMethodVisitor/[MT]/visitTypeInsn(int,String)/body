{
  buf.setLength(0);
  buf.append(tab2).append(OPCODES[opcode]).append(' ');
  if (desc.startsWith("[")) {
    appendDescriptor(FIELD_DESCRIPTOR,desc);
  }
 else {
    appendDescriptor(INTERNAL_NAME,desc);
  }
  buf.append('\n');
  text.add(buf.toString());
  if (mv != null) {
    mv.visitTypeInsn(opcode,desc);
  }
}
