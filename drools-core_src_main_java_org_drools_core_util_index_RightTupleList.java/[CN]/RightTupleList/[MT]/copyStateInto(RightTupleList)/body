{
  other.previous=previous;
  other.next=next;
  other.first=first;
  other.last=last;
  other.hashCode=hashCode;
  other.index=index;
  other.iterator=iterator;
  other.size=size;
  for (RightTuple current=first; current != null; current=(RightTuple)current.getNext()) {
    current.setMemory(other);
  }
}
