{
  name=name.substring(0,1).toUpperCase() + name.substring(1);
  MethodVisitor mv=cw.visitMethod(ACC_PUBLIC + ACC_ABSTRACT,"set" + name,"(" + BuildUtils.getTypeDescriptor(type) + ")V",generic == null ? null : "(" + BuildUtils.getTypeDescriptor(type).replace(";","<" + BuildUtils.getTypeDescriptor(generic) + ">;") + ")V",null);
  mv.visitEnd();
}
