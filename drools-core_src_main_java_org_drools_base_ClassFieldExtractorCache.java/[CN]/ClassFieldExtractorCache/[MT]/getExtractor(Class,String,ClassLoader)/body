{
  if (cache == null) {
    cache=new HashMap();
  }
  final ExtractorKey key=new ExtractorKey(clazz,fieldName);
  if (cache.containsKey(key)) {
    return (ClassFieldExtractor)cache.get(key);
  }
 else {
    final ClassFieldExtractor ex=new ClassFieldExtractor(clazz,fieldName,classLoader,factory);
    cache.put(key,ex);
    return ex;
  }
}
