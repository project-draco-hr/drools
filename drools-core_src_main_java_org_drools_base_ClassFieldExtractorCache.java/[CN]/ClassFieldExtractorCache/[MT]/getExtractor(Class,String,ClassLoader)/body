{
  if (cache == null) {
    cache=new HashMap();
  }
  final String key=clazz.getName() + "|" + fieldName;
  if (cache.containsKey(key)) {
    return (ClassFieldExtractor)cache.get(key);
  }
 else {
    final ClassFieldExtractor ex=new ClassFieldExtractor(clazz,fieldName,classLoader);
    cache.put(key,ex);
    return ex;
  }
}
