{
  this.severityMap=new HashMap<String,ProblemSeverity>();
  Map<String,String> temp=new HashMap<String,String>();
  this.chainedProperties.mapStartsWith(temp,ProblemSeverityOption.PROPERTY_NAME,true);
  int index=ProblemSeverityOption.PROPERTY_NAME.length();
  for (  Map.Entry<String,String> entry : temp.entrySet()) {
    String identifier=entry.getKey().trim().substring(index);
    this.severityMap.put(identifier,ProblemSeverityOption.get(identifier,entry.getValue()).getSeverity());
  }
}
