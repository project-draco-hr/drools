{
  this.handle=handle;
  this.sink=sink;
  RightTuple last=handle.getLastRightTuple();
  if (last == null) {
    handle.setFirstRightTuple(this);
    handle.setLastRightTuple(this);
  }
 else {
    this.handlePrevious=last;
    last.setHandleNext(this);
    this.handle.setLastRightTuple(this);
  }
}
