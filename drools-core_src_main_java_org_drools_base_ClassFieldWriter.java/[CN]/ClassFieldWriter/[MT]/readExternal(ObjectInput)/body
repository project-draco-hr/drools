{
  clazz=(Class<?>)is.readObject();
  fieldName=(String)is.readObject();
  if (is instanceof DroolsObjectInput) {
    DroolsObjectInput droolsInput=(DroolsObjectInput)is;
    writer=droolsInput.getExtractorFactory().getWriter(clazz,fieldName,droolsInput.getClassLoader());
  }
 else   writer=ClassFieldAccessorCache.getInstance().getWriter(clazz,fieldName,findClassLoader(null));
}
