{
  final LeapsWorkingMemory workingMemory=new LeapsWorkingMemory(this.workingMemoryCounter++,this);
  for (final Iterator it=this.leapsRules.values().iterator(); it.hasNext(); ) {
    workingMemory.addLeapsRules((List)it.next());
  }
  super.addWorkingMemory(workingMemory,keepReference);
  return workingMemory;
}
