{
  final AlphaNodeSwitch alphaSwitch1=new AlphaNodeSwitch(this.constraint1);
  alphaSwitch1.addAlphaNode(this.alphaNode1);
  Assert.assertEquals("AlphaSwitch should not be empty",1,alphaSwitch1.getSwitchCount());
  alphaSwitch1.removeAlphaNode(this.alphaNode1);
  Assert.assertEquals("AlphaSwitch should be empty",0,alphaSwitch1.getSwitchCount());
}
