{
  if (this.lhs == null) {
    return null;
  }
  for (int i=0; i < this.lhs.length; i++) {
    if (this.lhs[i] instanceof FactPattern) {
      final FactPattern p=(FactPattern)this.lhs[i];
      if (p.boundName != null && var.equals(p.boundName)) {
        return p;
      }
    }
  }
  return null;
}
