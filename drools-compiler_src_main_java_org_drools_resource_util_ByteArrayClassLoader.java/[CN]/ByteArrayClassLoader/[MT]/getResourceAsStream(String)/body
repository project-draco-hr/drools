{
  final byte[] bytes=(byte[])this.resources.get(name);
  if (bytes != null) {
    return new ByteArrayInputStream(bytes);
  }
 else {
    InputStream input=this.getParent().getResourceAsStream(name);
    if (input == null) {
      input=super.getResourceAsStream(name);
    }
    return input;
  }
}
