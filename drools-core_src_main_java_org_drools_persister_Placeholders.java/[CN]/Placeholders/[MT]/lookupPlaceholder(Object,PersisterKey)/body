{
  final int hashCode=key.hashCode(object);
  final int index=indexOf(hashCode,this.table.length);
  PlaceholderEntry current=(PlaceholderEntry)this.table[index];
  while (current != null) {
    if (hashCode == current.hashCode && current.key == key && key.equal(object,current.object)) {
      return current;
    }
    current=(PlaceholderEntry)current.getNext();
  }
  return null;
}
