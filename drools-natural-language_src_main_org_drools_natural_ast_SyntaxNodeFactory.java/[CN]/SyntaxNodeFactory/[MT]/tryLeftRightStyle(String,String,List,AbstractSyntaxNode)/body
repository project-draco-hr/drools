{
  if (vars.size() == 0) {
    result=new SubstitutionNode(token,exp);
  }
 else   if (vars.contains(ArgumentNode.LEFT) && vars.contains(ArgumentNode.RIGHT)) {
    result=new LeftRightNode(token,exp,1,1);
  }
 else   if (vars.contains(ArgumentNode.LEFT)) {
    result=new LeftOnlyNode(token,exp,1);
  }
 else   if (vars.contains(ArgumentNode.RIGHT)) {
    result=new RightOnlyNode(token,exp,1);
  }
  return result;
}
