{
  boolean stateChanged=false;
  Cell[] rowOfCells=null;
  Cell cell=null;
  for (int i=0; i < this.cells.length; i++) {
    rowOfCells=this.cells[i];
    for (int j=0; j < rowOfCells.length; j++) {
      cell=rowOfCells[j];
      stateChanged|=cell.transitionState();
    }
  }
  return stateChanged;
}
