{
  int number=0;
  Cell[] rowOfCells=null;
  Cell cell=null;
  for (int i=0; i < cells.length; i++) {
    rowOfCells=cells[i];
    for (int j=0; j < rowOfCells.length; j++) {
      cell=rowOfCells[j];
      if (cell.getCellState() == CellState.LIVE) {
        number++;
      }
    }
  }
  return number;
}
