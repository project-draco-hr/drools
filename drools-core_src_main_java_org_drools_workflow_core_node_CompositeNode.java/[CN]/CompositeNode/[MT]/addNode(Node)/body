{
  if (node.getId() <= 0) {
    long id=0;
    for (    Node n : nodeContainer.getNodes()) {
      if (n.getId() > id) {
        id=n.getId();
      }
    }
    ((org.drools.workflow.core.Node)node).setId(++id);
  }
  nodeContainer.addNode(node);
  ((org.drools.workflow.core.Node)node).setNodeContainer(this);
}
