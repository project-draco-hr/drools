{
  VariableResolverFactory factory=unit.getFactory(null,null,handle.getObject(),(LeftTuple)leftTuple,null,(InternalWorkingMemory)workingMemory,workingMemory.getGlobalResolver());
  final Object value=MVEL.executeExpression(this.expression,handle.getObject(),factory);
  if (this.function.supportsReverse()) {
    ((MVELAccumulatorFunctionContext)context).reverseSupport.put(Integer.valueOf(handle.getId()),value);
  }
  this.function.accumulate(((MVELAccumulatorFunctionContext)context).context,value);
}
