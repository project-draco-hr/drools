{
  if (this.constraints == Collections.EMPTY_LIST) {
    this.constraints=new ArrayList(1);
  }
  Declaration declaration=this.declarations != null ? (Declaration)this.declarations.get(identifier) : null;
  if (declaration == null) {
    declaration=new Declaration(identifier,extractor,this);
    this.constraints.add(declaration);
    if (this.declarations == null) {
      this.declarations=new HashMap(2);
    }
    this.declarations.put(declaration.getIdentifier(),declaration);
  }
  return declaration;
}
