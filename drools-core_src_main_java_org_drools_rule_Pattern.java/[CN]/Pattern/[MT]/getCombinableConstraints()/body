{
  List<MvelConstraint> combinableConstraints=new ArrayList<MvelConstraint>();
  for (  Constraint constraint : constraints) {
    if (constraint instanceof MvelConstraint && !((MvelConstraint)constraint).isUnification() && !((MvelConstraint)constraint).isIndexable()&& constraint.getType() == ConstraintType.BETA) {
      combinableConstraints.add((MvelConstraint)constraint);
    }
  }
  return combinableConstraints;
}
