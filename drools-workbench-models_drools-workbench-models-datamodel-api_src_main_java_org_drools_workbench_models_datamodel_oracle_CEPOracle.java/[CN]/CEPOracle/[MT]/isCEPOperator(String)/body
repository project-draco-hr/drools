{
  if (operator == null) {
    return false;
  }
  String[] operators=OracleUtils.joinArrays(SIMPLE_CEP_OPERATORS,COMPLEX_CEP_OPERATORS,SIMPLE_CEP_CONNECTIVES,COMPLEX_CEP_CONNECTIVES);
  for (int i=0; i < operators.length; i++) {
    if (operator.equals(operators[i])) {
      return true;
    }
  }
  return false;
}
