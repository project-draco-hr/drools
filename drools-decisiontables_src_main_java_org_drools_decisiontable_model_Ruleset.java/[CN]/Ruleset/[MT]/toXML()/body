{
  String xml="<?xml version=\"1.0\"?> " + "<rule-set name=\"" + _name + "\" "+ "xmlns=\"http://drools.org/rules\" "+ "xmlns:java=\"http://drools.org/semantics/java\" "+ "xmlns:xs=\"http://www.w3.org/2001/XMLSchema-instance\" "+ "xs:schemaLocation=\"http://drools.org/rules rules.xsd http://drools.org/semantics/java java.xsd\">";
  xml=xml + "\n";
  xml=xml + generateXml(_imports) + "\n";
  xml=xml + generateXml(_variables);
  xml=xml + "\n" + _functions.toXML()+ "\n\n";
  xml=xml + generateXml(_rules);
  xml=xml + "\n\n\n</rule-set>";
  return xml.trim();
}
