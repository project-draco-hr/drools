{
  if (this.firstNode != node && this.lastNode != node) {
    node.getPrevious().setNext(node.getNext());
    node.getNext().setPrevious(node.getPrevious());
    this.size--;
  }
 else {
    if (this.firstNode == node) {
      removeFirst();
    }
 else     if (this.lastNode == node) {
      removeLast();
    }
  }
}
