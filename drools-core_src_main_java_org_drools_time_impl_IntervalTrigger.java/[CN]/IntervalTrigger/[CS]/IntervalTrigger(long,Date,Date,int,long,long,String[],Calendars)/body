{
  this.period=period;
  if (startTime == null) {
    startTime=new Date(timestamp);
  }
  setStartTime(startTime);
  if (endTime != null) {
    setEndTime(endTime);
  }
  this.repeatLimit=repeatLimit;
  this.calendarNames=calendarNames;
  this.calendars=calendars;
  this.nextFireTime=new Date(timestamp + delay);
  setFirstFireTime();
  updateToNextIncludeDate();
}
