 else         if ("float".equals(field.getTypeName())) {
          mv.visitMethodInsn(Opcodes.INVOKESTATIC,Type.getInternalName(Float.class),"floatToIntBits","(F)I");
        }
 else         if ("double".equals(field.getTypeName())) {
