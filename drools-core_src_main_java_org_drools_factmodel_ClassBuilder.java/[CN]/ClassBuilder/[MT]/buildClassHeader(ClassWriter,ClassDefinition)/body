{
  String[] original=classDef.getInterfaces();
  String[] interfaces=new String[original.length];
  for (int i=0; i < original.length; i++) {
    interfaces[i]=getInternalType(original[i]);
  }
  cw.visit(Opcodes.V1_4,Opcodes.ACC_PUBLIC + Opcodes.ACC_SUPER,getInternalType(classDef.getClassName()),null,getInternalType(classDef.getSuperClass()),interfaces);
  cw.visitSource(classDef.getClassName() + ".java",null);
}
