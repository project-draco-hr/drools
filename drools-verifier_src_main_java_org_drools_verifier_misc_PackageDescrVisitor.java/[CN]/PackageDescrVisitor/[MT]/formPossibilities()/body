{
  for (  SubPattern subPattern : solvers.getPatternPossibilities()) {
    Pattern pattern=data.getVerifierObject(VerifierComponentType.PATTERN,subPattern.getPatternGuid());
    subPattern.setPattern(pattern);
    data.add(subPattern);
  }
  for (  SubRule subRule : solvers.getRulePossibilities()) {
    VerifierRule rule=data.getVerifierObject(VerifierComponentType.RULE,subRule.getRuleGuid());
    subRule.setRule(rule);
    data.add(subRule);
  }
}
