{
  ObjectType objectType=data.getObjectTypeByFullName(name);
  if (objectType == null) {
    objectType=new ObjectType();
    objectType.setName(name);
    String fullName=imports.get(name);
    if (fullName == null)     fullName=name;
    objectType.setFullName(fullName);
    data.add(objectType);
  }
  return objectType;
}
