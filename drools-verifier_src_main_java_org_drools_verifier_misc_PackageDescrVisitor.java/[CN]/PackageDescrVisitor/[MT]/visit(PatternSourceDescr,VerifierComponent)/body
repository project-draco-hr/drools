{
  if (descr instanceof AccumulateDescr) {
    return visit((AccumulateDescr)descr,parent);
  }
 else   if (descr instanceof CollectDescr) {
    return visit((CollectDescr)descr,parent);
  }
 else   if (descr instanceof FromDescr) {
    return visit((FromDescr)descr,parent);
  }
 else {
    throw new UnknownDescriptionException(descr);
  }
}
