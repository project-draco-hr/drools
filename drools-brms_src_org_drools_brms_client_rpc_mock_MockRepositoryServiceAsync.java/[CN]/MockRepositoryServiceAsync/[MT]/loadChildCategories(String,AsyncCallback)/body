{
  final AsyncCallback cb=callback;
  final String cat=categoryPath;
  Timer t=new Timer(){
    public void run(){
      log("loadChildCategories","loading cat path: " + cat);
      String[] result=new String[]{"Cat 1","Cat 2","Cat 3"};
      cb.onSuccess(result);
    }
  }
;
  t.schedule(1000);
}
