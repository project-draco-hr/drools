{
  try {
    createParser(charStream);
    Method ruleName=DRLXParser.class.getMethod(testRuleName);
    Object ruleReturn=ruleName.invoke(parser);
    if (parser.hasErrors()) {
      System.out.println(parser.getErrorMessages());
    }
    return ruleReturn;
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
  return null;
}
