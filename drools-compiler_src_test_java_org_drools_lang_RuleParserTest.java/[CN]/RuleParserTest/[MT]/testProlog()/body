{
  parse("package foo; import com.foo.Bar; import com.foo.Baz;").prolog();
  assertEquals("foo",this.parser.getPackageDescr().getName());
  assertEquals(2,this.parser.getPackageDescr().getImports().size());
  assertEquals("com.foo.Bar",this.parser.getPackageDescr().getImports().get(0));
  assertEquals("com.foo.Baz",this.parser.getPackageDescr().getImports().get(1));
  assertFalse(this.parser.hasErrors());
}
