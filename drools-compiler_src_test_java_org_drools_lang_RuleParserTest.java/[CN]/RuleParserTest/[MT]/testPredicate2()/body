{
  final PatternDescr pattern=new PatternDescr();
  parse("( $var.equals(\"xyz\") )").constraints(pattern);
  assertFalse(this.parser.getErrorMessages().toString(),this.parser.hasErrors());
  final List constraints=pattern.getDescrs();
  assertEquals(1,constraints.size());
  final PredicateDescr predicate=(PredicateDescr)constraints.get(0);
  assertEquals(" $var.equals(\"xyz\") ",predicate.getContent());
}
