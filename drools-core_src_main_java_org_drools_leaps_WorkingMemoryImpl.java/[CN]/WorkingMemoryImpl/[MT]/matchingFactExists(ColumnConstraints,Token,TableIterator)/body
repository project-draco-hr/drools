{
  boolean found=false;
  FactHandleImpl factHandle;
  while (tableIterator.hasNext() && !found) {
    factHandle=(FactHandleImpl)tableIterator.next();
    for (Iterator alphas=constraints.getAlpha(); alphas.hasNext() && !found; ) {
      found=((LiteralConstraint)alphas.next()).isAllowed(factHandle,token,this);
    }
    if (!found) {
      found=constraints.getBeta().isAllowed(factHandle,token,this);
    }
  }
  return found;
}
