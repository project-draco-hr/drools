{
  ArrayList list=new ArrayList();
  Class[] interfaces=c.getInterfaces();
  for (int i=0; i < interfaces.length; i++) {
    list.add(this.getFactTable(interfaces[i]));
  }
  Class bufClass=c;
  while (bufClass != null) {
    list.add(this.getFactTable(bufClass));
    bufClass=bufClass.getSuperclass();
  }
  return list;
}
