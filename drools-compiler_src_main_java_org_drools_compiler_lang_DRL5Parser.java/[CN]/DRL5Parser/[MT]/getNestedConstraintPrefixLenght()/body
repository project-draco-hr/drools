{
  int cursor=0;
  int lastToken=input.LA(++cursor);
  while (true) {
    int nextToken=input.LA(++cursor);
switch (lastToken) {
case DRL5Lexer.ID:
      if (nextToken != DRL5Lexer.DOT && nextToken != DRL5Lexer.SHARP) {
        return -1;
      }
    break;
case DRL5Lexer.DOT:
  if (nextToken == DRL5Lexer.LEFT_PAREN) {
    return cursor;
  }
case DRL5Lexer.SHARP:
if (nextToken != DRL5Lexer.ID) {
  return -1;
}
break;
default :
return -1;
}
lastToken=nextToken;
}
}
