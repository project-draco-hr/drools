{
  WorkingMemoryImpl workingMemory=new WorkingMemoryImpl(new RuleBaseImpl());
  Rule rule=new Rule("test-rule");
  PropagationContext context=new PropagationContextImpl(0,PropagationContext.ASSERTION,null,null);
  MockObjectSource source=new MockObjectSource(15);
  LiteralConstraint constraint=new LiteralConstraint(null,null,null);
  AlphaNode alphaNode=new AlphaNode(2,constraint,false,source);
  MockObjectSink sink=new MockObjectSink();
  alphaNode.addObjectSink(sink);
  FactHandleImpl f0=new FactHandleImpl(0);
  assertLength(0,sink.getRetracted());
  Set memory=(Set)workingMemory.getNodeMemory(alphaNode);
  assertLength(0,memory);
  alphaNode.retractObject(f0,context,workingMemory);
  assertLength(1,sink.getRetracted());
  assertLength(0,memory);
  Object[] list=(Object[])sink.getRetracted().get(0);
  assertSame(f0,list[0]);
}
