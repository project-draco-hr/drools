{
  MockObjectSource source=new MockObjectSource(15);
  AlphaNode alphaNode=new AlphaNode(2,null,source);
  assertEquals(2,alphaNode.getId());
  assertLength(0,source.getObjectSinks());
  alphaNode.attach();
  assertLength(1,source.getObjectSinks());
  assertSame(alphaNode,source.getObjectSinks().get(0));
}
