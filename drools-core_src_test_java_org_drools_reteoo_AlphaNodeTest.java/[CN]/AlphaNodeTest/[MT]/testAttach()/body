{
  final MockObjectSource source=new MockObjectSource(15);
  final AlphaNode alphaNode=new AlphaNode(2,null,source);
  assertEquals(2,alphaNode.getId());
  assertLength(0,source.getObjectSinksAsList());
  alphaNode.attach();
  assertLength(1,source.getObjectSinksAsList());
  assertSame(alphaNode,source.getObjectSinks().getLastObjectSink());
}
