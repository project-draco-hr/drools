{
  try {
    closeElement();
    writeIdent();
    this.w.write("<".concat(qName));
    if (atts != null && atts.getLength() > 0) {
      writeAttributes(atts);
    }
    if (!this.optimizeEmptyElements) {
      this.w.write(">\n");
    }
 else {
      this.openElement=true;
    }
    this.ident+=2;
  }
 catch (  final IOException ex) {
    throw new SAXException(ex);
  }
}
