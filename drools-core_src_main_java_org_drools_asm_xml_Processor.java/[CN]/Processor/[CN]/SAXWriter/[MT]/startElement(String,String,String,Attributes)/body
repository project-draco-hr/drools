{
  try {
    closeElement();
    writeIdent();
    w.write("<".concat(qName));
    if (atts != null && atts.getLength() > 0)     writeAttributes(atts);
    if (!optimizeEmptyElements) {
      w.write(">\n");
    }
 else {
      openElement=true;
    }
    ident+=2;
  }
 catch (  IOException ex) {
    throw new SAXException(ex);
  }
}
