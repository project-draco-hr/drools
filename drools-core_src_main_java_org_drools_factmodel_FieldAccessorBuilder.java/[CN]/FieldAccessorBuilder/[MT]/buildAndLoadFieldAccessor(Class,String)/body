{
  ClassDefinition accClass=new ClassDefinition(clazz.getName() + fieldName.substring(0,1).toUpperCase() + fieldName.substring(1)+ "FA");
  try {
    return Class.forName(accClass.getClassName());
  }
 catch (  ClassNotFoundException e) {
    byte[] serializedClazz=this.buildFieldAccessor(clazz,fieldName);
    Class newClazz=this.loadClass(accClass.getClassName(),serializedClazz);
    return newClazz;
  }
}
