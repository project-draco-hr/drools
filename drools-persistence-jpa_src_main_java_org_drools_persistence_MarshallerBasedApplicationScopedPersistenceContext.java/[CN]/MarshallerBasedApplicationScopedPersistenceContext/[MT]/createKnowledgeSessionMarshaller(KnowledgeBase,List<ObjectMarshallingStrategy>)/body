{
  List<ObjectMarshallingStrategy> strategies=marshallingStrategies;
  if (strategies != null) {
    statefulKnowledgeMarshaller=MarshallerFactory.newMarshaller(kbase,strategies.toArray(new ObjectMarshallingStrategy[strategies.size()]));
  }
 else {
    statefulKnowledgeMarshaller=MarshallerFactory.newMarshaller(kbase);
  }
}
