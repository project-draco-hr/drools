{
  try {
    RuleBaseWrapper.getInstance().getRuleBase().addPackage(pkg);
  }
 catch (  Exception e) {
    throw new RuleTestServiceUnavailableException("Could not load rule package: " + pkg.getName());
  }
  parseTestSuite();
  RuleBaseWrapper.getInstance().getRuleBase().removePackage(pkg.getName());
  return true;
}
