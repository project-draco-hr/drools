{
  try {
    this.lock.lock();
    if (a.length < this.size) {
      a=(Object[])java.lang.reflect.Array.newInstance(a.getClass().getComponentType(),this.size);
    }
    System.arraycopy(this.elements,1,a,0,this.size);
    if (a.length > this.size) {
      a[this.size]=null;
    }
    return a;
  }
  finally {
    this.lock.unlock();
  }
}
