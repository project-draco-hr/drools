{
  final Object element=this.elements[index];
  int hole=index;
  while ((hole * 2) <= this.size) {
    int child=hole * 2;
    if (child != this.size && compare(this.elements[child + 1],this.elements[child]) > 0) {
      child++;
    }
    if (compare(this.elements[child],element) <= 0) {
      break;
    }
    this.elements[hole]=this.elements[child];
    hole=child;
  }
  this.elements[hole]=element;
}
