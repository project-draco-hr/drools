{
  if (clazz.isInstance(baseDescr)) {
    if (this.descrs.isEmpty()) {
      addDescr(baseDescr);
      return;
    }
    for (int i=this.descrs.size() - 1; i >= 0; i--) {
      if (this.descrs.get(i) instanceof FieldConstraintDescr) {
        insertDescr(i - 1,baseDescr);
        return;
      }
    }
  }
}
