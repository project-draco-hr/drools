{
  String DROOLS_RULE_SERVICE_PROVIDER_CLASS="org.drools.jsr94.rules.RuleServiceProviderImpl";
  String DROOLS_RULE_SERVICE_PROVIDER="http://drools.org/";
  RuleServiceProviderManager.registerRuleServiceProvider(DROOLS_RULE_SERVICE_PROVIDER,RuleServiceProviderImpl.class);
  Class.forName(DROOLS_RULE_SERVICE_PROVIDER_CLASS);
  runRuleset(DROOLS_RULE_SERVICE_PROVIDER,"testagent.drl","Gecko");
}
