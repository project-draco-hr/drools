{
  assertFalse(EvaluatorFactory.getEvaluator(Evaluator.OBJECT_TYPE,"==").evaluate("foo","bar"));
  String foo="foo";
  assertTrue(EvaluatorFactory.getEvaluator(Evaluator.OBJECT_TYPE,"==").evaluate(foo,foo));
  assertTrue(EvaluatorFactory.getEvaluator(Evaluator.OBJECT_TYPE,"!=").evaluate("foo","bar"));
  List list=new ArrayList();
  list.add("foo");
  assertTrue(EvaluatorFactory.getEvaluator(Evaluator.OBJECT_TYPE,"contains").evaluate("foo",list));
  assertFalse(EvaluatorFactory.getEvaluator(Evaluator.OBJECT_TYPE,"contains").evaluate("bar",list));
}
