{
  Entry[] entries=(Entry[])tms.getEqualityKeyMap().toArray();
  for (  Entry e : entries) {
    EqualityKey key=(EqualityKey)((ObjectEntry)e).getValue();
    if (key.getFactHandle().getObject().toString().equals(id)) {
      return key;
    }
  }
  fail("Unable to find Equality Key" + id);
  return null;
}
