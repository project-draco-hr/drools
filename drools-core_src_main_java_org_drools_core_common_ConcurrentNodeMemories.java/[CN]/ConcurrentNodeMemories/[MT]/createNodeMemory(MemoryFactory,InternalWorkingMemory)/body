{
  try {
    this.lock.lock();
    Memory memory=this.memories.get(node.getId());
    if (memory == null) {
      memory=node.createMemory(this.rulebase.getConfiguration(),wm);
      if (!this.memories.compareAndSet(node.getId(),null,memory)) {
        memory=this.memories.get(node.getId());
      }
    }
    return memory;
  }
  finally {
    this.lock.unlock();
  }
}
