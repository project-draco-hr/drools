{
  PomModel pomModel=new PomModel();
  MavenProject mavenProject=parseMavenPom(pomStream);
  pomModel.setReleaseId(new ReleaseIdImpl(mavenProject.getGroupId(),mavenProject.getArtifactId(),mavenProject.getVersion()));
  try {
    MavenProject parentProject=mavenProject.getParent();
    if (parentProject != null) {
      pomModel.setParentReleaseId(new ReleaseIdImpl(parentProject.getGroupId(),parentProject.getArtifactId(),parentProject.getVersion()));
    }
  }
 catch (  Exception e) {
  }
  for (  Dependency dep : mavenProject.getDependencies()) {
    String scope=dep.getScope();
    if ("provided".equals(scope) || "test".equals(scope)) {
      continue;
    }
    pomModel.addDependency(new ReleaseIdImpl(dep.getGroupId(),dep.getArtifactId(),dep.getVersion()));
  }
  return pomModel;
}
