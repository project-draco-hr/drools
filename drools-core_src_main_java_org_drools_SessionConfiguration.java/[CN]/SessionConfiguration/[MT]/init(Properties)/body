{
  this.immutable=false;
  this.chainedProperties=new ChainedProperties("session.conf");
  if (properties != null) {
    this.chainedProperties.addProperties(properties);
  }
  setKeepReference(Boolean.valueOf(this.chainedProperties.getProperty("drools.keepReference","true")).booleanValue());
  setClockType(ClockType.resolveClockType(this.chainedProperties.getProperty("drools.clockType","realtime")));
}
