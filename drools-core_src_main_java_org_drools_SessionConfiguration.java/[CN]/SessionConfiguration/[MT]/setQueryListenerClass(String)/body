{
  checkCanChange();
  try {
    Class<? extends InternalViewChangedEventListener> listenerClass=(Class<? extends InternalViewChangedEventListener>)Class.forName(property,true,this.classLoader);
    setQueryListenerClass(listenerClass);
  }
 catch (  ClassNotFoundException e) {
    throw new RuntimeDroolsException("Unable to find query listener class : '" + property + "'",e);
  }
}
