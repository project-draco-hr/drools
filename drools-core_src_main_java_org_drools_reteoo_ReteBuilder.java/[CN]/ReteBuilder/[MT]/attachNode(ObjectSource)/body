{
  ObjectSource node=(ObjectSource)this.attachedNodes.get(candidate);
  if (node == null) {
    if (this.workingMemories.length == 0) {
      candidate.attach();
    }
 else {
      candidate.attach(workingMemories,null);
    }
    this.attachedNodes.put(candidate,candidate);
    node=candidate;
  }
 else {
    this.id--;
  }
  return node;
}
