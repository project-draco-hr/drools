{
  try {
    long lastMod=grabLastMod();
    if (lastMod == 0) {
      if (cacheFileExists())       return fromCache();
    }
    if (lastMod > 0 && lastMod > lastRead) {
      if (CACHE_DIR != null && url.getProtocol().equals("http") || url.getProtocol().equals("https")) {
        cacheStream();
      }
    }
    this.lastRead=lastMod;
    return grabStream();
  }
 catch (  IOException e) {
    if (cacheFileExists()) {
      return fromCache();
    }
 else {
      throw e;
    }
  }
}
