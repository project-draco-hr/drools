{
  try {
    File fi=getCacheFile();
    if (fi.exists())     fi.delete();
    FileOutputStream fout=new FileOutputStream(fi);
    InputStream in=grabStream();
    byte[] buffer=new byte[DEFAULT_BUFFER_SIZE];
    int n;
    while (-1 != (n=in.read(buffer))) {
      fout.write(buffer,0,n);
    }
    fout.flush();
    fout.close();
    in.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
