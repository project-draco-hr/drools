{
  And temp=null;
  if (!and.getChildren().isEmpty()) {
    temp=(And)and.clone();
    and.getChildren().clear();
  }
  final Column column=new Column(0,new ClassObjectType(InitialFact.class));
  and.addChild(column);
  if (temp != null) {
    and.getChildren().addAll(temp.getChildren());
  }
  this.currentOffsetAdjustment=1;
}
