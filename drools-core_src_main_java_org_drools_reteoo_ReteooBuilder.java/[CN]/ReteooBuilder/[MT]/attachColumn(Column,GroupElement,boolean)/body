{
  column.adjustOffset(this.currentOffsetAdjustment);
  if (column.getDeclaration() != null) {
    final Declaration declaration=column.getDeclaration();
    this.declarations.put(declaration.getIdentifier(),declaration);
  }
  final List predicates=attachAlphaNodes(column,removeIdentities);
  BetaNodeConstraints binder;
  if (!predicates.isEmpty()) {
    binder=new BetaNodeConstraints((FieldConstraint[])predicates.toArray(new FieldConstraint[predicates.size()]));
  }
 else {
    binder=new BetaNodeConstraints();
  }
  return binder;
}
