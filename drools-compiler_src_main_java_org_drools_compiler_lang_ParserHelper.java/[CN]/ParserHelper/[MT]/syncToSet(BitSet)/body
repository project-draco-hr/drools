{
  int mark=-1;
  try {
    mark=input.mark();
    while (!memberOfFollowSet(follow)) {
      if (input.LA(1) == Token.EOF) {
        input.rewind();
        mark=-1;
        return;
      }
      reportError(new MismatchedSetException(follow,input));
      input.consume();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (mark != -1) {
      input.release(mark);
    }
  }
}
