{
  if (input.LA(1) == DRLLexer.ID) {
    Token id=match(input,DRLLexer.ID,null,null,DroolsEditorType.IDENTIFIER);
    if (state.failed)     return null;
    return id.getText();
  }
 else   if (input.LA(1) == DRLLexer.STRING) {
    Token id=match(input,DRLLexer.STRING,null,null,DroolsEditorType.IDENTIFIER);
    if (state.failed)     return null;
    return StringUtils.unescapeJava(safeStripStringDelimiters(id.getText()));
  }
 else {
    throw new MismatchedTokenException(DRLLexer.ID,input);
  }
}
