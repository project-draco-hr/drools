{
  AgendaGroupImpl group=(AgendaGroupImpl)getNextFocus();
  if (group == null) {
    return false;
  }
  ActivationQueue queue=(ActivationQueue)group.getPriorityQueue().get();
  Activation item=queue.remove();
  if (filter == null || filter.accept(item)) {
    fireActivation(item);
  }
  return true;
}
