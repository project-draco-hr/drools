{
  if (p1file.exists()) {
    try {
      Thread.sleep(1000);
    }
 catch (    Exception e) {
      throw new RuntimeException("Unable to sleep");
    }
  }
  FileOutputStream out=new FileOutputStream(p1file);
  try {
    DroolsStreamUtils.streamOut(out,pkg);
  }
  finally {
    out.close();
  }
}
