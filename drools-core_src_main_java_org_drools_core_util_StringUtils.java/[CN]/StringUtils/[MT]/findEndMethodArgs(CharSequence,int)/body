{
  boolean isQuoted=false;
  int nestingLevel=0;
  for (int i=startMethodArgs; i < string.length(); i++) {
switch (string.charAt(i)) {
case '(':
      if (!isQuoted) {
        nestingLevel++;
      }
    break;
case ')':
  if (!isQuoted) {
    nestingLevel--;
    if (nestingLevel == 0) {
      return i;
    }
  }
break;
case '"':
case '\'':
isQuoted=!isQuoted;
break;
}
}
return -1;
}
