{
  setupJoinNode();
  try {
    test().left().insert(a0,a1).right().insert(b0,b1).result().run();
    test().left().delete(a0,a1).result().delete(t(a0,b0)).run();
    fail("Should not reach here");
  }
 catch (  AssertionError e) {
    assertTrue(e.getMessage().contains("delete 0 expected"));
  }
}
