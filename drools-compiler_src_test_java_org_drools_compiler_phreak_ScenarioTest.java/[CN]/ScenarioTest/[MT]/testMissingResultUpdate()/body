{
  setupJoinNode();
  try {
    test().left().insert(a0,a1).right().insert(b0,b1).result().run();
    test().left().update(a0,a1).result().update(t(a0,b1)).run();
    fail("Should not reach here");
  }
 catch (  AssertionError e) {
    assertTrue(e.getMessage().contains("Update excpected more"));
  }
}
