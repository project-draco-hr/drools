{
  List[] usedIdentifiers=null;
  try {
    usedIdentifiers=this.analyzer.analyzeBlock(text,new Set[]{context.getDeclarations().keySet(),context.getPkg().getGlobals().keySet()});
  }
 catch (  final Exception e) {
    context.getErrors().add(new RuleError(context.getRule(),descr,null,"Unable to determine the used declarations"));
  }
  return usedIdentifiers;
}
