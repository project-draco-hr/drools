{
  WorkItem workItem=getWorkItem(workItemId);
  if (workItem != null) {
    WorkItemHandler handler=(WorkItemHandler)this.workItemHandlers.get(workItem.getName());
    if (handler != null) {
      handler.executeWorkItem(workItem,this);
    }
 else {
      throwWorkItemNotFoundException(workItem);
    }
  }
}
