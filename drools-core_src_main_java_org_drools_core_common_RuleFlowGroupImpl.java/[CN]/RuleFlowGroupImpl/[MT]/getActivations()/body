{
synchronized (list) {
    Activation[] activations=new Activation[list.size()];
    FastIterator it=list.fastIterator();
    int i=0;
    for (ActivationNode entry=list.getFirst(); entry != null; entry=(ActivationNode)it.next(entry)) {
      activations[i++]=entry.getActivation();
    }
    return activations;
  }
}
