{
  String[] kbases=kproject.getKBases().keySet().toArray(new String[kproject.getKBases().size()]);
  Arrays.sort(kbases);
  try {
    for (    String kbaseQName : kbases) {
      writeKBaseProducer(kproject.getKBases().get(kbaseQName),kproject);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
