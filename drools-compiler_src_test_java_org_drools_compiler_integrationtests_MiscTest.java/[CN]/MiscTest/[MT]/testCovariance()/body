{
  String str="import MiscTest.*\n" + "rule x\n" + "when\n"+ "   $b : ClassB( )\n"+ "   $a : ClassA( b.id == $b.id )\n"+ "then\n"+ "end\n";
  KnowledgeBase kbase=loadKnowledgeBaseFromString(str);
  StatefulKnowledgeSession ksession=kbase.newStatefulKnowledgeSession();
  ClassA a=new ClassA();
  ClassB b=new ClassB();
  a.setB(b);
  ksession.insert(a);
  ksession.insert(b);
  assertEquals(1,ksession.fireAllRules());
}
