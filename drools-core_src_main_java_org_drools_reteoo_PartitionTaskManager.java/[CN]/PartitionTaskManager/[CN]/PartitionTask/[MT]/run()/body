{
  if (checkAndSetRunning()) {
    return;
  }
  while (!shutdown.get()) {
    try {
      if (Thread.currentThread().isInterrupted()) {
        cancel();
        break;
      }
      Action action=queue.take();
      action.execute(workingMemory);
    }
 catch (    InterruptedException e) {
      cancel();
    }
  }
}
