{
  boolean result=true;
  if (isRunning()) {
    this.task.shutdown();
    this.pool.shutdown();
    try {
      while (!this.pool.awaitTermination(10,TimeUnit.SECONDS)) {
        ;
      }
      result=this.pool.isTerminated();
    }
 catch (    InterruptedException e) {
      result=false;
    }
    this.pool=null;
  }
  return result;
}
