{
  clazz=(Class<?>)is.readObject();
  fieldName=(String)is.readObject();
  if (is instanceof DroolsObjectInput) {
    DroolsObjectInput droolsInput=(DroolsObjectInput)is;
    reader=droolsInput.getExtractorFactory().getReader(clazz,fieldName,droolsInput.getClassLoader());
  }
 else   reader=ClassFieldAccessorCache.getInstance().getReader(clazz,fieldName,getClass().getClassLoader());
}
