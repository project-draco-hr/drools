{
  DroolsMVELFactory factory=(DroolsMVELFactory)context;
  factory.setContext(tuple,null,null,workingMemory,null);
  Package pkg=workingMemory.getRuleBase().getPackage("MAIN");
  if (pkg != null) {
    MVELDialectData data=(MVELDialectData)pkg.getDialectDatas().getDialectData("mvel");
    factory.setNextFactory(data.getFunctionFactory());
  }
  final Boolean result=(Boolean)MVEL.executeExpression(this.expr,new Object(),factory);
  return result.booleanValue();
}
