{
  boolean isDirty=false;
  for (Iterator it=this.dialects.values().iterator(); it.hasNext(); ) {
    DialectData data=(DialectData)it.next();
    if (data.isDirty()) {
      isDirty=true;
      break;
    }
  }
  if (isDirty) {
    this.classLoader=new CompositePackageClassLoader(this.parentClassLoader);
    for (Iterator it=this.dialects.values().iterator(); it.hasNext(); ) {
      DialectData data=(DialectData)it.next();
      data.reload();
    }
  }
}
