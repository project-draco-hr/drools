{
  final StringBuffer buf=new StringBuffer();
  buf.append("( ");
  for (Iterator it=this.restrictions.iterator(); it.hasNext(); ) {
    buf.append(it.next().toString());
    if (it.hasNext()) {
      buf.append(this.connective.toString());
    }
  }
  buf.append("  )");
  return buf.toString();
}
