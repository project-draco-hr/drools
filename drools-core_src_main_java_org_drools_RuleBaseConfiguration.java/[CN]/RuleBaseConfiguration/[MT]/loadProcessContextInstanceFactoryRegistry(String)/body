{
  String content=ConfFileUtils.URLContentsToString(ConfFileUtils.getURL(factoryLocation,null,RuleBaseConfiguration.class));
  Map<Class<? extends Context>,ContextInstanceFactory> map=(Map<Class<? extends Context>,ContextInstanceFactory>)MVEL.eval(content,new HashMap());
  if (map != null) {
    for (    Entry<Class<? extends Context>,ContextInstanceFactory> entry : map.entrySet()) {
      this.processContextInstanceFactoryRegistry.register(entry.getKey(),entry.getValue());
    }
  }
}
