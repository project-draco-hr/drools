{
  final CollectDescr collectDescr=(CollectDescr)descr;
  final PatternBuilder patternBuilder=(PatternBuilder)context.getDialect().getBuilder(PatternDescr.class);
  final Pattern sourcePattern=patternBuilder.build(context,collectDescr.getSourcePattern());
  if (sourcePattern == null) {
    return null;
  }
  final Pattern resultPattern=patternBuilder.build(context,collectDescr.getResultPattern());
  final String className="collect" + context.getNextId();
  collectDescr.setClassMethodName(className);
  final Collect collect=new Collect(sourcePattern,resultPattern);
  return collect;
}
