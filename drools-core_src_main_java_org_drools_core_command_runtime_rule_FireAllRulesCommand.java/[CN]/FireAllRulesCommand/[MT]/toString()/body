{
  if (max != -1 && getAgendaFilter() != null) {
    return "session.fireAllRules( " + agendaFilter + ", "+ max+ " );";
  }
 else   if (max != -1) {
    return "session.fireAllRules( " + max + " );";
  }
 else   if (agendaFilter != null) {
    return "session.fireAllRules( " + agendaFilter + " );";
  }
 else {
    return "session.fireAllRules();";
  }
}
