{
  if (this.firstNode == null) {
    return null;
  }
  final TupleSinkNode node=this.firstNode;
  this.firstNode=node.getNextTupleSinkNode();
  node.setNextTupleSinkNode(null);
  if (this.firstNode != null) {
    this.firstNode.setPreviousTupleSinkNode(null);
  }
 else {
    this.lastNode=null;
  }
  this.size--;
  return node;
}
