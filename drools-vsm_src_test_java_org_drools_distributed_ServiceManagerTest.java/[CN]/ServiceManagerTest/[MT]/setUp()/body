{
  this.server=new ServiceManagerServer();
  Thread thread=new Thread(server);
  thread.start();
  Thread.sleep(500);
  this.client=new ServiceManagerClient("client 1",new MessageHandler(SystemEventListenerFactory.getSystemEventListener()));
  NioSocketConnector connector=new NioSocketConnector();
  SocketAddress address=new InetSocketAddress("127.0.0.1",9123);
  client.connect(connector,address);
}
