{
  final Object[] objArray=new Object[1];
  final TestBean obj=new TestBean();
  obj.setBlah(false);
  obj.setSomething("no");
  obj.setObjArray(objArray);
  final ClassFieldReader ext=store.getReader(TestBean.class,"blah",getClass().getClassLoader());
  assertEquals(false,((Boolean)ext.getValue(null,obj)).booleanValue());
  final ClassFieldReader ext2=store.getReader(TestBean.class,"fooBar",getClass().getClassLoader());
  assertEquals("fooBar",ext2.getValue(null,obj));
  final ClassFieldReader ext3=store.getReader(TestBean.class,"objArray",getClass().getClassLoader());
  assertEquals(objArray,ext3.getValue(null,obj));
}
