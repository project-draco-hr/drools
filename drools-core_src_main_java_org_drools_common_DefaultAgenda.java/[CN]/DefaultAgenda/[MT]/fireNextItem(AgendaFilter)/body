{
  final InternalAgendaGroup group=(InternalAgendaGroup)getNextFocus();
  if (group == null) {
    return false;
  }
  final AgendaItem item=(AgendaItem)group.getNext();
  if (item == null) {
    return false;
  }
  if (filter == null || filter.accept(item)) {
    fireActivation(item);
  }
  return true;
}
