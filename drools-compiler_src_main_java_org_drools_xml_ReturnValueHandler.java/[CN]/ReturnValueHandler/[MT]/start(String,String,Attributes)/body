{
  this.xmlPackageReader.startConfiguration(localName,attrs);
  final String fieldName=attrs.getValue("field-name");
  if (fieldName == null || fieldName.trim().equals("")) {
    throw new SAXParseException("<return-value> requires a 'field-name' attribute",this.xmlPackageReader.getLocator());
  }
  final String evaluator=attrs.getValue("evaluator");
  if (evaluator == null || evaluator.trim().equals("")) {
    throw new SAXParseException("<return-value> requires an 'evaluator' attribute",this.xmlPackageReader.getLocator());
  }
  final ReturnValueDescr returnValueDescr=new ReturnValueDescr(fieldName,evaluator);
  return returnValueDescr;
}
