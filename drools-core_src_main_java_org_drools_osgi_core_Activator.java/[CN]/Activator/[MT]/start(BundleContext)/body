{
  logger.info("registering core  services");
  this.resourceReg=bc.registerService(new String[]{ResourceFactoryService.class.getName(),Service.class.getName()},new ResourceFactoryServiceImpl(),new Hashtable());
  this.kbaseReg=bc.registerService(new String[]{KnowledgeBaseFactoryService.class.getName(),Service.class.getName()},new KnowledgeBaseFactoryServiceImpl(),new Hashtable());
  this.marshallerProviderReg=bc.registerService(new String[]{MarshallerProvider.class.getName(),Service.class.getName()},new MarshallerProviderImpl(),new Hashtable());
  logger.info("core services registered");
}
