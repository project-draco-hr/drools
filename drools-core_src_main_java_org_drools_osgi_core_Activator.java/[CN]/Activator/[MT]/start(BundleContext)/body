{
  System.out.println("registering core  services");
  this.resourceReg=bc.registerService(ResourceProvider.class.getName(),new ResourceProviderImpl(),new Hashtable());
  this.kbaseReg=bc.registerService(KnowledgeBaseProvider.class.getName(),new KnowledgeBaseProviderImpl(),new Hashtable());
  System.out.println("core services registered");
}
