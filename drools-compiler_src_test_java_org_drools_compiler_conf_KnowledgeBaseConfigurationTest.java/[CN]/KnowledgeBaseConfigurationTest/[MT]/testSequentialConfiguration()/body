{
  config.setOption(SequentialOption.YES);
  assertEquals(SequentialOption.YES,config.getOption(SequentialOption.class));
  assertEquals("true",config.getProperty(SequentialOption.PROPERTY_NAME));
  config.setProperty(SequentialOption.PROPERTY_NAME,"false");
  assertEquals(SequentialOption.NO,config.getOption(SequentialOption.class));
  assertEquals("false",config.getProperty(SequentialOption.PROPERTY_NAME));
}
