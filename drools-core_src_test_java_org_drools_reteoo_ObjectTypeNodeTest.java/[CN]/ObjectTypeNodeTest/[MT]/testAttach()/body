{
  Rete source=new Rete();
  ObjectType objectType=new ClassObjectType(String.class);
  ObjectTypeNode objectTypeNode=new ObjectTypeNode(1,objectType,source);
  assertEquals(1,objectTypeNode.getId());
  assertLength(0,source.getObjectTypeNodes());
  objectTypeNode.attach();
  assertLength(1,source.getObjectTypeNodes());
  assertSame(objectTypeNode,source.getObjectTypeNode(objectType));
}
