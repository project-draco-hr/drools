{
  MockObjectSource source=new MockObjectSource(15);
  ObjectTypeNode objectTypeNode=new ObjectTypeNode(1,new ClassObjectType(String.class),source);
  assertEquals(1,objectTypeNode.getId());
  assertLength(0,source.getObjectSinks());
  objectTypeNode.attach();
  assertLength(1,source.getObjectSinks());
  assertSame(objectTypeNode,source.getObjectSinks().get(0));
}
