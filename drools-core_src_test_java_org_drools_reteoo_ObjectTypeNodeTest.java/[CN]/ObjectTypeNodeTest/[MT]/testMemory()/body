{
  final ReteooWorkingMemory workingMemory=new ReteooWorkingMemory(1,(ReteooRuleBase)RuleBaseFactory.newRuleBase());
  final ObjectTypeNode objectTypeNode=new ObjectTypeNode(1,new ClassObjectType(String.class),new Rete(),3);
  final FactHashTable memory=(FactHashTable)workingMemory.getNodeMemory(objectTypeNode);
  assertNotNull(memory);
}
