{
  ReteooRuleBase ruleBase=(ReteooRuleBase)RuleBaseFactory.newRuleBase();
  IdGenerator idGenerator=ruleBase.getReteooBuilder().getIdGenerator();
  final ReteooWorkingMemory workingMemory=(ReteooWorkingMemory)ruleBase.newStatefulSession();
  final ObjectTypeNode objectTypeNode=new ObjectTypeNode(idGenerator.getNextId(),new ClassObjectType(String.class),buildContext);
  final FactHashTable memory=(FactHashTable)workingMemory.getNodeMemory(objectTypeNode);
  assertNotNull(memory);
}
