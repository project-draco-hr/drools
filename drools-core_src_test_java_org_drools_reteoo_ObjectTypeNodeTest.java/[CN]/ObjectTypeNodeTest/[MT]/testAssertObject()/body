{
  PropagationContext context=new PropagationContextImpl(0,PropagationContext.ASSERTION,null,null);
  WorkingMemoryImpl workingMemory=new WorkingMemoryImpl(new RuleBaseImpl());
  Rete source=new Rete();
  ObjectTypeNode objectTypeNode=new ObjectTypeNode(1,new ClassObjectType(String.class),source);
  MockObjectSink sink=new MockObjectSink();
  objectTypeNode.addObjectSink(sink);
  Object string1="cheese";
  FactHandleImpl handle1=new FactHandleImpl(1);
  workingMemory.putObject(handle1,string1);
  objectTypeNode.assertObject(handle1,context,workingMemory);
  List asserted=sink.getAsserted();
  assertLength(1,asserted);
  assertSame(string1,workingMemory.getObject((FactHandleImpl)((Object[])asserted.get(0))[0]));
  PrimitiveLongMap memory=(PrimitiveLongMap)workingMemory.getNodeMemory(objectTypeNode);
  assertEquals(1,memory.size());
  assertSame(handle1,memory.get(handle1.getId()));
}
