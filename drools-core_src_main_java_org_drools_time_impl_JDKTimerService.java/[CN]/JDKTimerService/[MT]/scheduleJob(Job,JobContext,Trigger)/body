{
  JDKJobHandle jobHandle=new JDKJobHandle();
  Date date=trigger.nextFireTime();
  if (date != null) {
    JDKCallableJob callableJob=new JDKCallableJob(job,ctx,trigger,jobHandle,this.scheduler);
    ScheduledFuture future=schedule(date,callableJob,this.scheduler);
    jobHandle.setFuture(future);
    return jobHandle;
  }
 else {
    return null;
  }
}
