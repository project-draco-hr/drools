{
  final String KEY="REPLACE";
  final int SIZE=100;
  final Package[] pkgs=new Package[SIZE];
  final String DRL="package org.drools\n" + "    no-loop true\n" + "    dialect \"java\"\n"+ "rule \"" + KEY + "\"\n"+ "salience 1\n"+ "when\n"+ "    $fact:Cheese(price == "+ KEY+ ", oldPrice not in (11,5))\n"+ "then\n"+ "    //$fact.excludeProduct("+ KEY+ ", 1, null, null);\n"+ "end\n";
  System.out.print("Building " + pkgs.length + " packages");
  for (int i=0; i < pkgs.length; i++) {
    pkgs[i]=getPackage(DRL.replaceAll(KEY,Integer.toString(i)));
    System.out.print(".");
  }
  System.out.println();
  return pkgs;
}
