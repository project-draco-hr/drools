{
  if (this.cache == Collections.EMPTY_MAP) {
    this.cache=new HashMap<String,Evaluator>();
  }
  String key=isNegated + ":" + parameterText;
  Evaluator eval=this.cache.get(key);
  if (eval == null) {
    if (isNegated) {
      eval=new NotAfterEvaluator(type,isNegated,parameterText);
    }
 else {
      eval=new AfterEvaluator(type,isNegated,parameterText);
    }
    this.cache.put(key,eval);
  }
  return eval;
}
