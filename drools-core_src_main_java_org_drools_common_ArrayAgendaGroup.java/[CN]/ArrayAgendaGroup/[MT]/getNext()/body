{
  Activation activation=null;
  while (this.index <= lastIndex) {
    LinkedList<LinkedListEntry<Activation>> list=this.array[this.index];
    if (list != null) {
      activation=list.removeFirst().getObject();
      if (list.isEmpty()) {
        this.array[this.index++]=null;
      }
      this.size--;
      break;
    }
    this.index++;
  }
  return activation;
}
