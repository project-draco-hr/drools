{
  SimpleLogicalDependency ld=(SimpleLogicalDependency)((LinkedListEntry)node).getObject();
  boolean neg=ld.getValue() != null && MODE.NEGATIVE.getId().equals(ld.getValue().toString());
  if (neg) {
    super.addLast(node);
    negCounter++;
  }
 else {
    super.addFirst(node);
    ld.setValue(MODE.POSITIVE.getId());
    posCounter++;
  }
}
