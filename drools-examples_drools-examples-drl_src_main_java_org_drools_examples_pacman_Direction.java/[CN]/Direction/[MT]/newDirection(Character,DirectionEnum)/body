{
switch (directionEnum) {
case LEFT:
{
      return new Direction(character,-1,0);
    }
case RIGHT:
{
    return new Direction(character,1,0);
  }
case UP:
{
  return new Direction(character,0,1);
}
case DOWN:
{
return new Direction(character,0,-1);
}
default :
{
}
}
throw new IllegalArgumentException("Direction must be a valid DirectionEnum");
}
