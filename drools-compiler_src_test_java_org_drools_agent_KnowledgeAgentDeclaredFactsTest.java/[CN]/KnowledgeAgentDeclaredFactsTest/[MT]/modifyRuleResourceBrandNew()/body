{
  String ruleString="package test; \n" + "declare TestFact \n" + "  message : String \n"+ "end \n"+ " \n "+ "rule test2 \n"+ "  when \n"+ "    $m : TestFact( message == \"test2\") \n"+ "  then \n"+ "    System.out.println(\"********** FOUND \" + $m.getMessage()); \n"+ "    $m.setMessage(\"Echo:\" + $m.getMessage()); \n"+ "end \n";
  res=fileManager.write("rule.drl",ruleString);
}
