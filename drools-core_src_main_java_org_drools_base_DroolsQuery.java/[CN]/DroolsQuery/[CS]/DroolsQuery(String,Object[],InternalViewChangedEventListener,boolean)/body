{
  super(params);
  this.name=name;
  this.resultsCollector=resultsCollector;
  this.open=open;
  if (params != null) {
    vars=new Variable[params.length];
    for (int i=0; i < params.length; i++) {
      if (params[i] == Variable.variable) {
        vars[i]=new Variable(getElements(),i);
        params[i]=null;
      }
    }
  }
}
