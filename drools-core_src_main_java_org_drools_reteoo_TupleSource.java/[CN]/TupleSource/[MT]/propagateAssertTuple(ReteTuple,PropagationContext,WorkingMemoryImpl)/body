{
  if (!this.isAttachingNewRule) {
    for (int i=0, size=this.tupleSinks.size(); i < size; i++) {
      ((TupleSink)this.tupleSinks.get(i)).assertTuple(tuple,context,workingMemory);
    }
  }
 else {
    ((TupleSink)this.tupleSinks.get(this.tupleSinks.size() - 1)).assertTuple(tuple,context,workingMemory);
  }
}
