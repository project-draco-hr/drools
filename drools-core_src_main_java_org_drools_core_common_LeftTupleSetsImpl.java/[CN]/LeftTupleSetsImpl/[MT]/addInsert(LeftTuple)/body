{
  if (leftTuple.getStagedType() == LeftTuple.UPDATE) {
    return false;
  }
  leftTuple.setStagedType(LeftTuple.INSERT);
  if (insertFirst == null) {
    insertFirst=leftTuple;
  }
 else {
    leftTuple.setStagedNext(insertFirst);
    insertFirst.setStagePrevious(leftTuple);
    insertFirst=leftTuple;
  }
  return (insertSize++ == 0);
}
