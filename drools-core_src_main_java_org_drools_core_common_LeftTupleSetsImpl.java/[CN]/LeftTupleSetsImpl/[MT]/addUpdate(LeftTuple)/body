{
  if (leftTuple.getStagedType() == LeftTuple.INSERT) {
    return false;
  }
  leftTuple.setStagedType(LeftTuple.UPDATE);
  if (updateFirst == null) {
    updateFirst=leftTuple;
    return true;
  }
  leftTuple.setStagedNext(updateFirst);
  updateFirst.setStagePrevious(leftTuple);
  updateFirst=leftTuple;
  return false;
}
