{
  Method[] methods=clazz.getDeclaredMethods();
  for (int i=0; i < methods.length; i++) {
    Method aMethod=methods[i];
    int modifiers=methods[i].getModifiers();
    String methodName=aMethod.getName();
    if (isNotGetterOrSetter(methodName) && isReasonableMethod(clazz,methodName) && Modifier.isPublic(modifiers)) {
      Class<?>[] listParam=aMethod.getParameterTypes();
      MethodInfo info=new MethodInfo(methodName,solveParams(listParam));
      this.methods.add(info);
    }
  }
}
