{
  int c=keyCount;
  RightTupleList[] tab=table;
  int index=hashCode & (tab.length - 1);
  RightTupleList first=tab[index];
  RightTupleList e=first;
  while (e != null) {
    if (e.matches(object,hashCode)) {
      return e;
    }
    e=(RightTupleList)e.next;
  }
  if (e == null) {
    if (c++ > threshold)     rehash();
    ++modCount;
    e=new RightTupleList(this.index,hashCode,first);
    tab[index]=e;
    keyCount=c;
  }
  return e;
}
