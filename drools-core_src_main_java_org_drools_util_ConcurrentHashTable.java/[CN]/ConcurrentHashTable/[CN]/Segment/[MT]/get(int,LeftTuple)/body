{
  lock();
  try {
    RightTupleList[] tab=table;
    int index=hashCode & (tab.length - 1);
    RightTupleList first=tab[index];
    RightTupleList entry=first;
    while (entry != null) {
      if (entry.matches(tuple,hashCode)) {
        return entry;
      }
      entry=(RightTupleList)entry.getNext();
    }
    return entry;
  }
  finally {
    unlock();
  }
}
