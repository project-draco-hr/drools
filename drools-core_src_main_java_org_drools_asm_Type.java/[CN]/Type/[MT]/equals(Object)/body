{
  if (this == o) {
    return true;
  }
  if (o == null || !(o instanceof Type)) {
    return false;
  }
  final Type t=(Type)o;
  if (this.sort != t.sort) {
    return false;
  }
  if (this.sort == Type.OBJECT || this.sort == Type.ARRAY) {
    if (this.len != t.len) {
      return false;
    }
    for (int i=this.off, j=t.off, end=i + this.len; i < end; i++, j++) {
      if (this.buf[i] != t.buf[j]) {
        return false;
      }
    }
  }
  return true;
}
