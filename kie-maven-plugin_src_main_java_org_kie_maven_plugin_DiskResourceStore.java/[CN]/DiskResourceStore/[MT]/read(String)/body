{
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(getFilePath(pResourceName));
    return readBytesFromInputStream(fis);
  }
 catch (  FileNotFoundException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
