{
  final TokenStack stack=new TokenStack();
  assertTrue(stack.empty());
  final Object object=new Object();
  final Token token1=new Token(null,new FactHandleImpl(1,object),null);
  stack.push(token1);
  final Token token2=new Token(null,new FactHandleImpl(2,object),null);
  stack.push(token2);
  final Token token10=new Token(null,new FactHandleImpl(10,object),null);
  stack.push(token10);
  final Token token8=new Token(null,new FactHandleImpl(8,object),null);
  stack.push(token8);
  final Token token6=new Token(null,new FactHandleImpl(6,object),null);
  stack.push(token6);
  final Token token3=new Token(null,new FactHandleImpl(3,object),null);
  stack.push(token3);
  final Token token4=new Token(null,new FactHandleImpl(4,object),null);
  stack.push(token4);
  assertEquals(token4,stack.peek());
  stack.pop();
  assertEquals(token3,stack.peek());
}
