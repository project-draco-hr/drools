{
  jitted=true;
  try {
    if (TEST_JITTING) {
      executeJitting(object,workingMemory,leftTuple);
    }
 else {
      ExecutorFactory.getExecutor().execute(new Runnable(){
        public void run(){
          executeJitting(object,workingMemory,leftTuple);
        }
      }
);
    }
  }
 catch (  Throwable t) {
    throw new RuntimeException("Exception jitting: " + expression,t);
  }
}
