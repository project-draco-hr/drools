{
  assertTrue(this.pendingActivationSevenBlockingFacts.containsBlockingFacts());
  assertFalse(this.pendingActivationNoBlockingFacts.containsBlockingFacts());
  this.pendingActivationNoBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationNoBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationNoBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  assertTrue(this.pendingActivationSevenBlockingFacts.containsBlockingFacts());
  assertFalse(this.pendingActivationNoBlockingFacts.containsBlockingFacts());
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  this.pendingActivationSevenBlockingFacts.decrementBlockingFactsCount();
  assertFalse(this.pendingActivationSevenBlockingFacts.containsBlockingFacts());
}
