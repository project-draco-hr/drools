{
  try {
    DSLTokenizedMappingFile file=new DSLTokenizedMappingFile();
    file.parseAndLoad(dsl);
    if (this.dslFiles == null) {
      this.dslFiles=new ArrayList<DSLTokenizedMappingFile>();
    }
    this.dslFiles.add(file);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
