{
  ClassFieldAccessorCache cache=ClassFieldAccessorCache.getInstance();
  ClassDefinition cd=type.getTypeClassDef();
  for (  FieldDefinition attrDef : cd.getFieldsDefinitions()) {
    ClassFieldReader reader=cache.getReader(cd.getDefinedClass(),attrDef.getName(),this.pkg.getDialectDatas().getClassLoader());
    ClassFieldWriter writer=cache.getWriter(cd.getDefinedClass(),attrDef.getName(),this.pkg.getDialectDatas().getClassLoader());
    ClassFieldAccessor accessor=new ClassFieldAccessor(reader,writer);
    attrDef.setFieldAccessor(accessor);
  }
}
