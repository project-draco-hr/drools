{
  try {
    PackageRegistry reg=this.pkgRegistryMap.get(typeDescr.getNamespace());
    if (reg != null) {
      String availableName=typeDescr.getType().getFullName();
      Class<?> resolvedType=reg.getTypeResolver().resolveType(availableName);
      if (resolvedType != null && typeDescr.getFields().size() > 1) {
        this.results.add(new TypeDeclarationError(typeDescr,"Duplicate type definition. A class with the name '" + resolvedType.getName() + "' was found in the classpath while trying to "+ "redefine the fields in the declare statement. Fields can only be defined for non-existing classes."));
      }
      return false;
    }
 else {
      return false;
    }
  }
 catch (  ClassNotFoundException cnfe) {
    return true;
  }
}
