{
  final FunctionBuilder builder=new FunctionBuilder();
  this.pkg.addFunction(functionDescr.getName());
  addClassCompileTask(this.pkg.getName() + "." + ucFirst(functionDescr.getName()),functionDescr,builder.build(this.pkg,functionDescr,getFunctionFixer(),getTypeResolver(),lineMappings),this.src,new FunctionErrorHandler(functionDescr,"Function Compilation error"));
}
