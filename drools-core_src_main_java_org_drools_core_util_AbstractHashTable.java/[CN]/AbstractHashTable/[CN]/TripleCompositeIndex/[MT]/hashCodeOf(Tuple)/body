{
  int hashCode=this.startResult;
  hashCode=LeftTupleIndexHashTable.PRIME * hashCode + this.index0.declaration.getHashCode(null,tuple.getObject(this.index0.declaration));
  hashCode=LeftTupleIndexHashTable.PRIME * hashCode + this.index1.declaration.getHashCode(null,tuple.getObject(this.index1.declaration));
  hashCode=LeftTupleIndexHashTable.PRIME * hashCode + this.index2.declaration.getHashCode(null,tuple.getObject(this.index2.declaration));
  return rehash(hashCode);
}
