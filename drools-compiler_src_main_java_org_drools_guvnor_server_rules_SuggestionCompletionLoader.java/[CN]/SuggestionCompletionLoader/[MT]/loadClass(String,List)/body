{
  Class clazz=null;
  try {
    clazz=resolver.resolveType(className);
  }
 catch (  ClassNotFoundException e1) {
    try {
      addJars(jars);
      clazz=resolver.resolveType(className);
    }
 catch (    Exception e) {
      this.errors.add("Class not found: " + className);
    }
  }
  return clazz;
}
