{
  final String inputKey="When the credit rating is {rating:regexp:\\d{3}}";
  final String inputValue="applicant:Applicant(credit=={rating})";
  DSLMappingEntry entry=createEntry(inputKey,inputValue);
  assertEquals("(\\W|^)When\\s+the\\s+credit\\s+rating\\s+is\\s+(\\d{3})(\\W|$)",entry.getKeyPattern().toString());
  assertEquals("$1applicant:Applicant(credit==$2)$3",entry.getValuePattern());
  String result=entry.getKeyPattern().matcher("When the credit rating is 555").replaceAll(entry.getValuePattern());
  assertEquals(result,"applicant:Applicant(credit==555)",result);
}
