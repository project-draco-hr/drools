{
  String drl="";
  drl+="package org.kie.test\n";
  drl+="global java.util.List list\n";
  drl+="rule rule1\n";
  drl+="when\n";
  drl+="then\n";
  drl+="list.add( drools.getRule().getName() );\n";
  drl+="end\n";
  drl+="rule rule2\n";
  drl+="when\n";
  drl+="then\n";
  drl+="list.add( drools.getRule().getName() );\n";
  drl+="end\n";
  DrlPackageParser s=DrlPackageParser.findPackageDataFromDrl(drl);
  assertEquals("org.kie.test",s.getName());
  assertEquals(2,s.getRules().size());
  assertEquals("",s.getDescription());
}
