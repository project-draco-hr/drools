{
  if (kbuilder != null) {
    return kbuilder;
  }
  kbuilder=KnowledgeBuilderFactory.newKnowledgeBuilder(kConf);
  CompositeKnowledgeBuilder ckbuilder=kbuilder.batch();
  for (  String kBaseFile : kBase.getFiles()) {
    buildKBaseFile(ckbuilder,kBase,kBaseFile);
  }
  ckbuilder.build();
  return kbuilder;
}
