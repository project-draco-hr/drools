{
  this.buf.setLength(0);
  appendComa(this.valueNumber++);
  if (name != null) {
    this.buf.append(name).append('=');
  }
  this.buf.append('@');
  appendDescriptor(TraceAbstractVisitor.FIELD_DESCRIPTOR,desc);
  this.buf.append('(');
  this.text.add(this.buf.toString());
  final TraceAnnotationVisitor tav=createTraceAnnotationVisitor();
  this.text.add(tav.getText());
  this.text.add(")");
  if (this.av != null) {
    tav.av=this.av.visitAnnotation(name,desc);
  }
  return tav;
}
