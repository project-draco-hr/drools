{
  if (signatureBuilder_ == null) {
    if (((bitField0_ & 0x00000002) == 0x00000002) && signature_ != org.drools.core.marshalling.impl.ProtobufMessages.Signature.getDefaultInstance()) {
      signature_=org.drools.core.marshalling.impl.ProtobufMessages.Signature.newBuilder(signature_).mergeFrom(value).buildPartial();
    }
