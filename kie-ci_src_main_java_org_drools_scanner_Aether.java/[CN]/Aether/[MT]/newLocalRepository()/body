{
  File m2RepoDir=new File(M2_REPO);
  if (!m2RepoDir.exists()) {
    return null;
  }
  try {
    String localRepositoryUrl=m2RepoDir.toURI().toURL().toExternalForm();
    return new RemoteRepository("local","default",localRepositoryUrl);
  }
 catch (  MalformedURLException e) {
  }
  return null;
}
