{
  if (value == null || value.isEmpty()) {
    return new Object[0];
  }
  Object[] ret=new Object[value.size()];
  int i=0;
  for (  ArrayList<Object> list : value) {
    int j=0;
    Object[] sublist=new Object[list.size()];
    for (    Object object : list) {
      sublist[j++]=object;
    }
    ret[i++]=sublist;
  }
  return ret;
}
