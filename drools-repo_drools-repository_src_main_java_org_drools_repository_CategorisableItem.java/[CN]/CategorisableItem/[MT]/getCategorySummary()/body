{
  final StringBuilder sum=new StringBuilder();
  doList(new Accum(){
    int count=0;
    public void add(    CategoryItem c){
      count++;
      if (count == 4) {
        sum.append("...");
      }
 else       if (count < 4) {
        sum.append(c.getName());
        sum.append(' ');
      }
    }
  }
);
  return sum.toString();
}
