{
  this.defaultEclipseSettings.put(CompilerOptions.OPTION_LineNumberAttribute,CompilerOptions.GENERATE);
  this.defaultEclipseSettings.put(CompilerOptions.OPTION_SourceFileAttribute,CompilerOptions.GENERATE);
  this.defaultEclipseSettings.put(CompilerOptions.OPTION_ReportUnusedImport,CompilerOptions.IGNORE);
  this.defaultEclipseSettings.put(CompilerOptions.OPTION_LocalVariableAttribute,CompilerOptions.GENERATE);
}
