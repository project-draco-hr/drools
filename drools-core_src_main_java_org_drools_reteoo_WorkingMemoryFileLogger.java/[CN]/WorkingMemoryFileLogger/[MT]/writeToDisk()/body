{
  try {
    XStream xstream=new XStream();
    ObjectOutputStream out=xstream.createObjectOutputStream(new FileWriter(fileName + (nbOfFile == 0 ? ".log" : nbOfFile + ".log"),false));
    out.writeObject(events);
    out.close();
    nbOfFile++;
    clear();
  }
 catch (  Throwable t) {
    t.printStackTrace(System.err);
  }
}
