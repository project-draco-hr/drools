{
  for (int i=0, size=unboundIdentifiers.size(); i < size; i++) {
    String identifier=(String)unboundIdentifiers.get(i);
    FieldBindingDescr implicitBinding=new FieldBindingDescr(identifier,identifier);
    final FieldExtractor extractor=getFieldExtractor(context,utils,implicitBinding,column.getObjectType(),implicitBinding.getFieldName(),false);
    if (extractor == null) {
      continue;
    }
    final Declaration declaration=new Declaration(identifier,extractor,column);
    factDeclarations.add(declaration);
  }
}
