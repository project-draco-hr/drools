{
  LeftTuple first=handle.getLastLeftTuple();
  if (first == null) {
    handle.setFirstLeftTuple(this);
    handle.setLastLeftTuple(this);
  }
 else {
    handle.getLastLeftTuple().leftParentNext=this;
    this.leftParentPrevious=handle.getLastLeftTuple();
    handle.setLastLeftTuple(this);
  }
}
