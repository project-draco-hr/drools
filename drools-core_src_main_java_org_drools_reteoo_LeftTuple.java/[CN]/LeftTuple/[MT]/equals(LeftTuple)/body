{
  if (other == this) {
    return true;
  }
 else   if (other == null) {
    return false;
  }
  if (this.hashCode() != other.hashCode()) {
    return false;
  }
  if (this.handle != other.handle) {
    return false;
  }
  if (this.parent == null) {
    return (other.parent == null);
  }
 else {
    return this.parent.equals(other.parent);
  }
}
