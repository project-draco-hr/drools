{
  name=name.trim();
  if (StringUtils.isEmpty(name)) {
    return null;
  }
  if (name.equals("drools.agent.scanResources ")) {
    return Boolean.toString(this.scanResources);
  }
 else   if (name.equals("drools.agent.newInstance ")) {
    return Boolean.toString(this.newInstance);
  }
  return null;
}
