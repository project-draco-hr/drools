{
  try {
    final PredicateContextEntry ctx=(PredicateContextEntry)context;
    return this.expression.evaluate(handle,ctx.leftTuple,this.previousDeclarations,this.localDeclarations,ctx.workingMemory,ctx.dialectContext);
  }
 catch (  final Exception e) {
    throw new RuntimeException("Exception executing predicate " + this.expression,e);
  }
}
