{
  final RuleFlowGroupNode node=activation.getRuleFlowGroupNode();
  this.list.remove(node);
  activation.setActivationGroupNode(null);
  if (this.autoDeactivate) {
    if (this.list.isEmpty()) {
      this.active=false;
      if (getProcessInstance() != null) {
        triggerCompleted();
      }
    }
  }
}
