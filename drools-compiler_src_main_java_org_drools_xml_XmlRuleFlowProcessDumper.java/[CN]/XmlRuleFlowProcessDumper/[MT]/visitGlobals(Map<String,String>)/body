{
  if (globals != null && globals.size() > 0) {
    xmlDump.append("    <globals>" + EOL);
    for (    Map.Entry<String,String> global : globals.entrySet()) {
      xmlDump.append("      <global identifier=\"" + global.getKey() + "\" type=\""+ global.getValue()+ "\" />"+ EOL);
    }
    xmlDump.append("    </globals>" + EOL);
  }
}
