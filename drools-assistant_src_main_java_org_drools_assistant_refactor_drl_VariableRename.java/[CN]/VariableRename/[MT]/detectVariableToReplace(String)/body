{
  for (int position=0; position < right.length(); position++) {
    if (right.charAt(position) == ':' || right.charAt(position) == '.' || right.charAt(position) == ')')     return right.substring(0,position).trim();
    if (right.charAt(position) == ',' || right.charAt(position) == '(')     return null;
  }
  return null;
}
