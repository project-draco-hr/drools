{
  setKSession(getModelSession(source,VERBOSE));
  setKbase(getKSession().getKieBase());
  getKSession().fireAllRules();
  getKSession().getEntryPoint("in_Feat1").insert(2.2);
  getKSession().fireAllRules();
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"Feat1"),true,false,"Test_MLP",2.2);
  refreshKSession();
  getKSession().getEntryPoint("in_Feat2").insert(5);
  getKSession().fireAllRules();
  checkFirstDataFieldOfTypeStatus(getKbase().getFactType(packageName,"Feat2"),true,false,"Test_MLP",5);
  checkGeneratedRules();
}
