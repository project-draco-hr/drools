{
  Class cls=findLoadedClass(name);
  if (cls == null) {
    cls=fastFindClass(name);
    if (cls == null) {
      for (      final ClassLoader parent : this.parents) {
        try {
          cls=Class.forName(name,true,parent);
        }
 catch (        ClassNotFoundException e) {
        }
        if (cls != null) {
          break;
        }
      }
      if (cls == null) {
        return null;
      }
    }
  }
  if (resolve) {
    resolveClass(cls);
  }
  return cls;
}
