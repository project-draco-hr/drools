{
  for (  final ClassLoader cl : this.classLoaders) {
    if (cl == classLoader) {
      return;
    }
  }
  this.classLoaders.add(classLoader);
  ClassLoader parent=classLoader.getParent();
  for (  final ClassLoader cl : this.parents) {
    if (cl == parent) {
      return;
    }
  }
  this.parents.add(parent);
}
