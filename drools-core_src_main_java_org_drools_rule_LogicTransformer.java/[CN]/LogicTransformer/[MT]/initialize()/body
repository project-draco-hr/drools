{
  addTransformationPair(And.class,And.class);
  addTransformationPair(Or.class,Or.class);
  addTransformationPair(Exist.class,Exist.class);
  addTransformationPair(Not.class,Or.class,new NotOrTransformation());
  addTransformationPair(Exist.class,Or.class,new ExistOrTransformation());
  addTransformationPair(And.class,Or.class,new AndOrTransformation());
}
