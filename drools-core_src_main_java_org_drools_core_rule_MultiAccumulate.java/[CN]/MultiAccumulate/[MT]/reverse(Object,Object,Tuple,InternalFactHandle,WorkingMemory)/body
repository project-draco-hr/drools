{
  try {
    for (int i=0; i < this.accumulators.length; i++) {
      this.accumulators[i].reverse(((Object[])workingMemoryContext)[i],((Object[])context)[i],leftTuple,handle,this.requiredDeclarations,getInnerDeclarationCache(),workingMemory);
    }
  }
 catch (  final Exception e) {
    throw new RuntimeException(e);
  }
}
