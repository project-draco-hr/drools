{
  try {
    template=getVelocity().getTemplate(templateName);
  }
 catch (  ResourceNotFoundException e) {
    throw new NaturalLanguageException("Unable to find output template.",e);
  }
catch (  ParseErrorException e) {
    throw new NaturalLanguageException("Parse error on output template.",e);
  }
catch (  Exception e) {
    throw new NaturalLanguageException("Error loading output template.",e);
  }
}
