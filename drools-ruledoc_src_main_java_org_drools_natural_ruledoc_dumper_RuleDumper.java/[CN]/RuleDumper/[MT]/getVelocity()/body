{
  if (velocity == null) {
synchronized (RuleDumper.class) {
      VelocityEngine e=new VelocityEngine();
      e.setProperty("resource.loader","class");
      e.setProperty("class.resource.loader.class","org.apache.velocity.runtime.resource.loader.ClasspathResourceLoader");
      e.init();
      velocity=e;
    }
  }
  return velocity;
}
