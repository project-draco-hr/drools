{
  ZipOutputStream out=null;
  try {
    outputFolder.mkdirs();
    File jarFile=new File(outputFolder,jarName);
    out=new ZipOutputStream(new FileOutputStream(jarFile));
    writeJarEntries(out,rootFolder,entries);
    out.close();
    return jarFile;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    try {
      if (out != null) {
        out.close();
      }
    }
 catch (    IOException e) {
    }
  }
}
