{
  String inputKey="- name is {name}";
  String inputValue="name == \"{name}\"";
  String expectedKeyP="-\\s*name\\s*is\\s*(.*?)$";
  String expectedValP="name == \"$1\"";
  DSLMappingEntry entry=new DefaultDSLMappingEntry(DSLMappingEntry.CONDITION,null,inputKey,inputValue);
  assertEquals(inputKey,entry.getMappingKey());
  assertEquals(expectedKeyP,entry.getKeyPattern().pattern());
  assertEquals(inputValue,entry.getMappingValue());
  assertEquals(expectedValP,entry.getValuePattern());
}
