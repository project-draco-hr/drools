{
  if (!cancel) {
    try {
      handler.beforeTaskStarts(delegate,Thread.currentThread());
      delegate.run();
    }
 catch (    Throwable t) {
      handler.taskExceptionRaised(delegate,Thread.currentThread(),t);
    }
 finally {
      handler.afterTaskFinishes(delegate,Thread.currentThread());
    }
  }
}
