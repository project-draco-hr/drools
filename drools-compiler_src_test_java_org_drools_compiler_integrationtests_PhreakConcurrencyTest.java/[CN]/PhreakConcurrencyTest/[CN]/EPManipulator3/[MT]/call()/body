{
  SessionEntryPoint ep=ksession.getEntryPoint("EP" + index);
  for (int i=0; i < 100; i++) {
    Thread.sleep(RANDOM.nextInt(100));
    if (fh == null) {
      fh=ep.insert("" + index);
    }
 else {
      if (RANDOM.nextInt(100) < 70) {
        ep.delete(fh);
        fh=null;
      }
 else {
        ep.update(fh,"" + index);
      }
    }
  }
  if (index == deleteIndex) {
    if (fh != null) {
      ep.delete(fh);
      fh=null;
    }
  }
 else   if (fh == null) {
    fh=ep.insert("" + index);
  }
  return true;
}
