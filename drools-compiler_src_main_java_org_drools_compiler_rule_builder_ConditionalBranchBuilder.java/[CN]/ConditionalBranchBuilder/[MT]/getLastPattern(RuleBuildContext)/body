{
  GroupElement ge=(GroupElement)context.getBuildStack().peek();
  Pattern lastPattern=getLastPattern(ge.getChildren());
  if (lastPattern == null) {
    RuleImpl parent=context.getRule().getParent();
    if (parent != null) {
      lastPattern=getLastPattern(parent.getLhs().getChildren());
    }
  }
  return lastPattern;
}
