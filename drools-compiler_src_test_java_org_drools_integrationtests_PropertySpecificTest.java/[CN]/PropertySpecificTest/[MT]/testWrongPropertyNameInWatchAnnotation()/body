{
  String rule="package org.drools\n" + "dialect \"mvel\"\n" + "declare A\n"+ "    s : String\n"+ "end\n"+ "declare B\n"+ "    @propertySpecific\n"+ "    on : boolean\n"+ "    s : String\n"+ "end\n"+ "rule R1\n"+ "when\n"+ "    A($s : s)\n"+ "    $b : B(s != $s) @watch( !s1, on )\n"+ "then\n"+ "    modify($b) { setS($s) }\n"+ "end\n"+ "rule R2\n"+ "when\n"+ "    $b : B(on == false)\n"+ "then\n"+ "    modify($b) { setOn(true) }\n"+ "end\n";
  KnowledgeBuilder kbuilder=KnowledgeBuilderFactory.newKnowledgeBuilder();
  kbuilder.add(ResourceFactory.newByteArrayResource(rule.getBytes()),ResourceType.DRL);
  assertEquals(1,kbuilder.getErrors().size());
}
