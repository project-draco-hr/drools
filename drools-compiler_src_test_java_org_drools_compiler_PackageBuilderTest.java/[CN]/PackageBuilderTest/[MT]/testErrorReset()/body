{
  final PackageBuilder builder=new PackageBuilder();
  builder.addPackageFromDrl(new StringReader("package foo \n rule ORB"));
  assertTrue(builder.hasErrors());
  builder.resetErrors();
  assertFalse(builder.hasErrors());
  builder.addPackageFromDrl(new StringReader("package foo \n rule ORB"));
  assertTrue(builder.hasErrors());
}
