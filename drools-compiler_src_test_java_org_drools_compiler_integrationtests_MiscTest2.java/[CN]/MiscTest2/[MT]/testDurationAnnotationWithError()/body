{
  String str="package org.drools.compiler.integrationtests;\n" + "import org.drools.compiler.integrationtests.MiscTest2.SimpleEvent\n" + "declare SimpleEvent\n"+ "    @role(event)\n"+ "    @duration(duratio)\n"+ "end\n";
  KnowledgeBuilder kbuilder=KnowledgeBuilderFactory.newKnowledgeBuilder();
  kbuilder.add(ResourceFactory.newByteArrayResource(str.getBytes()),ResourceType.DRL);
  assertTrue(kbuilder.hasErrors());
}
