{
  while (this.row <= this.length) {
    while (this.list == null) {
      if (this.row < length) {
        this.list=(TupleList)this.table[this.row];
        this.row++;
      }
 else {
        return null;
      }
      if (this.list != null) {
        this.tuple=this.list.getFirst();
        return this.tuple;
      }
    }
    this.tuple=(Tuple)this.tuple.getNext();
    if (this.tuple != null) {
      return this.tuple;
    }
 else {
      this.list=(TupleList)this.list.getNext();
      if (this.list != null) {
        this.tuple=this.list.getFirst();
        return this.tuple;
      }
    }
  }
  return null;
}
