{
  String chunk="";
  int first=-1, last=first;
  try {
    Token t=match(input,DRLLexer.ID,DroolsSoftKeywords.THEN,null,DroolsEditorType.KEYWORD);
    if (state.failed)     return;
    first=input.index();
    if (state.backtracking == 0) {
      rule.getDescr().setConsequenceLocation(t.getLine(),t.getCharPositionInLine());
    }
    while (!helper.validateIdentifierKey(DroolsSoftKeywords.END)) {
      input.consume();
    }
    last=input.LT(-1).getTokenIndex();
    if (last >= first) {
      chunk=input.toString(first,last);
    }
    rule.rhs(chunk);
  }
 catch (  RecognitionException re) {
    reportError(re);
  }
}
