{
  StringBuilder builder=new StringBuilder();
  for (  String k : key) {
    match(input,DRLLexer.ID,k,null,DroolsEditorType.KEYWORD);
    if (state.failed)     return;
    builder.append(k);
  }
  if (state.backtracking == 0)   attribute.name(builder.toString());
  String value="true";
  if (input.LA(1) == DRLLexer.BOOL) {
    Token bool=match(input,DRLLexer.BOOL,null,null,DroolsEditorType.KEYWORD);
    if (state.failed)     return;
    value=bool.getText();
  }
  if (state.backtracking == 0) {
    attribute.value(value);
  }
}
