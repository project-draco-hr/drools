{
  ImportDescrBuilder imp=null;
  try {
    imp=helper.start(ImportDescrBuilder.class);
    match(input,DRLLexer.ID,DroolsSoftKeywords.IMPORT,null,DroolsEditorType.KEYWORD);
    if (helper.validateIdentifierKey(DroolsSoftKeywords.FUNCTION)) {
      match(input,DRLLexer.ID,DroolsSoftKeywords.FUNCTION,null,DroolsEditorType.KEYWORD);
    }
