{
  super();
  this.stream=stream;
  this.ruleBase=ruleBase;
  this.sinks=sinks;
  handles=new HashMap<Integer,InternalFactHandle>();
  this.rightTuples=new HashMap<RightTupleKey,RightTuple>();
  this.terminalTupleMap=new HashMap<Integer,LeftTuple>();
