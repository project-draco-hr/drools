{
  if (type.matchesExtension(".drl")) {
    DrlParser p=new DrlParser();
    try {
      PackageDescr pkg=p.parse(resource.getInputStream());
      addPackageDescr(pkg);
    }
 catch (    DroolsParserException e) {
      errors.add(new VerifierError(e.getMessage()));
    }
catch (    IOException e) {
      errors.add(new VerifierError(e.getMessage()));
    }
  }
}
