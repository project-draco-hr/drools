{
  RiaPathMemory pathMem=bm.getRiaRuleMemory();
  SegmentMemory[] subnetworkSmems=pathMem.getSegmentMemories();
  SegmentMemory subSmem=null;
  for (int i=0; subSmem == null; i++) {
    subSmem=subnetworkSmems[i];
  }
  StackEntry stackEntry=new StackEntry(liaNode,betaNode,sink,rmem,nodeMem,smems,smemIndex,srcTuples,visitedRules,false);
  stack.add(stackEntry);
  if (log.isTraceEnabled()) {
    int offset=getOffset(betaNode);
    log.trace("{} RiaQueue {} {}",indent(offset),betaNode.toString(),srcTuples.toStringSizes());
  }
  eval2(liaNode,pathMem,(LeftTupleSink)subSmem.getRootNode(),subSmem.getNodeMemories().getFirst(),subnetworkSmems,subSmem.getPos(),subSmem.getStagedLeftTuples(),wm,stack,visitedRules,true);
}
