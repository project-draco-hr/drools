{
  try {
    URL url=new URL(uri);
    URLConnection con=url.openConnection();
    HttpURLConnection httpCon=(HttpURLConnection)con;
    httpCon.setRequestMethod("HEAD");
    System.err.println(httpCon.getHeaderFields());
  }
 catch (  IOException e) {
    throw new RuntimeDroolsException(e);
  }
}
