{
  if (dirty) {
    resetIdGenerator();
    updateTupleSinkId(this,this);
    dirty=false;
  }
  context.setObjectType(objectType);
  if (compiledNetwork != null) {
    compiledNetwork.modifyObject(factHandle,modifyPreviousTuples,context.getModificationMask() > 0L ? context.adaptModificationMaskForObjectType(objectType,workingMemory) : context,workingMemory);
  }
 else {
    if (factHandle.getObject() instanceof TraitProxy && typeMask != null) {
      BitSet vetoMask=((TraitProxy)factHandle.getObject()).getTypeFilter();
      if (vetoMask == null || typeMask.isEmpty() || !HierarchyEncoderImpl.supersetOrEqualset(vetoMask,this.typeMask)) {
        this.sink.propagateModifyObject(factHandle,modifyPreviousTuples,context.getModificationMask() > 0L ? context.adaptModificationMaskForObjectType(objectType,workingMemory) : context,workingMemory);
      }
    }
 else {
      this.sink.propagateModifyObject(factHandle,modifyPreviousTuples,context.getModificationMask() > 0L ? context.adaptModificationMaskForObjectType(objectType,workingMemory) : context,workingMemory);
    }
  }
}
