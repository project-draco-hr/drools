{
  if (!this.baseObjectType.isAssignableFrom(workingMemory.getObjectTypeConfigurationRegistry().getObjectTypeConf(context.getEntryPoint(),((InternalFactHandle)context.getFactHandle()).getObject()).getConcreteObjectTypeNode().getObjectType())) {
    this.sink.propagateAssertLeftTuple(leftTuple,context,workingMemory,this.tupleMemoryEnabled);
  }
}
