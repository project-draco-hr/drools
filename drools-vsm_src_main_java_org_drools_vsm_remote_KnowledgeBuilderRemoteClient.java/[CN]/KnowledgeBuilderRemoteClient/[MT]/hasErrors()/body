{
  String commandId="kbuilder.hasErrors_" + serviceManager.getNextId();
  String kresultsId="kresults_" + serviceManager.getSessionId();
  Message msg=new Message(serviceManager.getSessionId(),serviceManager.getNextId(),false,new KnowledgeContextResolveFromContextCommand(new KnowledgeBuilderHasErrorsCommand(commandId),instanceId,null,null,kresultsId));
  BlockingMessageResponseHandler handler=new BlockingMessageResponseHandler();
  try {
    serviceManager.client.addResponseHandler(msg.getResponseId(),handler);
    serviceManager.client.write(msg);
    Object object=handler.getMessage().getPayload();
    if (object == null) {
      throw new RuntimeException("Response was not correctly received");
    }
    System.out.println("object" + object);
    return (Boolean)((ExecutionResults)object).getValue(commandId);
  }
 catch (  Exception e) {
    throw new RuntimeException("Unable to execute message",e);
  }
}
