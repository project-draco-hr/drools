{
  int position;
  for (position=0; position <= right.length() - 1; position++) {
    if (right.charAt(position) == ',' || right.charAt(position) == '(')     break;
    if (right.charAt(position) == ':')     return true;
  }
  for (position=left.length() - 1; position >= 0; position--) {
    if (left.charAt(position) == '>' || left.charAt(position) == '<' || left.charAt(position) == '=')     return true;
    if (left.charAt(position) == ',')     return false;
  }
  return false;
}
