{
  final QueryElement qe=(QueryElement)rce;
  context.pushRuleComponent(qe);
  qe.getResultPattern().setOffset(context.getCurrentPatternOffset());
  utils.checkUnboundDeclarations(context,qe.getRequiredDeclarations());
  context.setTupleSource((LeftTupleSource)utils.attachNode(context,new QueryElementNode(context.getNextId(),context.getTupleSource(),qe,false,context)));
  context.popRuleComponent();
}
