{
  char ch=160;
  String lno="";
  if (lineNumber == 1 || (modulus >= 1 && (lineNumber % modulus == 0))) {
    lno="" + lineNumber;
  }
  while (lno.length() < width) {
    lno=ch + lno;
  }
  lno+=separator;
  char chars[]=new char[lno.length()];
  for (int count=0; count < lno.length(); count++) {
    chars[count]=lno.charAt(count);
  }
  characters(chars,0,lno.length());
}
