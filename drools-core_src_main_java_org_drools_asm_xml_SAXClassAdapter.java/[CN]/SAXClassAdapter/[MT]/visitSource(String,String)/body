{
  if (source == null && debug == null) {
    return;
  }
  final AttributesImpl att=new AttributesImpl();
  if (source != null) {
    att.addAttribute("","file","file","",encode(source));
  }
  if (debug != null) {
    att.addAttribute("","debug","debug","",encode(debug));
  }
  addElement("source",att);
}
