{
  if (this == obj) {
    return true;
  }
  if (obj == null || !(obj instanceof FromNode)) {
    return false;
  }
  FromNode other=(FromNode)obj;
  return leftInput.equals(other.leftInput) && dataProvider.equals(other.dataProvider) && areNullSafeEquals(from.getResultPattern(),other.from.getResultPattern())&& Arrays.equals(alphaConstraints,other.alphaConstraints)&& betaConstraints.equals(other.betaConstraints);
}
