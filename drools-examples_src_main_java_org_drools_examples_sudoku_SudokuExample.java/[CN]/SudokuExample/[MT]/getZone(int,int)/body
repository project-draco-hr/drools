{
  if (column < 3 && row < 3) {
    return 1;
  }
 else   if (column < 6 && row < 3) {
    return 2;
  }
 else   if (column < 9 && row < 3) {
    return 3;
  }
 else   if (column < 3 && row < 6) {
    return 4;
  }
 else   if (column < 6 && row < 6) {
    return 5;
  }
 else   if (column < 9 && row < 6) {
    return 6;
  }
 else   if (column < 3) {
    return 7;
  }
 else   if (column < 6) {
    return 8;
  }
 else   if (column < 9) {
    return 9;
  }
  return 0;
}
