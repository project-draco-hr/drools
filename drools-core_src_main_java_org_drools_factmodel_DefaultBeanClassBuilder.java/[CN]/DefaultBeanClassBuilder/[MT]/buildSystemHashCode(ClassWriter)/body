{
{
    MethodVisitor mv=cw.visitMethod(ACC_PUBLIC,"hashCode","()I",null,null);
    mv.visitCode();
    mv.visitVarInsn(ALOAD,0);
    mv.visitMethodInsn(INVOKESTATIC,"java/lang/System","identityHashCode","(Ljava/lang/Object;)I");
    mv.visitInsn(IRETURN);
    mv.visitMaxs(1,1);
    mv.visitEnd();
  }
}
