{
  try {
    BetaLeftMemory memory=BetaMemoryFactory.newLeftMemory(this.config,null);
    Assert.assertNotNull("BetaMemoryFactory should not return null",memory);
    Assert.assertTrue("Without constraints, BetaMemoryFactory should " + "return an instance of DefaultLeftMemory",memory instanceof DefaultLeftMemory);
    memory=BetaMemoryFactory.newLeftMemory(this.config,this.binder);
    Assert.assertNotNull("BetaMemoryFactory should not return null",memory);
  }
 catch (  final Exception e) {
    Assert.fail("BetaLeftMemory is not supposed to throw exceptions");
  }
}
