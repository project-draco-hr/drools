{
  if (activation.getRule() instanceof Query) {
    this.workingMemory.addToQueryResults(activation.getRule().getName(),activation.getTuple());
  }
 else {
    if (activation.getRule().getXorGroup() == null || (activation.getRule().getXorGroup() != null && this.getXorGroup(activation.getRule().getXorGroup()).isEmpty())) {
      super.fireActivation(activation);
      if (activation.getRule().getXorGroup() != null) {
        this.getXorGroup(activation.getRule().getXorGroup());
      }
    }
  }
}
