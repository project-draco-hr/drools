{
  for (  Map.Entry<String,Object> entry : ((NodeImpl)node).getMetaData().entrySet()) {
    String name=entry.getKey();
    if (!"x".equals(name) && !"y".equals(name) && !"width".equals(name)&& !"height".equals(name)&& !"color".equals(name)&& entry.getValue() instanceof String) {
      return true;
    }
  }
  return false;
}
