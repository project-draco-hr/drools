{
  VariableResolverFactory factory=(VariableResolverFactory)ctx;
  unit.updateFactory(null,null,object,(LeftTuple)tuple,null,(InternalWorkingMemory)workingMemory,workingMemory.getGlobalResolver(),factory);
  Package pkg=workingMemory.getRuleBase().getPackage("MAIN");
  if (pkg != null) {
    MVELDialectRuntimeData data=(MVELDialectRuntimeData)pkg.getDialectRuntimeRegistry().getDialectData(this.id);
    factory.setNextFactory(data.getFunctionFactory());
  }
  return org.drools.base.FieldFactory.getFieldValue(MVEL.executeExpression(this.expr,object,factory));
}
