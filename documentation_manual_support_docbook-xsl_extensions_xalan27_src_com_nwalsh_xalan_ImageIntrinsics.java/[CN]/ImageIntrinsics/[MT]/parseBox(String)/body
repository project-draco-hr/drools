{
  int[] corners=new int[4];
  int count=0;
  StringTokenizer st=new StringTokenizer(line);
  while (count < 4 && st.hasMoreTokens()) {
    try {
      corners[count++]=Integer.parseInt(st.nextToken());
    }
 catch (    Exception e) {
    }
  }
  width=corners[2] - corners[0];
  depth=corners[3] - corners[1];
}
