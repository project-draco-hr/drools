{
  String value=null;
  for (  Properties props : this.props) {
    value=props.getProperty(key);
    if (value != null) {
      break;
    }
  }
  if (value == null) {
    for (    Properties props : this.defaultProps) {
      value=props.getProperty(key);
      if (value != null) {
        break;
      }
    }
  }
  return (value != null) ? value : defaultValue;
}
