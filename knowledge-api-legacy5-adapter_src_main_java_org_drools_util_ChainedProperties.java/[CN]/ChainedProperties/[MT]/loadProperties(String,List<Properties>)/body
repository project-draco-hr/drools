{
  if (fileName != null) {
    File file=new File(fileName);
    if (file != null && file.exists()) {
      try {
        loadProperties(file.toURL(),chain);
      }
 catch (      MalformedURLException e) {
        throw new IllegalArgumentException("file.toURL() failed for " + fileName + " properties value '"+ file+ "'");
      }
    }
 else {
    }
  }
}
