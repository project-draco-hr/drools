{
  this.configuration=configuration;
  this.resource=resource;
  this.changes=changes;
  if (changes != null) {
    changeMap=new HashMap<String,ResourceChange>();
    for (    ResourceChange c : changes.getChanges()) {
      changeMap.put(assetId(c.getType(),c.getName()),c);
    }
  }
 else {
    changeMap=null;
  }
}
