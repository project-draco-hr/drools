{
  StringBuilder sb=new StringBuilder();
  for (  Map<Integer,String> map : paraphrases) {
    for (    Entry<Integer,String> activeEntry : map.entrySet()) {
      if (activeEntry.getValue().length() == 0) {
        sb.append(String.format(PARSER_LOCATION_MESSAGE_PART,parserLocations[activeEntry.getKey()]));
      }
 else {
        sb.append(String.format(PARSER_LOCATION_MESSAGE_COMPLETE,parserLocations[activeEntry.getKey()],activeEntry.getValue()));
      }
    }
  }
  return sb.toString();
}
