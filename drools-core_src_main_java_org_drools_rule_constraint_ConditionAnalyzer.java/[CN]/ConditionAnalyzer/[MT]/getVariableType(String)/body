{
  for (  Declaration declaration : declarations) {
    if (declaration.getBindingName().equals(name)) {
      Class<?> clazz=declaration.getValueType().getClassType();
      if (clazz == int.class) {
        return Integer.class;
      }
      if (clazz == long.class) {
        return Long.class;
      }
      return clazz;
    }
  }
  return Object.class;
}
