{
  List list=new ArrayList();
  list.add("org.drools.StaticMethods");
  TypeResolver typeResolver=new ClassTypeResolver(list);
  list=new ArrayList();
  list.add("StaticMethods.*");
  FunctionResolver functionResolver=new StaticMethodFunctionResolver(list,typeResolver);
  assertEquals("org.drools.StaticMethods",functionResolver.resolveFunction("getString1","a"));
  Map map=new HashMap();
  map.put("a",String.class);
  assertEquals("org.drools.StaticMethods",functionResolver.resolveFunction("getString1","a",new DeclarationScopeResolver(new Map[]{map})));
}
