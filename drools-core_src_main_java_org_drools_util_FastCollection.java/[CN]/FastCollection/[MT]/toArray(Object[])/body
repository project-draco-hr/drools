{
  final int size=size();
  if (array.length < size) {
    throw new UnsupportedOperationException("Destination array too small");
  }
  if (array.length > size) {
    array[size]=null;
  }
  int i=0;
  final Object[] arrayView=array;
  for (Record r=head(), end=tail(); (r=r.getNext()) != end; ) {
    arrayView[i++]=valueOf(r);
  }
  return array;
}
