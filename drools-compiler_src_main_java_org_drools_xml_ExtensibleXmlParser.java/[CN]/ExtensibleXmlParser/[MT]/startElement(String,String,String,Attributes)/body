{
  if (direction == 1) {
    this.peer=null;
  }
 else {
    direction=1;
  }
  final Handler handler=getHandler(uri,localName);
  if (handler == null) {
    startElementBuilder(localName,attrs);
    return;
  }
  validate(uri,localName,handler);
  final Object node=handler.start(uri,localName,attrs,this);
  if (node == null) {
    this.parents.add(Null.instance);
  }
 else {
    this.parents.add(node);
  }
}
