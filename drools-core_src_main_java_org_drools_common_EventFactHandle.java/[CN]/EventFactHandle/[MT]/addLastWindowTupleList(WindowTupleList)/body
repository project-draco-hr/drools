{
  WindowTupleList previous=this.lastWindowTuple;
  if (previous == null) {
    window.setListPrevious(null);
    window.setListNext(null);
    firstWindowTuple=window;
    lastWindowTuple=window;
  }
 else {
    window.setListPrevious(previous);
    window.setListNext(null);
    previous.setListNext(window);
    lastWindowTuple=window;
  }
}
