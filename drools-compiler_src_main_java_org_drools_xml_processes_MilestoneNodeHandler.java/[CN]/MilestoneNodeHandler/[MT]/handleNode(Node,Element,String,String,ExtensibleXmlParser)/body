{
  super.handleNode(node,element,uri,localName,parser);
  MilestoneNode milestone=(MilestoneNode)node;
  String text=((Text)element.getChildNodes().item(0)).getWholeText();
  if (text != null) {
    text.trim();
    if ("".equals(text)) {
      text=null;
    }
  }
  milestone.setConstraint(text);
}
