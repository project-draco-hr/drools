{
  StringBuilder sb=new StringBuilder();
  sb.append("rule ");
  sb.append(ruleName);
  sb.append("\n");
  sb.append("when\n");
  if (patterns != null) {
    sb.append(patterns);
  }
  sb.append("then\n");
  sb.append("list.add( drools.getRule().getName() );\n");
  sb.append("end\n");
  return sb.toString();
}
