{
  Drl drl=new Drl();
  drl.setPackage_("com.simple");
  drl.setImports(new String[]{"com.sample.TestDataModel.*","asdfd.sss"});
  Rule rule=new Rule();
  rule.setRuleName("test");
  rule.setWhenPart(new String[]{"........"});
  rule.setThenPart(new String[]{"---------"});
  drl.addRule(rule);
  rule.setRuleName("test");
  rule.setWhenPart(new String[]{"!!!!!!!!!"});
  rule.setThenPart(new String[]{"---------"});
  drl.addRule(rule);
  System.out.println(drl);
}
