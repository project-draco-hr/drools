{
  try {
    startOperation();
    this.ruleBase.readLock();
    this.lock.lock();
    getEntryPointNode().retractObject(factHandle,null,this.getObjectTypeConfigurationRegistry().getObjectTypeConf(this.getEntryPoint(),factHandle.getObject()),this);
    getFactHandleFactory().destroyFactHandle(factHandle);
  }
  finally {
    this.lock.unlock();
    this.ruleBase.readUnlock();
    endOperation();
  }
}
