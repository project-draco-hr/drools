{
  try {
    startOperation();
    this.ruleBase.readLock();
    this.lock.lock();
    final PropagationContext propagationContext=new PropagationContextImpl(getNextPropagationIdCounter(),PropagationContext.ASSERTION,null,null,factHandle,agenda.getActiveActivations(),agenda.getDormantActivations(),getEntryPoint());
    getEntryPointNode().retractQuery(factHandle,propagationContext,this);
    getFactHandleFactory().destroyFactHandle(factHandle);
  }
  finally {
    this.lock.unlock();
    this.ruleBase.readUnlock();
    endOperation();
  }
}
