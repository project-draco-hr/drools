{
  long time1=timing(new Runnable(){
    public void run(){
      readDRL(FN1);
    }
  }
);
  long time2=timing(new Runnable(){
    public void run(){
      readDRL(FN2);
    }
  }
);
  long time3=timing(new Runnable(){
    public void run(){
      readDRL(FN3);
    }
  }
);
  System.out.println("mveljava: " + time1 / 1000.);
  System.out.println("    mvel: " + time2 / 1000.);
  System.out.println("    java: " + time3 / 1000.);
}
