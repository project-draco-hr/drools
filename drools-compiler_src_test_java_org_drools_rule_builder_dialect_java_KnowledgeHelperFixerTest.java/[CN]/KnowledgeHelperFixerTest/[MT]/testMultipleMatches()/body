{
  String result=KnowledgeHelperFixerTest.fixer.fix("modify(myObject); modify(myObject );");
  assertEquals("drools.modifyObject( myObject ); drools.modifyObject( myObject );",result);
  result=KnowledgeHelperFixerTest.fixer.fix("xxx modify(myObject ); modify(myObject ); modify(yourObject ); yyy");
  assertEquals("xxx drools.modifyObject( myObject ); drools.modifyObject( myObject ); drools.modifyObject( yourObject ); yyy",result);
}
